<script type="text/javascript">
document.getElementById('indexDiv').style.display = 'none';
document.getElementById('collapsedIndex').style.display = 'block';
document.getElementById('expandedIndex').style.display = 'none';
document.getElementById('collapsedIndex').onclick = function(e) {
document.getElementById('indexDiv').style.display = 'block';
document.getElementById('expandedIndex').style.display = 'block';
document.getElementById('collapsedIndex').style.display = 'none';
}
document.getElementById('expandedIndex').onclick = function(e) {
document.getElementById('indexDiv').style.display = 'none';
document.getElementById('expandedIndex').style.display = 'none';
document.getElementById('collapsedIndex').style.display = 'block';
}
</script>
