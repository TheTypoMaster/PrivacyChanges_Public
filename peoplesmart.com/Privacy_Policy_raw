<script>

	$(function() {
		$('header form:visible').searchHeader();

		//Add class for adding styles for autocomlpete on peages with header search
		//Need refactoring after project release 

		$('body').addClass('headerSearchPage');

		// Header mobile search fields dropdown
		$('.formElDropdown').dropdown({
			parentNode: '.searchTop_M',
			speed: 400,
			autoClose: false
		});

		// Sticky init. Exclude mobile devices
		if (!(/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent))) {
			$(".header").sticky();
		}
		
		//minimize, maximaze 
		$('._showBox').showHide({
			after: function () {
				iePlaceholders();
			}
		});
		
		// Drop init
		$('.formDrop').drop({
			disable_search_threshold: 30
		});

		$(".searchTop_M input[type=text]").each(function () {
			$(this).on("keypress", function (e) {
				if (e.which === 13) {
					$(this).closest("form").find(".btn").click();
				}
			});
		})

	});
</script>
	<main class="main" role="main">
		







