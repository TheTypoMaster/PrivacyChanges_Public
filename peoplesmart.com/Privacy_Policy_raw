<script>	

	$(function () {
		
		// Header search form dropdown switcher
		$('.dropdown').dropdown({
			parentNode: '.searchTopDropdownWrap',
			collapseGroup: '._headerForm',
			selectMode: true,
			speed: 400,
			whenColapsed: function () {
				iePlaceholders();
			}
		});

		// Emulation cklick on seach type dropdown to choose search type
		var searchType = $('.searchTop .' + 'person'),
			headerSearchButtonFlag = true;
		searchType.click();

		//Submit active form
		$('.searchTopWrap .btnSearch').on('click', function (e) {
			e.preventDefault();
			var form = $('._headerForm.active form');
			form.submit();
		});

		$('.searchTopWrap form').submit(function (e) {
			var id;
			if (headerSearchButtonFlag) {
				headerSearchButtonFlag = false;
				$('.searchTopWrap .btnSearch').trigger('click');
			} else {
				id = "#" + $(this).attr('id');
				customFormValidation(id, '.pseudoInput');
			}
			
		});

	});

</script>
	
