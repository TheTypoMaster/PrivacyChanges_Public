<script type="text/javascript" src="http://demandware.edgesuite.net/aaeo_prd/on/demandware.static/-/Sites-MichaelsUS-Library/default/v1441122771280/js/jquery.hoverIntent.minified.js"></script> 
<script>
    $(document).ready(function(){
        $('.country-name').hoverIntent({
            over: showCountries,
            out: hideCountries, 
            timeout: 200
        });
        // $('a[data-id=weekly-ad],a[data-id=deals]').parent().css({'background':'#d30e2c'});

        // ajax call for email sign up from header
        $('#signup-email-btn').on('click',function(e){
            e.preventDefault();
            
            var postURL = $(this).attr('data-action'),
                emailInput = $('.new-email-signup input[type=email]').val();

            $.ajax({
                method: "POST",
                url: postURL,
                data: { email: emailInput }
            }).done(function( msg ) {
                console.log(msg);
                if (msg === "Success") {
                    $('.new-email-signup').html("<h3>Thank you! You have opted in to receive emails.</h3>");
                } else {
                    $('.new-email-signup h3').append('<div class="error-form">Sorry, something has gone wrong on our end. Please try again or contact customer services.</div>')
                }
            });
        });

        // ajax call for email sign up from footer
        $('#signup').on('click',function(e){
            e.preventDefault();
            
            var postURL = $(this).attr('data-action'),
                emailInput = $('.sign-up-promotions input[type=email]').val();

            $.ajax({
                method: "POST",
                url: postURL,
                data: { email: emailInput }
            }).done(function( msg ) {
                console.log(msg);
                if (msg === "Success") {
                    $('.sign-up-promotions').html("<p>Thank you! You have opted in to receive emails.</p>");
                } else {
                    $('.sign-up-promotions').append('<div class="error-form">Sorry, something has gone wrong on our end. Please try again or contact customer services.</div>')
                }
            });
        });

    });
    function showCountries() {
        $(this).addClass('hover-countrybtn').find('.select-country').show();
    }
    function hideCountries() {
        $(this).removeClass('hover-countrybtn').find('.select-country').hide();
    }
</script>
