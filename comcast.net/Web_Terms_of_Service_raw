   <script type="text/javascript">
    var CIMA = CIMA || {};

    CIMA.write_cookie = function(name, value, path, domain) {
      document.cookie = name + '=' + value + '; path=' + path + '; domain=' + domain;
    };

    CIMA.read_cookie = function(name) {
      var value = "; " + document.cookie,
          parts = value.split("; " + name + "=");
      return (parts.length === 2) ? parts.pop().split(";").shift() : false;
    };

    CIMA.reset_cookie = function(name, path, domain) {
      document.cookie = name + '=; expires=Thu, 01 Jan 1970 00:00:01 GMT; path=' + path + '; domain=' + domain;
    };

    CIMA.can_write_cookies = function(domain) {
      var test_cookie_name = 'weWillNeverUseThis',
          test_value = 'set';
      CIMA.write_cookie(test_cookie_name, test_value, '/', domain);
      if (CIMA.read_cookie(test_cookie_name) === test_value) {
        CIMA.reset_cookie(test_cookie_name, '/', domain);
        return true;
      }
      return false;
    };

    CIMA.authenticate = function() {
      var authentication_url;

      // eliminate consecutive IPA calls if user is off-network.
      // only attempt IP Auth if no MYG cookie exists, or no previous IPA attempt was made (no ipa_failed cookie)
      if (!CIMA.read_cookie('MYG') && CIMA.read_cookie('ipa_failed') !== '1') {
        authentication_url = CIMA.build_authentication_url(true); // true: pass ipa param with CIMA call
      } else {
        authentication_url = CIMA.build_authentication_url(false); // false: do not pass ipa param with CIMA call
      }
      if (authentication_url) {
        window.location = authentication_url;
      }
    };

    CIMA.build_authentication_url = function(use_ipa) {
      use_ipa = use_ipa || false;
      var target_url,
          ipa_param = use_ipa ? 'ipAuth=true&' : '';

      if (CIM && CIM.properties && CIM.properties["passiveOauthEndpoint"]) {
        target_url = CIM.properties['passiveOauthEndpoint'];
        if (target_url.indexOf('?') >= 0) {
          target_url = target_url + '&';
        } else {
          target_url = target_url + '?';
        }
        target_url = target_url + ipa_param + 'continue=' + window.location;
      }
      return target_url;
    };

    CIMA.authenticate_if_needed = function() {
      var flag_cookie_name = 'authChecked',
          domain = '.' + document.location.hostname;

      if (!CIMA.read_cookie("_Xfinity-Portal_session")){
        // no session cookie, user not logged in
        if (CIMA.can_write_cookies(domain)) {
          if (CIMA.read_cookie(flag_cookie_name) === '1') {
            // passive oauth was already attempted, only reset the cookie
            CIMA.write_cookie(flag_cookie_name, '0', '/', domain);
            if (!CIMA.read_cookie('MYG')) {
              // we can assume CIMA IP Auth failed because no MYG was set after initial oauth attempt with IPA param
              CIMA.write_cookie('ipa_failed', '1', '/', domain);
            }
          }
          else {
            // passive oauth was not attempted - do so now and set the cookie flag
            CIMA.write_cookie(flag_cookie_name, '1', '/', domain);
            CIMA.authenticate();
          }
        }
      } else {
        // if user has session, remove ipa_failed cookie if it exists
        if (CIMA.can_write_cookies(domain)) {
          CIMA.reset_cookie('ipa_failed', '/', domain);
        }
      }
    };

    CIMA.authenticate_if_needed();
    </script>





