     <script type="text/javascript">

// Set header background photo Image Credit depending on the image shown

	var currentDate = new Date();
	var currentHour = currentDate.getHours();
	//currentHour = 15;

	var datSunrise = 5;
	var datDay = 10;
	var datMidday = 15;
	var datSunset = 19;
	var datNight = 22;
	
	if (currentHour >= datSunrise && currentHour < datDay) {
			
		document.getElementById("imageCredit1").style.display = "block";


	} else if (currentHour >= datDay && currentHour < datMidday) {
		
		document.getElementById("imageCredit2").style.display = "block";

		
	} else if (currentHour >= datMidday && currentHour < datSunset) {
		
		document.getElementById("imageCredit3").style.display = "block";

		
	} else if (currentHour >= datSunset && currentHour < datNight) {
		
		document.getElementById("imageCredit4").style.display = "block";

		
	} else {
		
		document.getElementById("imageCredit5").style.display = "block";

		
	}

</script>   
