<script type="text/javascript">
	var googleAdSlots = {};
	googletag.cmd.push(function()
	{googleAdSlots['div-gpt-ad-top_banner'] = googletag.defineSlot('/1010203/728x90A_General', [728,90], 'div-gpt-ad-top_banner').addService(googletag.pubads());
googleAdSlots['div-gpt-ad-ad_footer_banner'] = googletag.defineSlot('/1010203/728x90B_General', [728,90], 'div-gpt-ad-ad_footer_banner').addService(googletag.pubads());
googletag.pubads().collapseEmptyDivs();
googletag.pubads().enableSingleRequest();
googletag.enableServices();
		
		
		
					googletag.pubads().setTargeting('contentid', ['bf01f1afcc10851fcf2980bc43c3412c']);
				
		
		
		googletag.pubads().setTargeting('ezclass', '60');
		
		
		
		(function(){
			var hasFlash = ((typeof navigator.plugins != "undefined" && typeof navigator.plugins["Shockwave Flash"] == "object") || (window.ActiveXObject && (new ActiveXObject("ShockwaveFlash.ShockwaveFlash")) != false));
			hasFlash = hasFlash ? 'yes' : 'no';
			googletag.pubads().setTargeting('hasFlash', hasFlash);
		})();
		

		
		googletag.pubads().setTargeting('page_url', jQuery( 'head link[rel = "canonical" ]' ).attr( 'href' ) );

		
		
		(function(){
				var visitNum;
				var vn = 1;
				var parts = document.cookie.split("s_vnum=");
				if (parts.length === 2) {
					vn = parts.pop().split(";").shift().split("vn%3D").pop();
				}
				vn = parseInt(vn);
				if (vn === 1) {
					visitNum = "1";
				} 
				else if (vn > 1 && vn < 5) {
					visitNum = "2-4";
				}
				else if (vn > 4) {
					visitNum = "5 or more";
				}
				else {
					visitNum = "unknown";
				}
				googletag.pubads().setTargeting('visitNum', visitNum );
		})();
		


		
		
		(function(){
				var visitDepth;
				var dov = 1;
				var parts = document.cookie.split("s_depth=");
				if (parts.length === 2) {
					dov = parts.pop().split(";").shift();
				}
				dov = parseInt(dov);
				if (dov < 5) {
					visitDepth = dov.toString();
				} 
				else if (dov < 11) {
					visitDepth = "5-10";
				}
				else if (dov < 21) {
					visitDepth = "11-20";
				}
				else {
					visitDepth = "21 or more";
				}
				googletag.pubads().setTargeting('visitDepth', visitDepth);
		})();
		

		
		
		(function()
		{
			var quantSegs = $.cookie( 'QuantCastAudienceSegments' );
			if( quantSegs )
			{
				googletag.pubads().setTargeting( 'qcsegments', quantSegs);
			}
		})();

		googletag.pubads().addEventListener( 'slotRenderEnded', function( event )
		{
			// Mark as rendered
			$.each( googleAdSlots, function()
			{
				if( this.k == event.slot.getName() )
				{
					this.rendered = new Date().getTime();
				}
			});
			
			// Check if we're done
			var done = true;
			$.each( googleAdSlots, function()
			{
				if( !this.rendered )
				{
					done = false;
				}
			});

			if( done )
			{
				if( typeof s != 'undefined' )
				{
					var map =
					{
						44 : 'div-gpt-ad-top_banner',
						45 : 'div-gpt-ad-ad_block_1',
						46 : 'div-gpt-ad-ad_block_2',
						47 : 'div-gpt-ad-ad_block_3',
						48 : 'div-gpt-ad-ad_footer_banner',
					}
					s.linkTrackVars='prop3,prop4,prop5,prop21,prop36,prop40,prop44,prop45,prop46,prop47,prop48,eVar36,events';
					s.linkTrackEvents='event3';
					s.events='event3';
					
					$.each( map, function( id )
					{
						if( typeof googleAdSlots[ this ] != 'undefined' )
						{
							s[ 'prop' + id ] = googleAdSlots[ this ].rendered - javascriptStart;
						}
					});
					
					s.tl( true, 'o', 'Ad load complete' );
				}
			}
		});
					

	});
		
	
		//on page load complete, we submit collected info to omniture
		jQuery( googletag ).on( 'gpt-page_load_complete', function( e,level,message,service,slot,reference )
		{
		});

		//stores loading time for specific ad slots
		jQuery( googletag ).on( 'gpt-slot_rendered', function( e,level,message,service,slot,reference,timing )
		{
			var adSlotNames = [  /728x90A/ig, /300x250-600-1050/ig, /300x250B/ig, /300x250C/ig, /728x90B/ig  ];
			googletag.adUnitTiming  = googletag.adUnitTiming || [ '', '', '', '', '' ];
			for( var i = 0; i < adSlotNames.length; i++ )
			{
				if( message.search( adSlotNames[ i ] ) > -1 )
				{
					googletag.adUnitTiming[ i ] = timing;
				}
			}
		});
	
