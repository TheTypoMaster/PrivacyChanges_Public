// Do not display if another overlay is showing
if (ta.overlays && ta.overlays.getCurrent && ta.overlays.getCurrent()) {
return;
}
// Do not display if meta focus (meta smoke-out) is showing
if (ta.metaFocus && ta.metaFocus.utils && ta.metaFocus.utils.isShowing()) {
return;
}
// Do not display on world- or non-geo- forums pages
}, 250); // setTimeout, delay for display
});
} catch (e) {
ta.util.error.record(e, 'Error with Viator overlay registration display logic.');
}
</script>
