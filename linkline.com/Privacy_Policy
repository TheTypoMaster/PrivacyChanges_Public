<script type="text/javascript">
$(document).ready(function(){

      var count = 30;
	  var countdown = setInterval(function(){
	    $("div#countdown").html("Redirecting in " + count + " seconds...");
	    if (count == 0) {
	      clearInterval(countdown);
	      window.open('http://www.uia.net/', "_self");
	
	    }
	    count--;
	  }, 1000);

});
</script>
