        $(document).ready(function () {
            $(document).on("click", ".facebuttonclass", function () {
                var $this = $(this).parent().find('.faceimage');

                //if ($this.attr('title') == undefined)
                //    linktext = "Car Image";
                //else
                //   linktext = $this.attr('title');

                var description = $("meta[property='og:description']").attr("content");
                var title = $("meta[property='og:title']").attr("content");
                var serverpageurl = "http://www.nadaguides.com/Privacy-policy";
                var ogurl = $("meta[property='og:url']").attr("content");

                if (description == undefined || description == '')
                    description = " ";
                if (title == undefined || title == '')
                    title = " ";
                if (ogurl == undefined || ogurl == '')
                    ogurl = serverpageurl;

                var obj = {
                    method: 'feed',
                    link: ogurl,
                    picture: $this.attr('src'),
                    name: title
                    , caption: "NADAguides: Vehicle Research Made Simple "  
                    , description: description
                };
                //debugger;
                //function callback(response) {
                //    alert("Post ID: " + response['post_id']);
                //}
                FB.ui(obj);//, callback);
                return false;
            });

            $('.faceimage').parent().hover(function () {
                if ($(this).find('.facebuttonclass').length == 0) {
                    $(this).append('<span class="facebuttonclass" style="background:url(http://images.nadaguides.com/redesign/FB-f-Logo__blue_29.png) no-repeat top left;height:40px;width:40px;position:absolute;left:'
                        + Math.round($(this).find('.faceimage').position().left + (($(this).find('.faceimage').width() - 35)))
                        + 'px;top:' + Math.round($(this).find('.faceimage').position().top)
                        + 'px;"></span>');
                }
            }, function () {
                $(this).find('.facebuttonclass').remove();
            });

        });
        
