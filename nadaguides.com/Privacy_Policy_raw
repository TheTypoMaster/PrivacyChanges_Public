    function display_sub(subPolicy) {
       
        $('#privacy_policy_page').hide();
        switch (subPolicy) {
            case "general_policy_page":
                $('#general_policy_page').show();
                break;
            case "vehicle_value_page":
                $('#vehicle_value_page').show();
                break;
            case "shoppingCart_policy_page":
                $('#shoppingCart_policy_page').show();
                break;
            case "optional_customer_survey":
                $('#optional_customer_survey').show();
                break;
            case "standard_image_link":
                $('#standard_image_link').show();
                break;
            case "customer_feedback_policy":
                $('#customer_feedback_policy').show();
                break;
            default: $('#privacy_policy_page').show();
        }
   
    }
    
    
</script> 

