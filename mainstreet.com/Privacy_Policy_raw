        $(function() {
            var pageMetaData;
            if (typeof Keystone !== 'undefined') {
                pageMetaData = Keystone.pageMetaData;
            }
            $(document.body).trigger('pageLoad', { pageMetaData: pageMetaData });
        });
    </script>

