      // Check for mobile device and redirect if appropriate, but not on pages in the list below
      (function() {
        var excludedPages = [
           'terms'
          ,'privacy'
          ,'mint-landing'
          ,'budget-landing'
          ,'two-for-the-money'
          ,'utilities'
          ,'biller-resources'
          ,'mintbills'
          ,'how-mint-bills-works'
          ,'help'

        ];
        var dev = null;

        if (excludedPages.indexOf(window.location.pathname.split('/')[1]) >= 0 || document.cookie.indexOf('noMobile=') >= 0) {
          return;
        }

        if (navigator.userAgent.match(/Android/i)) {
          dev = 'android';
        }
        else if (navigator.userAgent.match(/iPhone/i) || navigator.userAgent.match(/iPod/i)) {
          dev = "ios";
        }
        else if (navigator.userAgent.match(/Windows Phone/i)) {
          dev = 'win8';
        }
        else if (navigator.userAgent.match(/Touch/i)) {
          dev = 'win8desk';
        }
        else if (navigator.userAgent.match(/iPad/i)) {
          dev = 'ios';
        }
        if (dev) {
          window.location.href = '/html/mobile.php?' + dev;
        }
      }());

    </script>
