<script>
    var customerLoggedIn = jQuery.cookie('cust_logged_in');
    if (customerLoggedIn) {
        var html;
        if (customerLoggedIn == 1 || customerLoggedIn.length > 12) {
            html = '<a rel="nofollow" title="Log Out" ' +
                'href="https://shop.harborfreight.com/customer/account/logout/">Log Out</a>' +
                '/ <a rel="nofollow" title="My Account" href="https://shop.harborfreight.com/customer/account/">My Account</a>';
        } else {
            html = '<a rel="nofollow" title="My Account"' +
                'href="https://shop.harborfreight.com/customer/account/">Hello ' + encodeURIComponent(customerLoggedIn) + '</a>' +
                ' / <a rel="nofollow" title="Log Out" href="https://shop.harborfreight.com/customer/account/logout/">Log Out</a>';
        }
        document.getElementById('myaccountlogin').innerHTML = html;
    }

    jQuery('.top-link-cart').attr("title", "My Cart()");
</script>
        

