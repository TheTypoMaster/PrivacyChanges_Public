 <script>
     var cartQty = jQuery.cookie("cart_qty");

     if (cartQty != null && isInt(cartQty) && cartQty >= 0) {

         var cartText;
         if (cartQty == 1) {
             cartText = "(1 Item)";
         } else if (cartQty > 1) {
             cartText = "(" + cartQty + " Items)"
         }

         if (cartText) {
             var myCart   = jQuery("#my-cart");
             var cartHtml = myCart.html().replace('title="My Cart"', 'title="My Cart ' + cartText + '"');
             myCart.html(cartHtml);
         }

         jQuery("#cart-qty").html(cartQty);
     }
</script>

