		<script>
			window.fbAsyncInit = function() {
				FB.init({
					appId 	 : 154739397877434, // App ID
					status     : true, // check login status
					cookie     : true, // enable cookies to allow the server to access the session
					xfbml      : true  // parse XFBML
				});
				FB.Event.subscribe('comment.create',
				function(response) {
					var commentQuery = FB.Data.query("SELECT text, fromid FROM comment WHERE post_fbid='"+response.commentID+"' AND object_id IN (SELECT comments_fbid FROM link_stat WHERE url='"+response.href+"')");
					var userQuery = FB.Data.query("SELECT name, pic_square FROM user WHERE uid in (select fromid from {0})", commentQuery);

					FB.Data.waitOn([commentQuery, userQuery], function() {
						var commentRow = commentQuery.value[0];
						var userRow = userQuery.value[0];
						saveSocialComment(commentRow.text, userRow.name, userRow.pic_square);
						//console.log(userRow.name+" (id: "+commentRow.fromid+") posted the comment: "+commentRow.text+" User Thumbnail URL is:"+userRow.pic_square);
					});
				}
			);
			};

			function fbLogin() {
				FB.getLoginStatus(function(response) {
					if (response.status === 'connected') {
						callLogin(response.authResponse.accessToken);
					} else if (response.status === 'not_authorized') {
						login();
					} else {
						login();
					}
				});
			}

			function login() {
				FB.login(function(response) {
					if (response.authResponse) {
						callLogin(response.authResponse.accessToken);
					} else {
						// no action
					}
				});
			}

			function callLogin(access_token) {
				$.post('/user/fbLogin',{'access_token':access_token}, 
				function(response) {
					json = $.parseJSON(response);
					if(json.type == 'SUCCESS') {
						window.location = json.homepage;
					} else if (json.type == 'ERROR') {
						// There was an error, no action
					}
				});
			}

			// Load the SDK Asynchronously
			(function(d){
				var js, id = 'facebook-jssdk'; if (d.getElementById(id)) {return;}
				js = d.createElement('script'); js.id = id; js.async = true;
				js.src = "//connect.facebook.net/en_US/all.js";
				d.getElementsByTagName('head')[0].appendChild(js);
			}(document));
			
			if(top !== self) {
				top.location.href = self.location.href;
			}
		</script>
