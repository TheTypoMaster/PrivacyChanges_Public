<script type="text/javascript">
	function changeTabCont(link){
		if (link.match('#')) {
			var toChange = link.split('#')[1];
			$("ul.profile-nav li").removeClass("active");
			$("a." + toChange).parent().parent().addClass("active");
			$("div.st-cont-wrapper").addClass("display-none")
			$("." + toChange + "-cont").removeClass("display-none")
		}
	}

	function sendSupport()
	{
		var email = $("#email").val();
		var name = $("#name").val();
		var message = $("#messages").val();
		
		$.post(
			'http://www.videobash.com/user/support',
			{'email':email, 'name':name, 'message':message},
			function(response) {
				if(response == 'Success'){
					document.location = 'http://www.videobash.com/support_confirmation';
				}
				else{
					$("#response").text(response);
				}
			}
		);
	}
	
	$(document).ready(function(){
		changeTabCont(document.location.toString());
		$("ul.profile-nav li > a, ul.list-image li a").click(function(){
			changeTabCont($(this).attr("href"));
		});
	});
</script>
