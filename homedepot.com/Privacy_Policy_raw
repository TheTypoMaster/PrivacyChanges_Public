	<script type="text/javascript">
			//List of schemes will be pushed to the Javascript array and passed to the refresh function
	//Certona Global Resx Object and initiate tracking cookie and call refresh function which calls Recommendation API

	!function (){
		var schemaList = $(".certona-content"),
			schemaArr = [],
			levels = ""
			resx = {};
		
		$.each(schemaList, function(){
			var $this = $(this);
			schemaArr.push($this.attr("id"));
		});
		
		if(schemaArr.length > 0){
			resx = {
			   appid: "HOMEDEPOT01",
			   links: "",
			   maxProducts: "16",
			   
			   levels: "Building Materials",
			   certonaSchema: schemaArr
			};
	
			//initialize certona tracking cookie creation
			if(certonaResx !== undefined){ certonaResx.getURL(); }
			THD.Thirdparty.Certona.refresh(resx);
		}
	}();
	</script>



	
