function getHostNameNonSecure() {
	if(''==='localhost'){
		return 'localhost';
	} else {
		if (''==='s') {
			return 'hd-pr71stg.homedepot.com';
		} else {
			return 'www.homedepot.com';
		}
	}
}

function getHostNameSecure() {
	if(''==='localhost'){
		return 'localhost';
	} else {
		if (''==='s') {
			return 'secure.hd-pr71stg.homedepot.com';
		} else {
			return 'secure2.homedepot.com';
		}
	}
}

