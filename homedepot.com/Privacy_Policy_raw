<script type="text/javascript">

var crumbStr = '', 
	crumbStrBuilder = '',
	lpUnit = '';


	crumbStrBuilder += 'Help and Customer Service Center';

	crumbStrBuilder += '>Customer Support: Privacy Security';









crumbStr += crumbStrBuilder.replace(/ /gi, '');

crumbStr = crumbStr.toLowerCase();

var splitCrumbStr = function(){
	var arrCrumbStr;
    if(typeof crumbStr !== 'undefined' && crumbStr !== null){
  	var arrCrumbStr = crumbStr.split('>');
  			return arrCrumbStr;
    }
  	else{
  			 return arrCrumbStr = "";
  	}
}

function lpGetStatus(){
	if(typeof readCookie("THD_USERSTATUS") !== undefined){
		if(readCookie("THD_USERSTATUS") ===  ""){
			return "guest";	
		}
		else if(readCookie("THD_USERSTATUS") ===  "1"){
			return "authenticated";	
		}
	}
}

function lpGetRegUser(){
	if(typeof readCookie("THD_USERSTATUS") !== undefined){
		if(readCookie("THD_USERSTATUS") ===  ""){
			return "no";	
		}
		else if(readCookie("THD_USERSTATUS") ===  "1"){
			return "yes";	
		}
	}
}

function lpGetUsertype(){
	if(typeof readCookie("THD_USERTYPE") !== undefined){
		if(readCookie("THD_USERTYPE") ===  "C"){
			return "no";	
		}
		else if(readCookie("THD_USERTYPE") ===  "P"){
			return "yes";	
		}
	}
}

var lpSetZipCode = readCookie("THD_STRFINDERZIP");
var lpSetStoreNum = readCookie("THD-LOC-STORE");
var lpSetCustStatus = lpGetStatus();
var lpSetUserType = lpGetUsertype();
var lpSetRegisteredUse = lpGetRegUser();
var getSections = splitCrumbStr();
for(i=0;i<4;i++){
	if(typeof getSections[i] == 'undefined') {
		getSections[i] = "null";
	}
}
if(lpUnit != ""){
	getSections[0] = lpUnit;
}

var lpTag = lpTag || {}; lpTag.vars = lpTag.vars  || []; lpTag.dbs = lpTag.dbs || [];
lpTag.section = 'thd';

var arrLPvars = [
	{ scope:'page', name:'unit', value:getSections[0] }, 
	{ scope:'session', name:'language', value:'english' },
	{ scope:'page', name:'Hierarchy', value:crumbStr },  
	{ scope:'page', name:'Section', value:getSections[1] },	
	{ scope:'page', name:'SubSection', value:getSections[2] },	
	{ scope:'page', name:'pageName', value:crumbStr },	
	{ scope:'page', name:'pageType', value:lpPageType },  
	{ scope:'session', name:'registeredUser', value:lpSetRegisteredUse },
	{ scope:'session', name:'CustomerStatus', value:lpSetCustStatus },
	{ scope:'session', name:'storeNumber', value:lpSetStoreNum },
	{ scope:'session', name:'zipCode', value:lpSetZipCode  },
	{ scope:'session', name:'ProVisitor', value:lpSetUserType },
	{ scope:'session', name:'mobileDevice', value:'false' }
];

lpTag.vars.push(arrLPvars);

<!-- Live Person END -->

</script>
