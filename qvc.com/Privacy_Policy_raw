		var ctmbAttribute = getUserAttrCookie();
		var cmKeyword = gup('keyword');                  
		
		if(cmKeyword == "") cmKeyword = null;    
		 
		var cmSearchTerm = "";
		var cmResultCount = "";
		    
		if(cmKeyword != null && window.location.href.indexOf("CatalogSearch?") == -1){                        
			cmSearchTerm = 'REDIRECT: ' + cmKeyword;
			cmResultCount = "1";
		}
		
		cmCreatePageviewTag("NAVIGATION - Customer Service \x3E FAQs \x3E The QVC Privacy Statement","LPCustomerService", cmSearchTerm, cmResultCount, "null-_-null-_-null-_-null-_-null-_-null-_-null-_-null-_-null-_-null-_-" + ctmbAttribute + "-_-null-_-WSC");									 
			
		
		function callCreatePageviewTag(){	
			var ctmbAttr = getUserAttrCookie();
			cmCreatePageviewTag("NAVIGATION - Customer Service \x3E FAQs \x3E The QVC Privacy Statement - Generic Form Error: User Invalid Input","LPCustomerService","","", "null-_-null-_-null-_-null-_-null-_-null-_-null-_-null-_-null-_-null-_-" + ctmbAttr + "-_-null-_-WSC");
		}

				
		if($("cmCategoryId") && $("cmEventId"))
			cmCreateConversionEventTag($("cmEventId").value, '1', $("cmCategoryId").value, '0');
	</script>

	

