<script type="text/javascript">

function CMCTemplateName(container){
	var cmcTemplateName = '';
	var bodyClass = container.className ? container.className : '';
	if(bodyClass.split(' ').length == 2){
		cmcTemplateName = bodyClass.split(' ')[1];
	}
	return cmcTemplateName;
}

function CMCEspotID(alink){
	var cmcEspotID = '';
	
	var container = alink.parentElement;
	// TODO: Get the espot ID
	return cmcEspotID;
}

function emptyFilter(index,text){
	return text != null && text != '';
}

function trimMapper(index,item){
	return (item.innerText == null ? null : item.innerText.trim());
}

//For BalconyContent
(function(){
	if($('div#divPageBuilderBalcony textarea.ConversionTag').length == 0) return; 	// There is no BalconyContent
	
	var containers = ['div#divPageBuilderBalcony div.divBalconyBanner', 'div#divPageBuilderBalcony div.divBalconySpotlight', 'div#divPageBuilderBalcony div.divBalconyOther'];
	for(var i = 0; i < containers.length; i++){
		var divs = $(containers[i]);
		
		
		for(var t = 0; t < divs.length; t++){
			var div = $(divs[t]);
			var arg5 = div.children('a').map(trimMapper).filter(emptyFilter).join(' | ');
			if(arg5 == '')arg5 = 'NULL';
			
			if(arg5!=null) arg5 = arg5.replace("\n"," ");
			
			var conversionTag = new Function(document.getElementById(div.attr('conversionTag')).value.replace("CM_PAGE_ID", "NAVIGATION - Customer Service > FAQs > The QVC Privacy Statement").replace("ARG5", arg5));
			conversionTag();
			
			
			var conversionClickTag = new Function(document.getElementById(div.attr('eventTag')).value.replace("CM_PAGE_ID", "NAVIGATION - Customer Service > FAQs > The QVC Privacy Statement").replace("ARG5", arg5));
			
			var links = div.children('a');
			for(var j = 0; j < links.length; j++){
				$(links[j]).on('click', conversionClickTag);
			}
		}
	}
})();

//For GeneralContent
(function(){
	if(document.getElementById('GeneralContentConversionEvent') == null) return;	//This is no GeneralContent
	
	var conversionTagStr = $('#GeneralContentConversionEvent').val();
	var arg5 = $('div#divGeneralContent a').map(trimMapper).filter(emptyFilter).join(' | ');
	
	if(arg5 == null || arg5 == "") {
		arg5 = "NULL";
	}
	var conversionTag = new Function(conversionTagStr.replace("CM_PAGE_ID", "NAVIGATION - Customer Service > FAQs > The QVC Privacy Statement").replace("ARG5",arg5));
	
	conversionTag();
	
	var conversionClickTagStr = $('#GeneralContentConversionClickEvent').val();
	var conversionClickTag = new Function(conversionClickTagStr.replace("CM_PAGE_ID", "NAVIGATION - Customer Service > FAQs > The QVC Privacy Statement"));
	var conversionLinks = $('div#divGeneralContent a');
	
	for(var i = 0; i < conversionLinks.length; i++){
		var conversionLink = conversionLinks[i];
		//var linkClass = conversionLink.get('class') ? conversionLink.get('class') : "";
		//if(linkClass == "GeneralContentConversionEvent"){
			$(conversionLink).on('click', conversionClickTag);
		//}
	}
})();

//For TopContent
(function(){
	if($('textarea.TopContentConversionEvent').length == 0) return; 	// There is no TopContent
	
	var conversionTags = $('textarea.TopContentConversionEvent').map(function(idx,item){return item.value});
	for(var i = 0; i < conversionTags.length; i++){
		if(conversionTags[i] != null && conversionTags[i] != ''){
			var needThrow = function(){
				var containers = $('div#divTopContent$'.replace("$", (i + 1)));
				
				return (containers.length > 0 && containers.map(trimMapper).filter(emptyFilter)[0] != '');
			};
			if(needThrow()){
				var arg5 = $('div#divTopContent$ a'.replace("$", (i + 1))).map(trimMapper).filter(emptyFilter).join(' | ');
				
				var conversionTagStr = conversionTags[i].replace("CM_PAGE_ID", "NAVIGATION - Customer Service > FAQs > The QVC Privacy Statement").replace("ARG5", arg5);
				var cmcTemplateName = $('div#divTopContent$'.replace("$", (i + 1))).first().attr('cmcTemplate');
				
				conversionTagStr = conversionTagStr.replace("CMC_TEMPLATE_NAME", cmcTemplateName);
				var conversionTag = new Function(conversionTagStr);
				conversionTag();
			}
		}
	}
	
	var containers = ['div#divTopContent1', 'div#divTopContent2', 'div#divTopContent3'];
	var conversionClickTags = $('textarea.TopContentConversionClickEvent').map(function(idx,item){return item.value});
	for(var i = 0; i < containers.length; i++){
		if($(containers[i]).length < 1)return; 	//No container div found.
		
		var conversionLinks = $('div#divContent $ a'.replace("$", containers[i]));
		
		var conversionClickEventTagStr = conversionClickTags[i];
		if(conversionClickEventTagStr != null){
			conversionClickEventTagStr = conversionClickEventTagStr.replace("CM_PAGE_ID", "NAVIGATION - Customer Service > FAQs > The QVC Privacy Statement");
			
			var espotType = $(containers[i]).first().attr('espotType');
			
			if(espotType == 'simple' || espotType == 'other'){
				
				var espotID = $(containers[i]).first().attr('espotID');
				
				var conversionClickFunc = new Function(conversionClickEventTagStr.replace("CMC_ESPOT_ID", espotID));
				
				for(var j = 0; j < conversionLinks.length; j++){
					var conversionLink = conversionLinks[j];
					$(conversionLink).on('click', conversionClickFunc);
				}
			}else{
				//var numEspots = $(containers[i])[0].get('numEspots');
				var espots = $('div#divBottomContent$ div.contentBlock'.replace('$', (i + 1)));
				
				for(var j = 0; j < espots.length; j++){
					var espot = espots[j];
					var links = $(espot).children('a');
					
					var espotID = $(espot).attr('espotID');
					
					for(var k = 0; k < links.length; k++){
						$(links[k]).on('click', function(){
							new Function(conversionClickEventTagStr.replace("CMC_ESPOT_ID", espotID))();
						});
					}
				}
			}
		}
	}
})();

//For BottomContent
(function(){
	if($('textarea.BottomContentConversionEvent').length == 0) return; 	// There is no BottomContent
	var conversionTags = $('textarea.BottomContentConversionEvent').map(function(idx, item){return item.value});
	for(var i = 0; i < conversionTags.length; i++){
		if(conversionTags[i] != null && conversionTags[i] != ''){
			var needThrow = function(){
				var containers = $('div#divBottomContent$'.replace("$", (i + 1)));
				
				return (containers.length > 0 && containers.map(trimMapper).filter(emptyFilter)[0] != '');
			};
			if(needThrow()){
				var conversionTagStr = conversionTags[i].replace("CM_PAGE_ID", "NAVIGATION - Customer Service > FAQs > The QVC Privacy Statement");
				
				var cmcTemplateName = $('div#divBottomContent$'.replace("$", (i + 1))).first().attr('cmcTemplate');
				
				conversionTagStr = conversionTagStr.replace("CMC_TEMPLATE_NAME", cmcTemplateName);

				var conversionTag = new Function(conversionTagStr);
				conversionTag();
			}
		}
	}
	
	var containers = ['div#divBottomContent1', 'div#divBottomContent2', 'div#divBottomContent3'];
	var conversionClickTags = $('textarea.BottomContentConversionClickEvent').map(function(idx, item){return item.vallue;});
	for(var i = 0; i < containers.length; i++){
		if($(containers[i]).length < 1)return; 
		
		var conversionLinks = $('div#divContent $ a'.replace("$", containers[i]));
		
		var conversionClickEventTagStr = conversionClickTags[i];
		if(conversionClickEventTagStr != null){
			conversionClickEventTagStr = conversionClickEventTagStr.replace("CM_PAGE_ID", "NAVIGATION - Customer Service > FAQs > The QVC Privacy Statement");
			
			var espotType = $(containers[i]).first().attr('espotType');
			
			if(espotType == 'simple' || espotType == 'other'){
				var espotID = $(containers[i]).first().attr('espotID');
				
				var conversionClickFunc = new Function(conversionClickEventTagStr.replace("CMC_ESPOT_ID", espotID));
				
				for(var j = 0; j < conversionLinks.length; j++){
					$(conversionLinks[j]).on('click', conversionClickFunc);
				}
			} else {
				var espots = $('div#divBottomContent$ div.contentBlock'.replace('$', (i + 1)));
				
				for(var j = 0; j < espots.length; j++){
					var espot = espots[j];
					var links = $(espot).children('a');
					
					var espotID = $(espot).attr('espotID');
					
					for(var k = 0; k < links.length; k++){
						$(links[k]).on('click', function(){
							new Function(conversionClickEventTagStr.replace("CMC_ESPOT_ID", espotID))();
						});
					}
				}
			}
		}
	}
})();


//For MidContent
(function(){
	if($('textarea.MidContentConversionEvent').length == 0) return; 	// There is no MidContent
	var conversionTags = $('textarea.MidContentConversionEvent').map(function(idx,item){return item.value;});
	for(var i = 0; i < conversionTags.length; i++){
		if(conversionTags[i] != null && conversionTags[i] != ''){
			var needThrow = function(){
				var containers = $('div#divMidContent$'.replace("$", (i + 1)));
				
				return (containers.length > 0 && containers.map(trimMapper).filter(emptyFilter)[0] != '');
			};
			if(needThrow()){
				var conversionTagStr = conversionTags[i].replace("CM_PAGE_ID", "NAVIGATION - Customer Service > FAQs > The QVC Privacy Statement");
				
				var cmcTemplateName = $('div#divMidContent$'.replace("$", (i + 1))).first().attr('cmcTemplate');
				
				conversionTagStr = conversionTagStr.replace("CMC_TEMPLATE_NAME", cmcTemplateName);

				var conversionTag = new Function(conversionTagStr);
				conversionTag();
			}
		}
	}
	
	var containers = ['div#divMidContent1', 'div#divMidContent2', 'div#divMidContent3'];
	var conversionClickTags = $('textarea.MidContentConversionClickEvent').map(function(idx,item){return item.value;});
	for(var i = 0; i < containers.length; i++){
		if($(containers[i]).length < 1)return;
		
		var conversionLinks = $('div#divContent $ a'.replace("$", containers[i]));
		var conversionClickEventTagStr = conversionClickTags[i];
		
		if(conversionClickEventTagStr != null){
			conversionClickEventTagStr = conversionClickEventTagStr.replace("CM_PAGE_ID", "NAVIGATION - Customer Service > FAQs > The QVC Privacy Statement");
			
			var espotType = $(containers[i]).first().attr('espotType');
			
			if(espotType == 'simple' || espotType == 'other'){
				var espotID = $(containers[i]).first().attr('espotID');
				
				var conversionClickFunc = new Function(conversionClickEventTagStr.replace("CMC_ESPOT_ID", espotID));
				
				for(var j = 0; j < conversionLinks.length; j++){
					$(conversionLinks[j]).on('click', conversionClickFunc);
				}
			} else {
				var espots = $('div#divMidContent$ div.contentBlock'.replace('$', (i + 1)));
				
				for(var j = 0; j < espots.length; j++){
					var espot = espots[j];
					var links = $(espot).children('a');
					
					var espotID = $(espot).attr('espotID');
					
					for(var k = 0; k < links.length; k++){
						$(links[k]).on('click', function(){
							new Function(conversionClickEventTagStr.replace("CMC_ESPOT_ID", espotID))();
						});
					}
				}
			}
		}
	}
})();

//For ResultsList
(function(){
	if(document.getElementById('ResultsListConversionEvent') == null) return; 	// There have not ResultsList
	var conversionTagStr = document.getElementById('ResultsListConversionEvent').value;
	var conversionTag = new Function(conversionTagStr.replace("CM_PAGE_ID", "NAVIGATION - Customer Service > FAQs > The QVC Privacy Statement"));

	conversionTag();
	
})();


(function(){
	var addScToLink = function(links){
	
		var navigationUrl=document.location.href;
		var scContent="";
	    if (navigationUrl.indexOf("REFINEMENTS") != -1){
	    scContent="User";
	    }else if(navigationUrl.indexOf("CatalogSearch")!=-1){
	    scContent="SRCH";
	    }else if(navigationUrl.indexOf(".category.")!=-1){
	    scContent="Targeted";
	    }else if(navigationUrl.indexOf(".content.")!=-1){
	    scContent="Tailored";
	    }
	   
	    
		
		for(var i=0; i<links.length; i++){
			var originalUrl=links[i].href;
			if(originalUrl == null || originalUrl == '' || originalUrl.indexOf('javascript') != -1){
				continue;
			}
			
			if(originalUrl.indexOf('sc=') == -1 && (originalUrl.indexOf('ProductDisplay') != -1 || originalUrl.indexOf('.product.') != -1)){
				if(originalUrl.split('?').length == 1){
					var tempUrl = originalUrl + "?sc="+scContent;
					$(links[i]).attr("href", tempUrl);
				}else {
					var tempUrl=originalUrl + "&sc="+scContent;
					$(links[i]).attr("href", tempUrl);
				}
	 		}else if(originalUrl.indexOf('-Zone') != -1 || originalUrl.indexOf('MERCHZONE') != -1){	
				var tempUrl = originalUrl.replace(/\bsc=[A-Za-z0-9]*\-(Zone|null)/g, "sc=Spotlight");
				$(links[i]).attr("href", tempUrl);
			}
	 	}	
	};
	
	var BreadcrubsLinks=$('div#breadCrumbs a');  
	addScToLink(BreadcrubsLinks);
	
	var AutomatedMessagingLinks=$('div#divBreadCrumbsPageTitleWrapper p a');
	addScToLink(BreadcrubsLinks);

	var GeneralContentLinks=$('div#divGeneralContent a');
	addScToLink(GeneralContentLinks);
	
	var balcanyContentLinks = $('div#divPageBuilderBalcony a');
	addScToLink(balcanyContentLinks);
	
	var generalContentLinks = $('div#divGeneralContent a');
	addScToLink(generalContentLinks);
	
	var leftRailLinks = $('div.divNavCustom a');
	addScToLink(leftRailLinks);
	
	var productListLinks = [];
	productListLinks = productListLinks.concat($('div#divTopContent1 a'));
	
	productListLinks = productListLinks.concat($('div#divTopContent2 a'));
	productListLinks = productListLinks.concat($('div#divTopContent3 a'));

	productListLinks = productListLinks.concat($('div#divMidContent1 a'));
	productListLinks = productListLinks.concat($('div#divMidContent2 a'));
	productListLinks = productListLinks.concat($('div#divMidContent3 a'));

	productListLinks = productListLinks.concat($('div#divBottomContent1 a'));
	productListLinks = productListLinks.concat($('div#divBottomContent2 a'));
	productListLinks = productListLinks.concat($('div#divBottomContent3 a'));
	
	addScToLink(productListLinks);
})();

</script>

