	for (var i = 0; i < noCheckBoxLinks.length; i++) {
		noCheckBoxLinks[i].onclick = function() {
			
		}
	}
	
	var linkChooseMores = $("p.chooseMore");
	for (var i = 0; i < linkChooseMores.length; i++) {
		linkChooseMores[i].onclick = function() {
			
		}
	}
	
}

//for defect 19322
function addLrfConversionTag() {
	var attr5s = $('div#divNavFaceted div h3');
	
	var attr5 = "";
	for (var i = 0; i < attr5s.length; i++) {
		var attr5One = attr5s[i];
		var label = '';
		if($(attr5One).children('a')[0]) {
			label = attr5One.innerText.replace($(attr5One).children('a')[0].innerText,"");
		} else {
			label = attr5One.innerText;
		}
		
		if(i == 0){
			attr5 += label;
		}else{
			attr5 += ' | ' + label;
		}

	}
	
	var funStr = $("textarea#lrcConversionTagTextarea").eq(0).text();
	funStr = funStr.replace('ATTRIBUTE5',attr5);
	
	var fun = new Function(funStr.substring(funStr.indexOf('cmCreat'),funStr.indexOf(')\');')+1));
	
	fun();
}
// end of defect 19322

function formatLRFAtt4(link) {
	var isSort = false;
	var isBoost = false;
	var isBury = false;
	var att4 = "";
	
	if (isBoost && isBury) {
		att4 = "BOOSTBURY | ";
	} else if (isBoost) {
		att4 = "BOOST | ";
	} else if (isBury) {
		att4 = "BURY | ";
	}
	if (isSort) {
		att4 += "SORT";
	}
	if (att4 != "" && att4.endsWith(" | ")) {
		att4 = att4.replace(" | ","");
	}
	return att4==""?null:att4;
}

