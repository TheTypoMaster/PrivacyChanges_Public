U-_-CREATENEWACCOUNT\"\x3ECreate a New QVC Account\x3C\x2Fa\x3E",
				speed : 500,
				mobilelimit : 10,
				content : function(h){
					var output = '<div id="'+this.id+'" class="'+this.state+'"><a id="'+this.linkid+'"  href="'+this.href+'" manual_cm_re="'+this.manual_cm_re+'" manual_cm_sp="'+this.manual_cm_sp+'">';
					output += '<span class="signInGreeting">'+this.text+'</span> ';
					output += '</a><div id="'+this.menuid+'" class="menuSignIn">'+h+"</div></div>";
					return output;
				},
				addevents : function(){
					var p = this;
				

				$('#'+p.id).on("mouseenter",function(event){
				tOut= setTimeout(function() {
		   	   			$('#'+p.id).children('#'+p.menuid).finish().fadeIn(p.speed);
						cmCreateManualLinkClickTag(document.URL+'&cm_re=MH-_-ACCOUNTMENU-_-'+responsiveViewVal.toUpperCase()+'&cm_sp=MH-_-ACCOUNTMENU-_-'+responsiveViewVal.toUpperCase());
		 
		 		}, 200);});
		 
		 		$('#'+p.id).on("mouseleave",function(event){
			 		clearTimeout(tOut);
			 		$('#'+p.id).children('#'+p.menuid).finish().fadeOut();
		 		});
				
					var click= 0;
					$('#'+p.linkid).on('touchstart', function(e){
					e.preventDefault();
					$('.divMainMenu').hide();
					$('#'+p.id).addClass('removeHover');
					if(click%2==0){
						$('.signInGreeting').addClass('hoverEffect');
						
						$('#'+p.id).children('#'+p.menuid).finish().fadeIn(p.speed);
						menuMask = document.createElement('div');
						menuMask.id = "menuClickMask";
						menuMask.addEventListener('touchstart',function(e){

							$('#'+p.menuid).finish().fadeOut();
							click++;
							this.parentNode.removeChild(this);
							$('.signInGreeting').removeClass('hoverEffect');
							$('#'+p.id).removeClass('removeHover');
							});
						document.body.appendChild(menuMask);
					}
					else{
						$('#'+p.id).children('#'+p.menuid).finish().fadeOut();
						$(menuMask).remove();
						$('.signInGreeting').removeClass('hoverEffect');
						
					}
					click++;
					console.log(click);
					});
				},
				build: function(){
					var isLoggedIn = null !== qCookie.get("qvcauthenticed");
					this.fn = qCookie.get("qvcfn");
					if(isLoggedIn || null !== this.fn){this.state ="plAuthenticated";}
					if(null !== this.fn){
	    				
	    				this.shfn = this.fn;
	    				if(this.fn.length > this.mobilelimit){this.shfn = this.fn.substring(0,this.mobilelimit)+"...";}
						this.text = "Hello, <span class=\"userfn\">" + this.fn+"</span><span class=\"userfnShort\">" + this.shfn+"</span>";
						document.write(this.content(this.menu));
					}else if(isLoggedIn){
						this.text = "Hello";
						this.subtextclass ="";
						this.href="http\x3A\x2F\x2Fwww.qvc.com\x2FMyAccountView\x3FlangId\x3D-1\x26storeId\x3D10251\x26catalogId\x3D10151";
						document.write(this.content(this.menu));
					}else{
						this.builddefault();
					}
				},
				builddefault: function(){
					try {
						document.write(this.content(this.menuDefault));
					}catch (e) {}
				},
				init: function(){
					this.build();
					this.addevents();
				}
			};
			pSignInLink.init();
		</script>
		
		
	</div> 

