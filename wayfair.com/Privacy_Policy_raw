  <script type="text/javascript">

    YUI_config.app.AdsShowGinmLoc = '';
    YUI_config.app.AdsShowGinmLink = 0;

    YUI_config.app.doAFS = '';
    YUI_config.app.adsenseLanguage = '1';
    YUI_config.app.AdPositions = '';
    YUI_config.app.sponsoredLinksTitle = 'Sponsored Links';
    YUI_config.app.store_domain = 'wayfair.com';
    YUI_config.app.isDetailPage = false
    window.wf = window.wf || {};
    window.wf.tracking = window.wf.tracking || {};
    window.wf.tracking.gmedia = window.wf.tracking.gmedia || {};

    wf.tracking.gmedia.AdsShowGinmLoc = '';
    wf.tracking.gmedia.AdsShowGinmLink = 0;

    wf.tracking.gmedia.doAFS = '';
    wf.tracking.gmedia.adsenseLanguage = '1';
    wf.tracking.gmedia.AdPositions = '';
    wf.tracking.gmedia.sponsoredLinksTitle = 'Sponsored Links';
    wf.tracking.gmedia.store_domain = 'wayfair.com';
    wf.tracking.gmedia.isDetailPage = false;
    wf.tracking.gmedia.sellerFirst = false

    
    // Sends a beacon back to us so that we can verify numbers
    // provided by google.
    function sendCsaBeacon(map) {
      if(!window.google_csa_send_beacon){
        return;
      }
      if(!window.csaBeaconRand){
        window.csaBeaconRand = Math.random();
      }
      var ts = new Date();
      var url  = [
          location.protocol, '//',
          location.hostname,
          '/ajax/csa.php?',
          'rfTs=' + ts.getTime(),
          '&rfTZ=' + ts.getTimezoneOffset(),
          '&rfRND=' + window.csaBeaconRand
        ];
      for(var k in map){
        url.push('&' + encodeURIComponent(k) + '=' +
            encodeURIComponent(map[k]));
      }
      var img = new Image(1, 1),
          l = url.join('');
      img.src = l;
    }

    window.google_csa_query = "";
    window.google_csa_client = '';
    window.google_csa_qry_lnk = "";
    //window.google_csa_qry_ctxt = "";
    window.google_csa_channel = "SoID_49,MkcID_6,pt_General";
    window.google_csa_hl = '1';
    window.google_csa_adtest = 'off';
    window.google_csa_send_beacon = false;
    window.google_csa_page_type = '';

    if(wf.tracking.gmedia.doAFS &&
      window.google_csa_send_beacon &&
      window.sendCsaBeacon){
      window.sendCsaBeacon({msg:50});
    }

    function google_csa_request_done(containerId, adsReturned){
      Y.Gmedia.googleCSARequestDone(containerId, adsReturned);
    }

    YUI_config.app.TopAdCount = 2;
    YUI_config.app.RightAdCount = 4;
    YUI_config.app.BottomAdCount = 3;

    wf.tracking.gmedia.TopAdCount = 2;
    wf.tracking.gmedia.RightAdCount = 4;
    wf.tracking.gmedia.BottomAdCount = 3;


    
    YUI_config.app.doAFC = '';
    wf.tracking.gmedia.doAFC = '';
    window.google_adtest = 'off';
    window.google_ad_channel = "SoID_49,MkcID_6,pt_General";
    window.google_ad_client = 'csnstores_js';
    wf.tracking.gmedia.AdPositionsFC = '';
    YUI_config.app.AdPositionsFC = '';

    function google_ad_request_done (google_ads) {
        if (Y && Y.Gmedia) {
          Y.Gmedia.googleAFCRequestDone(google_ads);
        } else if ($.tracking.gmedia) {
          $.tracking.gmedia.googleAFCRequestDone(gooleAds);
        }
    }

    YUI_config.app.TopAFCCount = 0;
    YUI_config.app.BottomAFCCount = 4;

    wf.tracking.gmedia.TopAFCCount = 0;
    wf.tracking.gmedia.BottomAFCCount = 4;
  </script>
