        <!-- 1st May 2015 - Added JS for Recaptcha for Sarah to incorporate in main.min.js -->
        <script type="text/javascript">
            var Tahzoo = Tahzoo || {};
            Tahzoo.Recaptcha = {};
            Tahzoo.Recaptcha.found = [];
            Tahzoo.Recaptcha.siteKey = "6Lco0wYTAAAAAPTUGjVoGVSR7zGQ6Pvk53T1nOQY";
            var createRecaptcha = function() {
                /*console.log("Recaptcha onload");*/
                if (Tahzoo.Recaptcha.found.length == 0) return false;
                /*console.log("Recaptcha siteKey: " + Tahzoo.Recaptcha.siteKey);*/
                for (var i = 0; i < Tahzoo.Recaptcha.found.length; i++) {
                    var rId = Tahzoo.Recaptcha.found[i].id;
                    if (rId != '' && grecaptcha) {
                        /*console.log("Recaptcha Processing: " + rId);*/
                        grecaptcha.render(rId, { 'sitekey': Tahzoo.Recaptcha.siteKey });
                    }
                }
            };
            jQuery(document).ready(function () {
                Tahzoo.Recaptcha.found = jQuery("div.recaptcha-wrapper");
                if (Tahzoo.Recaptcha.found.length > 0) {
                    /*console.log("Recaptcha Active, Found: " + Tahzoo.Recaptcha.found.length);*/
                    jQuery.getScript("https://www.google.com/recaptcha/api.js?onload=createRecaptcha&render=explicit");
                } else {
                    /*console.log("Recaptcha Not Active");*/
                }
            });
        </script>
        <!-- End of JS for Recaptcha -->
               
