<script type="text/javascript">
function IE6_ReadCookie(value)
{
	var curCookie = value + "="
	var curArray = document.cookie.split(';');
	
	for ( x = 0 ; x < curArray.length ; x++ )
	{
		var c = curArray[x];
		while (c.charAt(0)==' ') c = c.substring(1,c.length);
		if (c.indexOf(curCookie) == 0) return c.substring(curCookie.length,c.length);
	}
	return null;
}

function IE6_SetCookie(cookier, val, expireday)
{
	var expDate = new Date ();
	expDate.setTime(expDate.getTime() + (expireday * 24 * 60 * 60 * 1000));
 
  document.cookie = cookier + "=" + escape(val) + 
  ((expireday == null) ? "" : "; expires=" + expDate.toGMTString()) + "; domain=.hsn.com";
}

function IE6_IntExt_Check()
{
	document.write("<img width=\"0\" height=\"0\" style=\"display: none;\" src=\"" + IE6_Protocol + "://hsn.com/favicon.ico\" onload=\"IE6_Show_PhaseOut_Banner()\" onerror=\"IE6_SetCookie('IE6PhaseOutShown', '1', 1)\">");
}

function IE6_Show_PhaseOut_Banner()
{
	document.getElementById("IE6_Phaseout_BG").innerHTML = "<img src=\"" + IE6_Protocol + "://img.hsni.com/images/Site_Relaunch_2007/Other/Misc_Banners/091312_IE6_Phase_Out_Message_v3_Sm.gif\" width=\"453\" height=\"50\" border=\"0\" usemap=\"#HP_IE6_Map\">"

	
	Hsn.Popup.setDraggable(false)
	Hsn.Popup.Show("IE6_Phaseout_Banner")
	IE6_SetCookie('IE6PhaseOutShown', '1', 1)
}

function IE6_CheckBrowser()
{
	// Now includes ie7 as well
	if (!navigator.userAgent)
		return false
	
	var Tmp = navigator.userAgent.toString()
	
	if (Tmp.indexOf("MSIE 6") >= 0)
		return true
	else if (Tmp.indexOf("MSIE 7") >= 0)
	{
		if (Tmp.indexOf("Trident") >= 0)
			return false
		else
			return true
	}
	else
		return false
}

if (window.location.protocol.toString().indexOf("https") >= 0)
	var IE6_Protocol = "https"
else
	var IE6_Protocol = "http"

var IE6_Banner = document.getElementById("IE6_Phaseout_Banner")
if ((IE6_Banner) && (IE6_CheckBrowser()) && (!IE6_ReadCookie("IE6PhaseOutShown")))
	IE6_IntExt_Check()
</script>
