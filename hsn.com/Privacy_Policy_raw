<input class="hDate" type="hidden" value="">

<script type="text/javascript">
jQuery(document).ready(function($) {

  var noAjax = jQuery('input#header-noajax').length > 0 ? jQuery('input#header-noajax').val() : false;

  if (!noAjax)
  {
     HSN.Instance().Events.On(HSN.AccountNavigation.EVENT.ACCOUNT_STATUS_COMPLETE, function(data) {
        //var hDate = parseInt(/\d+/.exec(data.serverTime));
        HSN.TodaysSpecialCountdown.Register("#Header_Featured_Nav .HP_TS_Timer", new Date(data.serverTime));
        
        //put hidden server date field
        $('.hDate').val(data.serverTime);
        $("#Header_Featured_Nav #todayspecial .Text").show();
    });
  }
  else
  {
    $("#Header_Featured_Nav #todayspecial .Text").html("DON'T MISS OUT!")
    $("#Header_Featured_Nav #todayspecial .Text").show()
  }
});
</script>

