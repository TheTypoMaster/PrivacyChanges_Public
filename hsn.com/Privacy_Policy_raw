<script type="text/javascript">
// When on appropriate pages, disable rollover and highlight icon
(function($) {

  var mPathname = window.location.pathname.toLowerCase()
  var mHostname = window.location.hostname.toLowerCase()
  
  if (mPathname.indexOf("/watch") == 0)
  {
    $("#mUtilityNav .watch").each(function(){
      this.onmouseover()
      this.onmouseout = function(){}
    })
  }
  
  if (mPathname.indexOf("/arcade") == 0)
  {
    $("#mUtilityNav .arcade").each(function(){
      this.onmouseover()
      this.onmouseout = function(){}
    })
  }
  
  if (mHostname == "lithiumstage.hsn.com/" || mHostname == "community.hsn.com/")
  {
    $("#mUtilityNav .community").each(function(){
      this.onmouseover()
      this.onmouseout = function(){}
    })
  }
  
})(jQuery);
</script>

