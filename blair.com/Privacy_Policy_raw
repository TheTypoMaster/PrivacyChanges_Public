<script type="text/javascript">
   var searchInstructions = "What are you looking for today?";
   var searchErrorText = "Please enter a search term and try your search again.";
	</script>

<script type="text/javascript">
	function openBasketPage()
	{
		
			window.location = 'https://www.blair.com/checkout/basket.jsp';
		
	}
	
	function openCheckoutPage()
	{
		if ($('#ucart-item-count').val() == "0") {
			/* if zero items in cart we will send to basket pg instead of checkout */
			window.location = 'https://www.blair.com/checkout/basket.jsp';
		} else {
			
				window.location = 'https://checkout.blair.com/checkout/checkout.jsp?LAST_SITE_CODE=bla';
			
		}
	}
	
	function displayShipToCountryHeader() {
		
       	
        var currentPage = '/custserv/custserv.jsp?pageName=Privacy';		
		var params = "ts=" + new Date().getTime();
		params += "&previousPage=" + currentPage;
		
		$.ajax({
			type: "POST",
			url: '/international/ship_to_country.jsp',
			data: params,
			dataType: "html",
			success: function(data) {
				if(data != null && data != "") {
					$("#headerShipToCountry").html(data);
				}
			}
		});
	}
	
	function displayShipToCountryContextChooserPage(previousPage) {
		var params = "?previousPage=" + previousPage;
		
				
		window.location = '/international/context_chooser.jsp' + params;		
		
    }
	
	$(function(){
		$("#widget-but-ucart").click(function(){
			openBasketPage();
		});

		$(".search-keyword-container .search-button").click(function(){
			submitSearchForm(document.searchForm);
		});

		if ($(".keyword-text").height() < 16) {
			$(".keyword-text").height("22px");
		}
		
		/* Borderfree - Context Chooser - Start */
		
			displayShipToCountryHeader();
		
		/* Borderfree - Context Chooser - End */
	});
	
		$(document).ready(function() {
			if(document.searchForm.keyword.value == "") {
				document.searchForm.keyword.value = searchInstructions;
			}
		});
	</script>


</div>
		
