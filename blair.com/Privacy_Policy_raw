<script type="text/javascript">
  $(document).ready(function() {
    var isBrandPanel = false;
    $('#showBrandPanel').click(function(e) {
      e.preventDefault;
      if (isBrandPanel) {
        isBrandPanel = false;
        $('#brandMask').hide();
        $("#brand-panel").hide();
        $("#showBrandPanel a").addClass('closed');
      } else {
        isBrandPanel = true;
        var calloutPosLeft = $('#tab-navigate').offset().left - $('#universal-header').offset().left -11;
	$('#brand-panel #callout').css('left', calloutPosLeft +'px');
        $("#brandMask").show();
	$("#showBrandPanel a").removeClass('closed');
	$("#brand-panel").show();
      }
    });
    $('#brandMask').click(function(e) {
      $('#showBrandPanel').click();
    });
  });
</script></div>
