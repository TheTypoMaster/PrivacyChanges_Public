    <script type='text/javascript'>
            var abTestingCookie = getCookie('ab_testing');
            if (abTestingCookie) {
                    abTestingWidgetRequired = abTestingCookie;
            } else {
                    var widgets = ['taboola', 'outbrain'];
                    abTestingWidgetRequired = widgets[Math.round(Math.random())];
                    setCookie('ab_testing', abTestingWidgetRequired, 365, '/');
            }

            jQuery(function() {

                    if (abTestingWidgetRequired == 'taboola') {
                            if (isMobile) {
                                    //call taboola mobile function
                            } else {
                                    //call taboola desktop function
                            }
                    } else if (abTestingWidgetRequired == 'outbrain') {
                            if (isMobile) {
                                    //call outbrain mobile function
                                    ab_testing_outbrain_mobile();
                            } else {
                                    //call outbrain desktop function
                                    ab_testing_outbrain_desktop();
                            }
                    }

            });

            function ab_testing_outbrain_desktop() {
                    var url = ab_testing_get_url();
                    var html = "<div class=\"OUTBRAIN\" data-src=\"" + url + "\" data-widget-id=\"AR_1\" data-ob-template=\"FoodNetworkUK\" ></div><script type=\"text/javascript\" async=\"async\" src=\"http://widgets.outbrain.com/outbrain.js\"><\/script>";
                    jQuery('#outbrain_container').html(html);
            }

            function ab_testing_outbrain_mobile() {
                    var url = ab_testing_get_url();
                    var html = "<div class=\"OUTBRAIN\" data-src=\"" + url + "\" data-widget-id=\"MB_1\" data-ob-template=\"FoodNetworkUK\"></div><script type=\"text/javascript\" async=\"async\" src=\"http://widgets.outbrain.com/outbrain.js\"><\/script>";
                    jQuery('#outbrain_container').html(html);
                    jQuery('#mobile-ab-testing-container').parent().height('450px');
            }

            function ab_testing_get_url() {
                    return 'http://www.foodnetwork.co.uk' + window.location.pathname;
            }

    </script>
    
    <div id="outbrain_container"></div>
    
