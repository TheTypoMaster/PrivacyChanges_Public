      <script type="text/javascript">

  
  var FILTER_QUERY = "categoryID:59236800";

  
  var INCLUDE_VARIATIONS = "false";

  $(window).load(function(){
    $.fn.elementRealWidth = function () {
      $clone = this.clone()
          .css({"visibility": "hidden","display": "block"})
          .appendTo(this.parent());
      var $width = $clone.outerWidth();
      $clone.remove();
      return $width;
    };
    $("input[name=keywords]").autocomplete("/store/msusa/en_US/DisplaySolrAutoCompletePage/ThemeID.33363200", {
    parse: function(jsonDataWithRequestID) {
      var jsonStart = jsonDataWithRequestID.indexOf('{');
      var jsonDataString = jsonDataWithRequestID.substring(jsonStart);
      var jsonData = eval('(' + jsonDataString + ')');
      var rows = new Array();
      var facetValue = null;
      var index = 0;
      var jsonFacetValues = jsonData.facet_counts.facet_fields.autoComplete;
      $.each(jsonFacetValues, function(i, jsonFacetValue) {
        if (facetValue == null) {
          facetValue = jsonFacetValue;
        } else {
          rows[index++] = { data:[facetValue], value:facetValue, result:facetValue };
          facetValue = null;
        }
      });
      return rows;
    },
    extraParams: {
      filterQuery: FILTER_QUERY,
      includeVariations: INCLUDE_VARIATIONS
    },
    max: 10,
    width: $('#productsearchform').elementRealWidth() - 2,
    scrollHeight: 300
    });
  });

  </script>

