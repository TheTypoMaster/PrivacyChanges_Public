  <script type="text/javascript">
  $(document).ready(function() {
    var lpChatInterval = setInterval(function(){
      var liveChatOn = false;
      if($('#lpChatDiv a').length === 0){ // Chat is inactive so hide the anchor.
        $('#lpChatDiv a:visible').hide();
        $('.live_chat').empty();
      } else if ($('#lpChatDiv a').length ){ // Chat is active so show the anchor.
        var chatWithTechNode = $('#lpChatDiv').html();
        $('#lpChatDiv a').removeAttr('title');
        $('#lpChatDiv a').addClass('top-level-link');
        if($('.live_chat').length){
          $('.live_chat').empty();
          $('.live_chat').siblings('.phone').length?$('.live_chat').append('  | '+chatWithTechNode):$('.live_chat').append(chatWithTechNode);
        }
        liveChatOn = true;
      }
      /**if(liveChatOn){
        clearInterval(lpChatInterval);
      }**/
    }, 1000 );

    

  });

  

  // begin Live Chat variables
    
