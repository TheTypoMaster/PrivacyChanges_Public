    $(document).ready(function () {

        render_faq();
    });
    function render_faq() {
        for (var i = 0; i < masterLanguage.privacy.length; i++) {
            $('.infoarea').append('<a href="#'+masterLanguage.privacy[i].keyname+'"><div class="infolinks">'+masterLanguage.privacy[i].q+'</div></a>');
            $('.faq_text').append('<div class="row"><div class="rowlabel"><div class="words" id="'+masterLanguage.privacy[i].keyname+'">'+masterLanguage.privacy[i].q+'</div></div><div class="rowcontent staticpage">'+masterLanguage.privacy[i].a+'</div></div>');
        }
    }
</script>

