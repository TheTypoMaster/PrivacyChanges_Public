        var _optaCallbacks;

        if (_optaParams) {
            _optaCallbacks = _optaParams.callbacks;
        }

        if (_optaCallbacks) {
            if( _optaCallbacks.indexOf(tmgOptaCallback) < 0) {
            _optaCallbacks.push(tmgOptaCallback);
            }
        } else {
            _optaCallbacks = [tmgOptaCallback];
