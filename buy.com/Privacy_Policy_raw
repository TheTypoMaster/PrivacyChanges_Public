<script type="text/javascript">
	(function () {
		
		var referrer = document.referrer;
		var date = new Date();
		if (qs["siteid"]) {
			
			date.setTime(date.getTime() - 1);
			document.cookie = "AdTrack=; path=/; domain=.rakuten.com; expires=" + new Date(0).toUTCString();

			
			
			document.cookie = "Affiliate=noad; domain=.rakuten.com;";

			
			document.cookie = "SessionSource=adid=" + encodeURIComponent(qs['adid']) + "&referer=" + encodeURIComponent(referrer) + "&pvd=0&scid=" + (qs['scid'] || qs['sclid'] || "") + "; path=/; domain=.rakuten.com";
			document.cookie = "ClickFromIsPaid=1; path=/; domain=.rakuten.com";

			var url = 'http://www.rakuten.com/clickfrom/affiliate.ashx?referrer=' + encodeURIComponent(referrer) + '&' + window.location.search.substring(1);
			var img = document.createElement('img');
			img.src = url;
			document.body.appendChild(img);
		}
		else if (qs['adid']) {
			var guid = date.getHours().toString() + date.getMinutes().toString() + date.getSeconds().toString() + ("000000000" + Math.floor(Math.random() * 1000000000)).slice(-9);
			
			var adTrackCookie = "AdTrack=Affiliate= &AffLink=TRUE&NcBty=&Ad=" + encodeURIComponent(qs['adid']) + "&Offer=" + encodeURIComponent(qs['offer'] || "") + "&Guid=" + guid + "&Referer=" + encodeURIComponent(referrer) + "; path=/; domain=.rakuten.com;";
			if (qs['adid'] == "18094") {
				date.setTime(date.getTime() + (14 * 86400000));
				adTrackCookie += " expires=" + date.toUTCString();
			}
			else {
				date.setTime(date.getTime() + (30 * 86400000));
				adTrackCookie += " expires=" + date.toUTCString();
			}
			document.cookie = adTrackCookie;

			var sessionSourceCookie = "SessionSource=adid=" + encodeURIComponent(qs['adid']) + "&referer=" + encodeURIComponent(referrer) + "&pvd=0&scid=" + (qs['scid'] || qs['sclid'] || "") + "; path=/; domain=.rakuten.com";
			document.cookie = sessionSourceCookie;

			document.cookie = "ClickFromIsPaid=1; path=/; domain=.rakuten.com";

			document.cookie = "ClickFrom=; path=/; domain=.rakuten.com; expires=" + new Date(0).toUTCString();

			var url = 'http://www.rakuten.com/clickfrom/clickfromlog.ashx?referrer=' + encodeURIComponent(referrer) + '&cfguid=' + encodeURIComponent(guid) + '&' + window.location.search.substring(1);
			var img = document.createElement('img');
			img.src = url;
			document.body.appendChild(img);
		}
	})();
</script>
