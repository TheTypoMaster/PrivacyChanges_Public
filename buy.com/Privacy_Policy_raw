<script type="text/javascript">
//<![CDATA[
    if ((/[?&]ic=1/i).test(window.location.search))
        document.cookie = "classicsticky=1; path=/; domain=rakuten.com;";

	if (!(/classicsticky=1/).test(document.cookie)) {
        var uaCheck = navigator.userAgent.toLowerCase();
        var pgCheck = window.location.pathname.toLowerCase();
		
		var qs = {};
		var qps = window.location.search.substring(1).split('&');
		for (var i = 0, j = qps.length; i < j; i++) {
			var p = qps[i].split('=');
			var v = p[1];
			qs[decodeURIComponent(p[0]).toLowerCase()] = v ? decodeURIComponent(v) : null;
		}
		var trackqs = '';
        if (qs['adid'])
            trackqs += 'adid=' + encodeURIComponent(qs['adid']);
        if (qs['omadtrack'])
            trackqs += (trackqs ? '&omadtrack=' : 'omadtrack=') + encodeURIComponent(qs['omadtrack']);
        if (qs['scid'])
        	trackqs += (trackqs ? '&scid=' : 'scid=') + encodeURIComponent(qs['scid']);
        if (qs['sclid'])
        	trackqs += (trackqs ? '&sclid=' : 'sclid=') + encodeURIComponent(qs['sclid']);

    	if ((uaCheck.indexOf("iphone") > -1) || (uaCheck.indexOf("ipod") > -1) || (uaCheck.indexOf("android") > -1 && uaCheck.indexOf("mobile") > -1) || (uaCheck.indexOf("blackberry") > -1) || (uaCheck.indexOf("midp") > -1) || (uaCheck.indexOf("cldc") > -1) || (uaCheck.indexOf("windows ce") > -1) || (uaCheck.indexOf("windows phone") > -1)) {
    		if (pgCheck.indexOf("usersearchresults") > -1 || pgCheck.indexOf("/umerch/") > -1 || pgCheck.indexOf("/sr/") > -1 && /qu=[^&]+/.test(location.search)) {
                window.location = 'http://m.rakuten.com/search/results/?term=' + encodeURIComponent(qs['qu']) + (qs['adid'] ? '&adid=' + encodeURIComponent(qs['adid']) : '') + (qs['omadtrack'] ? '&omadtrack=' + encodeURIComponent(qs['omadtrack']) : '');
            }
            else if (pgCheck.indexOf("/prod/") > -1 || pgCheck.indexOf("/pr/") > -1 || pgCheck.indexOf("product.asp") > -1) {
            	var tmpqs = qs['listingid'] ? '?listingid=' + encodeURIComponent(qs['listingid']) : qs['sellerid'] ? '?sellerid=' + encodeURIComponent(qs['sellerid']) : '';
            	
				if (trackqs)
					tmpqs += (tmpqs ? '&' + trackqs : '?' + trackqs);
				window.location = 'http://m.rakuten.com/product/' + tmpqs;
            }
            else if (pgCheck.indexOf("/todaysdeals/") > -1 || pgCheck.indexOf('todaysdeals.aspx') > -1)
            	window.location = 'http://m.rakuten.com/deals/today';
            else if (pgCheck.indexOf("62329.html") > -1 || /loc=62329/.test(location.search))
            	window.location = 'http://m.rakuten.com/deals/weekly';
            else if (pgCheck.indexOf("/shakin/") > -1)
            	window.location = 'http://m.rakuten.com/deals/trending';
            else if (pgCheck == "/" || pgCheck == "/default.asp" || pgCheck == "/ct/default.aspx" || pgCheck == "/ct/default2.aspx") {
				window.location = 'http://m.rakuten.com' + (trackqs ? '?' + trackqs : '');
            }
        }
    }
//]]>
</script>

