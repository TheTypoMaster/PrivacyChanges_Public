// <![CDATA[
if (typeof BUY == "undefined") var BUY = {};
if (typeof BUY.Tracking == "undefined") BUY.Tracking = {};
BUY.Tracking.Load = function(url) {
	var isHTTPS = window.parent.location.protocol === "https:"; 
	var $body = $(document.body);

	//Clickstream - set shopper cookie
	if (GetCookie('ShopperManager%2F') == 'null') {
		var smdate = new Date();
		smdate.setDate(smdate.getDate() + 365);
		document.cookie = "ShopperManager%2F=ShopperManager%2F=" + Math.uuid(32) + ";expires=" + smdate.toUTCString() + "; domain=rakuten.com; path=/";
	}	

	var fSid;
	var sid;
	var pid;
    
	if (GetCookie('ShopperManager%2F') != 'null') {
			fSid = GetCookie('ShopperManager%2F');
			sid = fSid.substr(16);
			pid = BUY.Tracking.GetReferrerWebAddress();
	}	
	else {
		sid = "";
		pid = "";
	} 

	
	var gsaCookie = GetCookie("GSASearch");
	var gsaParam = "";
	if (gsaCookie)
		gsaParam = "&gsap=1";
	document.cookie = "GSASearch=; path=/; expires=Thu, 01-Jan-70 00:00:01 GMT;";
	
	if (BUY.Search && typeof BUY.Search.ResultCount == "number")
	{
		gsaParam += "&resultCount=" + BUY.Search.ResultCount;
	}
	
	if (!url) url = "tocfeature.aspx?loc=79085";
	if(url.indexOf("?") == -1) url += "?";
	url += gsaParam;

	if (isHTTPS) {
		$body.append("<img src=\"https://traffic.buyservices.com/traffic.asmx/InsertEvent4?EventType=CL&EID=10000&ShopperId=" + sid + "&IP=19&referrer=" + pid + "&Url=" + encodeURIComponent(url) + "\" width=\"1\" height=\"1\" border=\"0\" />");
	} else {
		$body.append("<img src=\"http://traffic.buyservices.com/traffic.asmx/InsertEvent4?EventType=CL&EID=10000&ShopperId=" + sid + "&IP=19&referrer=" + pid + "&Url=" + encodeURIComponent(url) + "\" width=\"1\" height=\"1\" border=\"0\" />");
		//$body.append("<iframe src=\"http://www.rakuten.com/corp/basket/wsCookie.asp\" width=\"1\" height=\"1\" marginheight=\"0\" marginwidth=\"0\" frameborder=\"0\" scrolling=\"no\"><\/iframe>");
	}
};
BUY.Tracking.GetReferrerWebAddress = function() {
	var referrer = document.referrer;
	var re = new RegExp("http://|https://", "gi");
	referrer = referrer.replace(re, "");
	referrerParts = referrer.split("/");
	return referrerParts[0];															
};

$(function(){
	if(!BUY.Tracking.DeferLoad)
		BUY.Tracking.Load();
});
