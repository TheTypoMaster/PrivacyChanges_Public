<script type="text/javascript" language="javascript">
    //placeholder logic:
    (function () {
        var $qu = $(document.forms["searchbox"].qu);
        var $placeHolder = $("#placeHolder");
        function $clearquwm() { $qu.css("background-color", "#ffffff"); }
        function $showquwm() { if ($qu.val() === "") $qu.css("background-color", "transparent"); }
        function $plchldc() { $clearquwm(); $qu.focus(); }
        $qu.focus($clearquwm).bind("dragenter", $clearquwm).bind("drop", $clearquwm);
        $placeHolder.click($plchldc);
        $qu.blur($showquwm).bind("dragleave", $showquwm).bind("dragend", function () { window.setTimeout($showquwm, 1); })
        if ($qu.val() !== "") $clearquwm();
        $(function () { if ($qu.val() !== "" && $qu.css("background-color") !== "#ffffff") $clearquwm(); });
    })();
    $('form.search-form').submit(function (e) {
        s.tl(this, 'o', 'header_searchbox|' + this.queryType.value + '|' + this.qu.value.replace(/[^a-zA-Z0-9 ]+/g, ' ') + '|' + window.location.href);
        return true;
    })
</script>
