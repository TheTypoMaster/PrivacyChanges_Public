                list += '<li><div class="size"><a href="javascript:void(0);" onclick="javascript:toggleHeaderSignUp(); s.tl(this,\'o\',\'' + header_slider_items[i].onclick + '\');"><img src="' + header_slider_items[i].src + '" border="0"></a></div></li>';
