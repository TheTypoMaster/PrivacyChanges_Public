(function() {
if(window.inlineURLs) {
inlineURLs.forEach(function(args, index) {
if(args.length) {
var use = args[2],
event = args[1],
URL = args[0].replace(/&#47;/g, '/'),
where,
doc;
var invokeComplete = function(evt) {
KOR.fireLoad(evt);
}
var injectScript = function(where, url, evt, hasFIF) {
var js = ((hasFIF) ? where : document).createElement('script');
js.src = url;
if(evt) {
if(window.attachEvent) {
js.onreadystatechange = function() {
if(this.readyState === 'loaded' || this.readyState === 'complete') {
js.onreadystatechange = null;
setTimeout(function() {
invokeComplete(evt);
}, 100);
}
}
} else {
js.onload = function() {
invokeComplete(evt);
}
}
}
if(hasFIF) {
where.body.appendChild(js);
} else {
where.parentNode.insertBefore(js, where);
}
}
where = document.getElementsByTagName('script')[0];
switch(true) {
case(use.toLowerCase() == 'fif'):
var frame = document.createElement('iframe');
