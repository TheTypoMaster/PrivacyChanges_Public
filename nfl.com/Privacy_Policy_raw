	<style>
	/** Inline Style Block **/
	#responsive-page{}
	#subs-list-items-sova > li{list-style:initial!important;list-style-position:inside!important;margin:3px; font-size:16px;line-height:24px;font-weight:normal;font-style:normal; color:#000}
	 #subs-list-items > li{list-style:initial!important;list-style-position:inside!important;margin:20px; font-size:16px;line-height:24px;font-weight:normal;font-style:normal; color:#000}
    #subs-list-items li ul li{list-style:circle!important;list-style-position: inside!important;margin:20px; font-size:16px;line-height:24px;font-weight:normal;font-style:normal; color:#000}
		ol.numbered_list li{list-style-type: decimal; margin-left: 20px; font-size:16px;line-height:24px;font-weight:normal;font-style:normal; color:#000}

	.font-14px-default{ color:#000 !important;}
		
