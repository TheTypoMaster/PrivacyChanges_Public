	// Set welcome message in header for desktop
	var loginStatus = (function () {
	  return {
	    loginCheck: function () {
		    if (typeof adageUser != "undefined" && adageUser.userID != null) {
			    $("#adage-register").text(adageUser.user.Profile.firstName);
			    $("#membership-level").text(adageUser.user.Membership.tier_name);
