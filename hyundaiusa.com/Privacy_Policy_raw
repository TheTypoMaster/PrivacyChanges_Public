<script type="text/javascript">
	var s_account = (function () {
		var language = 'en-US';
		var production = /www\.hyundaiusa\.com/.test(window.location.host);

		if (language == 'ko-KR') {
			return production ? 
				"hmahyundaiusa-korean" : 
				"hmadevhyundaiusa-korean";
		} else if (language == 'es-US') {
			return production ? 
				"hmahyundaiusa-espanol" : 
				"hmadevhyundaiusa-espanol";
		} else {
			return production ? 
				"hmahyundaiUSA" : 
				"hmadevhyundaiUSA";
		}
	})();
</script>
