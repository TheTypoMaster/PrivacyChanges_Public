<script type="text/javascript">
    $jq(function () {
        var url = location.href.toLowerCase();
        $jq('.header-link a').each(function () {
            $jq(this).removeClass('highlighted');
            if (this.href.toLowerCase() == url) {
                $jq(this).addClass('highlighted');
            }
        });
    });
</script>

