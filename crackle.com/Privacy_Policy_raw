<script type="text/javascript">
    function OmnTrackCustomLink(name)
    {
        s.linkTrackVars = 'eVar17,eVar18,eVar19';
        s.eVar17 = s.getTimeParting('h', '-7'); // Set hour 
        s.eVar18 = s.getTimeParting('d', '-7'); // Set day
        s.eVar19 = s.getTimeParting('w', '-7'); // Set Weekend / Weekday
        s.tl(this,'o',name);
    }	
	
    function OmnTrackExitLink(name)
    {
        s.linkTrackVars = 'eVar17,eVar18,eVar19';
        s.eVar17 = s.getTimeParting('h', '-7'); // Set hour 
        s.eVar18 = s.getTimeParting('d', '-7'); // Set day
        s.eVar19 = s.getTimeParting('w', '-7'); // Set Weekend / Weekday
        s.tl(this,'e',name);
    }	
	
    var campaignId = 0;
    var cmpre = /[&?#]cmpid=([^&?#=]*)/;
    var res = cmpre.exec(window.location);
    if (res && res[1]) { campaignId = res[1]; }
	
    var refDomain = "";
    if (! document.referrer.match(/crackle\.com/))
    {
        var domMa = /(?:http:\/\/)?(?:https:\/\/)?([^/]*)/i.exec(document.referrer)
        if (domMa && domMa[1])
            refDomain = domMa[1];
    }
