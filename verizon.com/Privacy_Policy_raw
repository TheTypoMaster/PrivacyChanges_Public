<script>
var strThisHref=(window.location.href).toLowerCase().substring(0,((window.location.href).toLowerCase().search(/goto=/)>-1)?(window.location.href).toLowerCase().search(/goto=/):999);
var scbSearchCharCount=0;
var scbSearchSuggChoice=-1;
var scbSearchSuggThreshold=3;
var scbSuggHover=false;
var scbSearchHide=false;
var scbSearchKeyTimer;
var scbSearchKeyDelay=300;//ms delay
scbSearchParams='&src=Corp';
scbSearchRefineForm='&ES=Corporate';
scbSearchRefineSuggest='&segments=Corporate&ES=Corporate';

function removeHttps(strUrl){
	var tmpStrUrl = strUrl.replace("https://","");
	tmpStrUrl = tmpStrUrl.replace("http://","");
	return tmpStrUrl;
}

function scbTrim(str){return str.replace(/^\s+|\s+$/g,'');}

function dispLayer(obj,val){
	document.getElementById(obj).style.display=val;
}
function getEvent(e){e=e||window.event;return e;}
function getEventTarget(e){e=e||window.event;return e.target||e.srcElement;}

var is_ie=((iePos!=-1)&&(!is_opera)&&(!is_khtml));
var is_konq=false;
var agt=navigator.userAgent.toLowerCase();
var appVer=navigator.appVersion.toLowerCase();
var iePos=appVer.indexOf('msie');
var is_opera=(agt.indexOf('opera')!=-1);
var is_safari=agt.indexOf('safari')!=-1;
var kqPos=agt.indexOf('konqueror');
if(kqPos!=-1){is_konq=true;}
var is_khtml=(is_safari||is_konq);

function scbSearchShow(){
	/* check to hide Search box in header -- default is SHOW */
	var boolSearchTop=true;
	arrHideSearch=new Array(
		'verizon.com/search/',
		'/support/residential',
		removeHttps('http://entertainment.verizon.com'),
		removeHttps('https://www.verizon.com')+'/ondemand/',
		removeHttps('https://www.verizon.com')+'/fiostv/',
		removeHttps('https://www.verizon.com')+'/mylibrary/',
		removeHttps('https://www.verizon.com')+'/myflexview/'
	);
	for(i=0;i<arrHideSearch.length;i++) {
		if(scbreshref.indexOf(arrHideSearch[i])>-1){
			boolSearchTop=false;
			break;
		}
	}
	if(boolSearchTop){dispLayer('scbsearchtopdiv','block');}
	dispLayer('scbsupportsearch','block');
}
function scbResSearch(frm){
	var strTerm=scbTrim(frm.q.value.toLowerCase());
	if( (strTerm==''||strTerm=='search'||strTerm=='search site'||strTerm=='search site & support'
		||strTerm=='search support'||strTerm=='how can we help you?') && scbSearchSuggChoice==-1){
		alert('Please type a question or keyword.');
		frm.q.focus();
		return false;
	}
	frm.action='http://www.verizon.com/search/SearchResults';
	if(frm.id=='scbsearchsupportform'){
		scbSearchRefineForm='&Endeca_user_segments=ResidentialSupport';
	}
	window.location.href=frm.action+'?Nty=1&Ntt='+encodeURIComponent(frm.q.value)+scbSearchParams+scbSearchRefineForm;
}
function scbSearchInit(){
	var scbSearchInput=document.getElementById('scbsearchtopinput');
	var scbSearchSuggestDiv=document.getElementById('scbsearchsuggdiv');
	if(scbSearchInput!=null && scbSearchSuggestDiv!=null){
		scbSearchSuggestDiv.onmouseover=function(e){e=getEvent(e);var idsugg=e.toElement||e.relatedTarget;if((scbSearchSuggestDiv.contains(idsugg))||!is_ie){scbSuggHover=true;}}
		scbSearchSuggestDiv.onmouseout=function(e){e=getEvent(e);var idsugg=e.relatedTarget||e.toElement;if((!scbSearchSuggestDiv.contains(idsugg))||!is_ie){scbSuggHover=false;}}
		scbSearchInput.onblur=function(){window.clearTimeout(scbSearchKeyTimer);if(this.value=='')this.value='Search';scbSearchSuggHide();}
		scbSearchInput.onfocus=function(){if(this.value=='Search')this.value='';}
		scbSearchInput.onkeyup=function(e){
			if(scbSearchInput.value.length<scbSearchSuggThreshold){
				scbSearchHide=true;
				scbSearchSuggReset();
				return false;
			}
			window.clearTimeout(scbSearchKeyTimer);
			e=e||event;
			var kCode=e.keyCode;
			if(kCode==37 || kCode==39){//handle left/right arrow
				return false;
			}
			if(kCode==27){//handle ESC hide suggestions
				scbSearchInput.value='';
				scbSearchSuggReset();
				return false;
			}
			if(kCode==13 || kCode==38 || kCode==40){//handle ENTER, up/dn arrow
				if(scbSearchSuggestDiv!=null){
					var arrSuggLinks=scbSearchSuggestDiv.getElementsByTagName('A');
					if(arrSuggLinks.length>0){
						if(kCode==13){//handle ENTER
							if(scbSearchSuggChoice==-1){//sugg choice NOT selected - submit FORM
								return true;
							}else{//go to sugg choice link
								window.location.href=arrSuggLinks[scbSearchSuggChoice].href;
								return false;
							}
						}
						scbSearchSuggShow();
						if(kCode==40){scbSearchSuggChoice++;}//dn
						else if(kCode==38){scbSearchSuggChoice--;}//up
						if(scbSearchSuggChoice+1 > arrSuggLinks.length){scbSearchSuggChoice=0;}//select first
						else if(scbSearchSuggChoice < 0){scbSearchSuggChoice=arrSuggLinks.length-1;}//select last
						for(var i=0;i<arrSuggLinks.length;i++){arrSuggLinks[i].className=(arrSuggLinks[i].className).replace(/ selected/g,'');}//reset all sugg link bg
						arrSuggLinks[scbSearchSuggChoice].className+=' selected';//highlight sugg link choice
					}
				}
				return false;
			}
			scbSearchKeyTimer=window.setTimeout(function(){scbSearchProcess();},scbSearchKeyDelay);
		}
	}
}
function scbSearchSuggShow(){

	dispLayer('scbsearchsuggdiv','block');
}
function scbSearchSuggHide(){
	if(	!scbSuggHover || scbSearchHide){
		dispLayer('scbsearchsuggdiv','none');
		scbSearchSuggChoice=-1;
		scbSearchHide=false;
	}
}
function scbSearchSuggReset(){
	scbSearchSuggHide();
	var scbSearchSuggestDiv=document.getElementById('scbsearchsuggdiv');
	if(scbSearchSuggestDiv!=null){scbSearchSuggestDiv.innerHTML='';}
}
function scbSearchProcess(){
	var scbSearchInput=document.getElementById('scbsearchtopinput');
	var scbSearchSuggestDiv=document.getElementById('scbsearchsuggdiv');
	if(scbSearchInput!=null && scbSearchSuggestDiv!=null){
		var sTerm=scbSearchInput.value;
		sTerm=sTerm.replace(/[\(\)\[\]\{\}\%\\\|]+/g,'');//special re chars
		sTerm=sTerm.replace(/[^a-zA-Z0-9 _-]+/g,'');//alphanumeric
		sTerm=scbTrim(sTerm);
		scbSearchCharCount=scbTrim(sTerm).length;
		if(scbSearchCharCount>=scbSearchSuggThreshold && sTerm!='Search' ){//make ajax call
			var myRequest=new scbAjaxObject('//www.verizon.com/search/headersearch/',scbSearchProcessData,sTerm);
			myRequest.update('Dy=1&contentPaths=%2Fcontent%2FShared%2FAuto-Suggest%20Panels&templateTypes=AutoSuggestPanel&Ntt='+sTerm+'*'+scbSearchRefineSuggest);//Server is contacted here.
		}else{
			scbSearchSuggReset();
		}
	}
}

function scbAjaxObject(sUrl,callbackFunction,sTerm){
	var that=this;
	this.updating=false;
	this.abort=function(){
		if(that.updating){
			that.updating=false;
			that.AJAX.abort();
			that.AJAX=null;
		}
	}
	this.update=function(passData,postMethod){
		if(that.updating){return false;}
		that.AJAX=null;
		if(window.XMLHttpRequest){
			that.AJAX=new XMLHttpRequest();
		}else{
			that.AJAX=new ActiveXObject("Microsoft.XMLHTTP");
		}
		if(that.AJAX==null){
			return false;
		}else{
			that.AJAX.onreadystatechange=function(){
				if(that.AJAX.readyState==4){
					that.updating=false;
					that.callback(that.AJAX.responseText,that.AJAX.status,that.AJAX.responseXML,sTerm);
					that.AJAX=null;
				}
			}
			that.updating=new Date();
			if(/post/i.test(postMethod)){
				var uri=urlCall+'?'+that.updating.getTime();
				that.AJAX.open("POST",uri,true);
				that.AJAX.setRequestHeader("Content-type","application/x-www-form-urlencoded");
				that.AJAX.setRequestHeader("Content-Length",passData.length);
				that.AJAX.send(passData);
			}else{
				//var uri=urlCall+'?'+passData+'&timestamp='+(that.updating.getTime());
				var uri=urlCall+'?'+passData;
				that.AJAX.open("GET",uri,true);
				that.AJAX.send(null);
			}
			return true;
		}
	}
	var urlCall=sUrl;
	this.callback=callbackFunction || function(){};
}
function scbSearchProcessData(responseText,responseStatus,responseXML,sTerm){
	if(responseStatus==200){
	
		var scbSearchSuggestDiv=document.getElementById('scbsearchsuggdiv');
		if(scbSearchSuggestDiv!=null){
			sTerm=sTerm.replace(/[\(\)\[\]\{\}\%\\\|]+/g,'');//special re chars
			sTerm=sTerm.replace(/\s{2,}/g,' ');//mult spaces to single
			responseText=scbTrim(responseText);
			responseText=responseText.replace(/VERIZON_KEYWORD/,'VERIZON KEYWORDS');
			responseText=responseText.replace(/\s{2,}/g,' ');//mult spaces to single
			scbSearchSuggestDiv.innerHTML=responseText;
			var scbSuggLinks=scbSearchSuggestDiv.getElementsByTagName('A');
			
			if(scbSuggLinks.length > 0){
				for(var i=0;i<scbSuggLinks.length;i++){
					var scbLinkText=scbSuggLinks[i].innerHTML;
					var re=new RegExp(sTerm,'gi');
					var strTemp=scbSuggLinks[i].innerHTML;
					scbSuggLinks[i].innerHTML=strTemp;
					if(scbSuggLinks[i].href.toLowerCase().indexOf('/search/searchresults')>-1){scbSuggLinks[i].href+=scbSearchParams;}
				}
			}
			scbSearchSuggChoice=-1;
			if(responseText.length>300){scbSearchSuggShow();}
			else{scbSearchSuggHide();}
		}
	}else{
		if(window.console){console.log(responseStatus+' -- Error Processing Request');}
	}
}
</script>
