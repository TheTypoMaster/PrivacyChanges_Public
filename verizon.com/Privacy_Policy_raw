<script type="text/javascript">
function getXMLObjectForStockQuote()  //XML OBJECT
{
   var xmlHttp = false;
   try {
     xmlHttp = new ActiveXObject("Msxml2.XMLHTTP")  
   }
   catch (e) {
     try {
       xmlHttp = new ActiveXObject("Microsoft.XMLHTTP")  
     }
     catch (e2) {
       xmlHttp = false   
     }
   }
   if (!xmlHttp && typeof XMLHttpRequest != 'undefined') {
     xmlHttp = new XMLHttpRequest();        
   }
   return xmlHttp;  
}
var xmlhttp = new getXMLObjectForStockQuote();    
        xmlhttp.open("GET","/about/stockquoteServlet",true);   		
    	xmlhttp.onreadystatechange  = handleServerResponseForStockQuote;
    	xmlhttp.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
    	xmlhttp.send(null);


function ajaxFunctionForStockQuote()
{
    
	if(xmlhttp)
	{     
    	xmlhttp.open("GET","/about/stockquoteServlet",true); 
    	xmlhttp.onreadystatechange  = handleServerResponseForStockQuote;
    	xmlhttp.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
    	xmlhttp.send(null);
    }
}
function handleServerResponseForStockQuote()
 {
   //alert('in response '+xmlhttp.readyState);
   if (xmlhttp.readyState == 4) {
    // alert('status '+xmlhttp.status);
     if(xmlhttp.status == 200) {
       var value=xmlhttp.responseText; //Update the HTML Form element 
       //alert('ajax response :'+value);
       
       var value_split = value.split(';');
       
       var Last_array = value_split[0];
       var Last_val_array = Last_array.split(':');
       var Last_val = Last_val_array[1];
       
       var Change_array = value_split[1];
       var Change_val_array = Change_array.split(':');
       var Change_val = Change_val_array[1];
       
       //Last_val="41.08";
      //Change_val="+0.76";
       if(Last_val==null || Last_val=='undefined'){
       		Last_val="";
       }
       if(Change_val==null || Change_val=='undefined' || Change_val=='12px')
       {
       		Change_val="";
       }
        document.getElementById("last").innerHTML = Last_val;
        document.getElementById("change").innerHTML = Change_val;     
       if(Change_val < 0)
       {
       	document.getElementById("stockimage").src="/onecms/privacypolicy/images/stock/arrow_dn.gif";
       	document.getElementById("change").style.color="#FF0000";
       }else{
       	document.getElementById("stockimage").src="/onecms/privacypolicy/images/stock/arrow_up.gif";
       	document.getElementById("change").style.color="#89c33f";
       }
      
    }
    }
    }

ajaxFunctionForStockQuote();
</script>



        
