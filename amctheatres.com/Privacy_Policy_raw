    <script type="text/javascript">
        amc.require.onReady('low', function () {
            amc.views.helpers.renderForDesktop(function () {
                amc.events.primaryTheatreMenuHover.one($(document), function (evt, data) {
                    var el = $('#theatre-list-menu-1693');
                    el.html($('#theatre-list-menu-template').html());
                    ko.applyBindings(new amc.views.TheatreListMenuViewModel(), el.get(0));
                });
            });
        });
    </script>
