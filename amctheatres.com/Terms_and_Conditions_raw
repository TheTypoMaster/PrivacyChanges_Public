                <p>
                    <span data-bind="text: AddressLine1"></span><br />
                    <span data-bind="visible: AddressLine2(), html: AddressLine2() + '<br />'"></span>
                    <span data-bind="text: City() + ', ' + State() + ' ' + FormattedPostalCode()"></span><br />

                    <!-- ko if: $parent.currentTheatreId() && $parent.currentTheatreId() == TheatreId() -->
                    <span data-bind="visible: $parent.showMakeCurrentTheatre"><strong>Your current theatre</strong></span>
                    <!-- /ko -->

                    <!-- ko if: !$parent.currentTheatreId() || $parent.currentTheatreId() != TheatreId() -->
                    <a href="#" data-bind="visible: $parent.showMakeCurrentTheatre, click: $parent.setTheatreAsCurrent">Make current theatre</a>
                    <!-- /ko -->
                </p>
            </div>
            <!-- /end .body -->
        </li>
        <li class="clearfix" data-bind="visible: ($index() + 1) % 3 == 0"></li>
    </script>
