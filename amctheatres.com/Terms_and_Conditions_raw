    <script type="text/javascript">
        amc = amc || {};
        amc.currentContext = amc.currentContext || {};
        amc.require.onReady(function () {
            if (amc.currentContext['mobile-account-navigation-bound'])
                return;

            amc.currentContext['mobile-account-navigation-bound'] = true;
            ko.applyBindings(new amc.views.MobileAccountNavigationViewModel(), $('.mobile-navbar').get(0));
        });
    </script>

    <script type="text/javascript">
        amc.require.onReady('low', function () {
            amc.views.helpers.renderForDesktop(function () {
                amc.events.primaryMovieMenuHover.one($(document), function (evt, data) {
                    $('#mainnav-movies-multi-5259').html($('#movie-menu-template').html());
                 
                    amc.events.myMoviesLoaded.bind($(document), function (evt, data) {
                        amc.myMovies.bindKo('#mainnav-movies-multi-5259 [data-bind-my-movie]');
                        amc.myMovies.myMoviesPopovers('#mainnav-movies-multi-5259 .my-movies-icon');
                    });                  
                });
            });
        });
    </script>
