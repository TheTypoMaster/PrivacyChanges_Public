<script type="text/javascript">
	var bvapiUrl = "http://worldmarket.ugc.bazaarvoice.com/static/3428-en_us/bvapi.js";
	window.loadBazaarvoiceApi = function(callback) {
		if (window.$BV) {
			callback();
		} else {
			jQuery.ajax({
				url: bvapiUrl,
				cache: true,
				dataType: "script",
				success: function() {
					jQuery($BV.docReady);
					callback();
			}
			});
		}
	};

loadBazaarvoiceApi(function() {
$BV.configure("global", {
	submissionContainerUrl: "http://worldmarket.com/ReviewSubmit.do"
	});
});
</script>
