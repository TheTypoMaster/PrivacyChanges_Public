(function(d){function i(){var b=d("script:first"),a=b.css("color"),c=false;if(/^rgba/.test(a))c=true;else try{c=a!=b.css("color","rgba(0, 0, 0, 0.5)").css("color");b.css("color",a)}catch(e){}return c}function g(b,a,c){var e="rgb"+(d.support.rgba?"a":"")+"("+parseInt(b[0]+c*(a[0]-b[0]),10)+","+parseInt(b[1]+c*(a[1]-b[1]),10)+","+parseInt(b[2]+c*(a[2]-b[2]),10);if(d.support.rgba)e+=","+(b&&a?parseFloat(b[3]+c*(a[3]-b[3])):1);e+=")";return e}function f(b){var a,c;if(a=/#([0-9a-fA-F]{2})([0-9a-fA-F]{2})([0-9a-fA-F]{2})/.exec(b))c=
[parseInt(a[1],16),parseInt(a[2],16),parseInt(a[3],16),1];else if(a=/#([0-9a-fA-F])([0-9a-fA-F])([0-9a-fA-F])/.exec(b))c=[parseInt(a[1],16)*17,parseInt(a[2],16)*17,parseInt(a[3],16)*17,1];else if(a=/rgb\(\s*([0-9]{1,3})\s*,\s*([0-9]{1,3})\s*,\s*([0-9]{1,3})\s*\)/.exec(b))c=[parseInt(a[1]),parseInt(a[2]),parseInt(a[3]),1];else if(a=/rgba\(\s*([0-9]{1,3})\s*,\s*([0-9]{1,3})\s*,\s*([0-9]{1,3})\s*,\s*([0-9\.]*)\s*\)/.exec(b))c=[parseInt(a[1],10),parseInt(a[2],10),parseInt(a[3],10),parseFloat(a[4])];return c}
d.extend(true,d,{support:{rgba:i()}});var h=["color","backgroundColor","borderBottomColor","borderLeftColor","borderRightColor","borderTopColor","outlineColor"];d.each(h,function(b,a){d.fx.step[a]=function(c){if(!c.init){c.a=f(d(c.elem).css(a));c.end=f(c.end);c.init=true}c.elem.style[a]=g(c.a,c.end,c.pos)}});d.fx.step.borderColor=function(b){if(!b.init)b.end=f(b.end);var a=h.slice(2,6);d.each(a,function(c,e){b.init||(b[e]={a:f(d(b.elem).css(e))});b.elem.style[e]=g(b[e].a,b.end,b.pos)});b.init=true}})(jQuery);


// add jQuery regex filter to use :regex selector
// http://james.padolsey.com/javascript/regex-selector-for-jquery/
jQuery.expr[':'].regex = function(elem, index, match) {
    var matchParams = match[3].split(','),
        validLabels = /^(data|css):/,
        attr = {
            method: matchParams[0].match(validLabels) ?
                        matchParams[0].split(':')[0] : 'attr',
            property: matchParams.shift().replace(validLabels,'')
        },
        regexFlags = 'ig',
        regex = new RegExp(matchParams.join('').replace(/^\s+|\s+$/g,''), regexFlags);
    return regex.test(jQuery(elem)[attr.method](attr.property));
}
// add simple popup plugin
// usage: define a div, optional with .closebutton and .calltoaction elements
//
// var mypopup = jQuery( DIV SELECTOR ).simplePopup( clickurl, cookie name )
// click URL and cookie name are optional
// click url is used to attach handler to .call to action
// cookiename is used to drop a cookie once the popup is shown
//
// public methods:
// mypopup.pop(), mypopup.unpop() mypopup.makemodal(), mypopup.makedraggable()
// pop() and unpop() do not return the popup and have to be last in the chain

//jQuery(".pqve_close_link", jQuery('#epqvPopup').contents()).remove()
// included in wm header pixels line 147
	jQuery.fn.simplePopup = function (clickurl, cookiename) {

			if (this.length > 1){
				this.each(function() {
					jQuery(this).simplePopup(clickurl, cookiename)
				});
				return this;
			}
			this.hide();
			var thispopup = this;

			this.preunpopcallback = function(){};
			this.prepopcallback = function(){};
			this.postunpopcallback = function(){};
			this.postpopcallback = function(){};


			this.overlay = jQuery("#simplepopupoverlay");

			this.modal = false;
			this.isdraggable = false;

			var closeBtn = jQuery("#toprightclose", thispopup);
			if (closeBtn.size()==0) {
				closeBtn = jQuery('<div id="toprightclose" class="closebutton"></div>')
				this.append(closeBtn);
			}

			this.hideCloseButton = function() {
				closeBtn.hide();
				return this;
			}

			this.showCloseButton = function() {
				closeBtn.show();
				return this;
			}

			this.makemodal = function() {
				var popup = this;

				if (!this.modal) {
					console.log("SimplePopup: make modal")
					if (this.overlay.size()==0) {
						console.log("SimplePopup: create Overlay")
						this.overlay =
							jQuery('<div/>', {id: "simplepopupoverlay"})
							.css({padding:0, background:"url(/images/misc/black60.png) repeat", zIndex: 10000, position:"fixed"})
							.css({width:"100%", height:jQuery(document).height(), left:0, top:0, display:"none"})
							.appendTo('body')
					}
					this.overlay.click(function(){thispopup.unpop(); return false; });
					this.css({"-moz-box-shadow": '', "-webkit-box-shadow": '', "box-shadow": "" });
					this.modal = true;
				}
				return this;
			}
			this.makedraggable = function(cancelItems) {
				if (!this.isdraggable) {
					cancelItems = (cancelItems ? cancelItems+"," : "" ) + "input,textarea,#toprightclose,a,select";
					console.log("SimplePopup: make draggable")
					this.draggable({ cancel: cancelItems,
						stop:function(){if (!thispopup.modal) thispopup.css("opacity", "1").css("cursor","pointer"); thispopup.addClass("no-reposition").removeClass("reposition");},
						start:function(){if (!thispopup.modal) thispopup.css("opacity", "0.5"); thispopup.css("cursor","move");}
					})
					.hover(
						function(){jQuery(thispopup).css("cursor","pointer")},
						function(){jQuery(thispopup).css("cursor","default")}
					)
					.mouseup(function(){jQuery(thispopup).css("cursor","pointer")})

					this.children().css("cursor","default");

					this.isdraggable = true;
				}
				return this;
			}

			this.unpop = function( ) {
				if ( typeof(this.preunpopcallback) ==="function" ) this.preunpopcallback();
				if ( thispopup.modal ) thispopup.overlay.hide();
				thispopup.delay(500).hide();
				thispopup.addClass("reposition").removeClass("no-reposition");
				repositionpopup();
				if ( typeof(this.postunpopcallback) ==="function" ) this.postunpopcallback();
			}

			this.pop = function( ) {
				if ( typeof(this.prepopcallback) ==="function" ) this.prepopcallback();
				if ( thispopup.modal ) thispopup.overlay.show();
				repositionpopup();
				thispopup.delay(500).show();
				if (cookiename) {
					jQuery.cookie(cookiename, new Date().getTime(), { path: "/"} );
				}
				if ( typeof(this.postpopcallback) ==="function" ) this.postpopcallback();
			}
			this.reposition = function () {
				if ( thispopup.hasClass("no-reposition") ) return;

				var t,l,b,r;

				if (  !thispopup.hasClass("reposition") && !isNaN(parseInt( thispopup.css("top") )) )  { // top set
					thispopup.addClass("no-reposition");
					t = thispopup.css("top") ;
					b = "auto";
				} else if ( !thispopup.hasClass("reposition") && !isNaN(parseInt( thispopup.css("bottom") )) ) { // bottom is set
					thispopup.addClass("no-reposition");
					t = "auto";
					b = thispopup.css("bottom");
				} else { // neither is set = center
					thispopup.addClass("reposition");
					t = (jQuery(window).height()-thispopup.outerHeight(false))/2;
					b = "auto";
				}
				if ( !thispopup.hasClass("reposition") && !isNaN(parseInt( thispopup.css("left") )) )  { // left set
					thispopup.addClass("no-reposition");
					l = thispopup.css("left") ;
					r = "auto";
				} else if ( !thispopup.hasClass("reposition") && !isNaN(parseInt( thispopup.css("right") )) ) { // bottom is set
					thispopup.addClass("no-reposition");
					l = "auto";
					r = thispopup.css("right");
				} else { // neither is set = center
					thispopup.addClass("reposition");
					l = (jQuery(window).width()-thispopup.outerWidth(false))/2;
					r = "auto";
				}

				if ( thispopup.hasClass("no-reposition") ) thispopup.css("position", "absolute") ;
				jQuery(thispopup).css({top:t, left:l, right:r, bottom:b})
				return thispopup;
			}
			// these vars are availble to the event handlers below - "this" is a different scope
			var removepopup = this.unpop;
			var showpopup = this.pop;
			var repositionpopup = this.reposition;

			jQuery(document).keyup(function(e) {
				if (e.keyCode == 27) {
					removepopup();
				}
			});
			jQuery(window).resize(function() {
				repositionpopup();
			});

			this.initialize = function () {
				if (cookiename) {
					if (!jQuery.cookie(cookiename)) {
						jQuery.cookie(cookiename, "seen", { path: "/"} );
					}
				}
				this
				.appendTo('body')  // for positioning
				.css({position:"fixed",zIndex: 10001}) // so it's above overlay
				.css({"-moz-box-shadow": "0px 0px 7px #999", "-webkit-box-shadow": "0px 0px 7px #999", "box-shadow": "0px 0px 7px #999" }); // shadow

				if (clickurl) {
					jQuery('.calltoaction', this).css({zIndex: 10002, cursor:"pointer"}).click(function(){
						console.log("call to action clicked");
						location.href = clickurl;
						return false;
					});
				}
				jQuery('.closebutton', this).css({zIndex: 10003, cursor:"poiner"}).click(function(){
					thispopup.unpop();
					return false;
				});
				this.addClass('SimplePopup');
				return this;
			}

			return this.initialize();
		};
/// END simple popup plugin

jQuery(function(){

if (!enableHackForWOMA1987) return; // skip if disabled

// completely override ML funciton to use SimplePopup plugin

window.closeDialogInfo = function (){
			jQuery().data.openDialogInfo_instances.each(function(){ this.unpop(); });
		}
window.openDialogInfo = function(id,header) {
	console.log("override fnct openDialogInfo - see header_pixels.html line 299")
	jQuery().data.openDialogInfo_instances = jQuery().data.openDialogInfo_instances || [];
	var f = jQuery().data.openDialogInfo_instances[id]
	if (!f || "TrackIfPopUp"===id) {
		console.log("create popup for "+id)
		f = jQuery('#'+id)
			.css({background: "#fff", textAlign: "left",width: 420, padding: "46px 40px"})
 			.simplePopup();
		if (header) {
			if("TrackIfPopUp"===id) {
				jQuery('#simple_popup_header').remove();
			}
			jQuery('<div/>', {id:'simple_popup_header'})
			.html(header)
			.addClass('simple_popup_header').prependTo(f);
		}
		jQuery().data.openDialogInfo_instances[id] = f
	}
	f.delay(300).makedraggable().makemodal().pop()
	f.showCloseButton();
	f.css('top', jQuery(".header").height() + "px");
	jQuery('html, body').animate({scrollTop: '0px'}, 300);
}

if (MarketLive && MarketLive.AccordionCheckout && MarketLive.AccordionCheckout.openDialogInfo) {
	MarketLive.AccordionCheckout.openDialogInfo = openDialogInfo;
}


jQuery.fn.ml_popup = function(dialog_title,close_text,url,type,width,height,showInCenter,scrolling) {
	console.log("override ml_popup plugin - see header_pixels.html line 320")
	jQuery().data.ml_popup_instances = jQuery().data.ml_popup_instances || [];
	var id = "popup-content";

	var f = jQuery().data.ml_popup_instances[id]
	if (!f) {
		console.log("create popup for "+id)
		f = jQuery('<div/>', {id: id})
			.appendTo('body')
			.css({height: height, width: width})
			.css({textAlign: "left",padding: "46px 40px"})
			.append(jQuery('<div/>', {id:"popup-iframe-wrapper"}))
			.simplePopup();
		jQuery(".closebutton#toprightclose").css({top: 52, right: 44});
		jQuery().data.ml_popup_instances[id] = f
		console.log(	jQuery().data.ml_popup_instances)
	}
	var frameid = new Date().getTime(); //EPQV-12 fix: use new frame id to make sure Safari will not cache frame content when going back
	var iframe = jQuery('<iframe name="' + frameid + '" framepadding="0" frameborder="0" style="background:transparent;border:none;" width="' + (width) + '" height="' + (height) + '" scrolling="'+((scrolling)?"yes":"no")+'" id="epqvPopup" allowTransparency="true"></iframe>');


	jQuery("#popup-iframe-wrapper", f).html("").append(iframe);

	jQuery.fn.dialog = function(s){ if ("close"===s) f.unpop()}

	iframe.attr('src', url);
	f.delay(300).hideCloseButton().makemodal().makedraggable().pop()
	return f;

}

}); // end override ML popup functions




