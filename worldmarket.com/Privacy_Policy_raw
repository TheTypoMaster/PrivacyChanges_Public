// start optimizely prep - carousel and prev/next buttons
// If URL has pdpcarousel=true parameter or if pdpNavOverride == false , do not execute code that overrides carousel with prev/next navigation.

try{
(function(oB, host){
var pdpNavOverride = false; // set to true to execute code and override pdp carousel navigation; set to false to skip
var pageurl = window.location.href; 

if (!pdpNavOverride || pageurl.indexOf('pdpcarousel=true')>-1) {
	console.log("keep current navigation (e.g. do not replace carousel with prev/next links)");
	return;
}

if (host.match(/worldmarket|staging/)==null) return;
jQuery('td>.nextPrevpadding').css({ height: "1px", visibility: "hidden" });

if ((!oB || oB[1].indexOf('1484430261') == -1 || oB[1].indexOf('1659510739') == -1 || oB[1].indexOf('1617350365')) && location.pathname.match(/\/product\//)!==null ) {
console.log("write next/prev links based on carousel")
jQuery(function(){
		  var p = jQuery('.imgdiv-carousel-selected:visible').parent().prev()[0];
                var n = jQuery('.imgdiv-carousel-selected:visible').parent().next()[0];
                pl = (p) ? '<a href="'+jQuery('a',p).attr("href").replace(/carousel=true/,"prevNext=true")+'">Prev Product</a>' : "Prev Product";
                nl = (n) ? '<a href="'+jQuery('a',n).attr("href").replace(/carousel=true/,"prevNext=true")+'">Next Product</a>' : "Next Product";
                jQuery('.nextPrevpadding').html('<span id="nextPrevPlaceHolder" style="float:right"><div class="ml-paging"><table cellspacing="0" cellpadding="0" border="0"><tbody><tr><td class="pdpPrevLink'+((p)?"":" sub")+'"><span>'+pl+'</span></td><td class="pdpNextLink'+((n)?"":" sub")+'"><span>'+nl+'</span></td></tr></tbody></table></div></span>').css({ height: "auto", visibility: "visible" });
  });
} 
})( document.cookie.match("optimizelyBuckets=([^;]*)"), location.hostname );
} catch(e) {
console.log("HideCarousel: "+e.toString())
} /* end optimizely prep - carousel and prev/next buttons*/


