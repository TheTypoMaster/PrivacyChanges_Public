(function(){
	var isKiosk = false; 
	var popupIsActive = false;       // [step 0] set to true if current CPWM promo includes a popup; otherwise set to false
	var newCookie = "2015wk22";      // [step 1a] set standard promo cookie name with each new promo
	var kioskCookie = "kiosk150727"; // [step 1b] set kiosk cookie name (if applicable)
	var showPop = false;
	var leftNavImage = "/text/promo-slots/2-left-of-search/2014/wk43/wk43-wmfs1502-leftnav.jpg";   // [step x] specify standard left nav image
	var promo2topbanner = "/text/promo-slots/2-left-of-search/2014/wk43/wk43-promo2-wmfs1502.png"; // [step x] specify standard top banner image
	var popupImage = "/text/promo-slots/2-left-of-search/2015/wk19/wk19_15off+FS100_popup.jpg";    // [step x] specify standard popup image
	var promo2Disclaimer = "Free Shipping on orders $150+";    
	var disclaimerID = "cpwmMarketingDisclaimer";
	var isAOS = jQuery('#storeClerkToolBar').length != 0; // If this element exists on page, we are in AOS (associate order system) mode

	// Test whether we are running on a store kiosk
	if (jQuery.cookie(kioskCookie)) {
		isKiosk = true;
	} else if (location.search.indexOf("code=WMNYSHIP") >-1 ) {
		isKiosk = true;
		jQuery.cookie(kioskCookie, "seen", { path: "/", expires: 365});
	}

	// If we are in Kiosk mode, set up kiosk image and disclaimer.
	// Otherwise, we are in standard promo mode. Use defaults (initialized above).
	// Note: there is currently no popup for kiosk.
	if (isKiosk) {
		popupIsActive = false;
		leftNavImage = "/text/promo-slots/2-left-of-search/2015/wk18/wk18-promo-kiosk-leftnav.jpg";
		promo2Disclaimer = "Pay no more than $100 per purchase for shipping, surcharges included.";
		promo2topbanner = "/text/promo-slots/2-left-of-search/2015/wk18/wk18-promo-kiosk-topbanner.png";
		var disclaimerID = "kioskDisclaimer";
	}

	// block partner popups if we have an active promotion with popup OR we are in kiosk mode OR we are in AOS mode
	window._cpwm_block_ups_stlhs = popupIsActive || isKiosk || isAOS; 

	// if there is an active CPWM promo popup and this is a new cookie, enable popup and block partner popups
	if (popupIsActive && !jQuery.cookie(newCookie)) {
		showPop = true;
	}

	jQuery(function(){
		// Set up top banner
		jQuery ('#cpwm_promo2 img').attr('src', promo2topbanner);

		// Set up Left Nav
		// [] change leftnav width and height if needed.
		if (jQuery('.navleftbg .leftPromoSlot').size() === 0) {
			jQuery('<div style="background:url(/images/worldmarket/en_us/global/globalgraphics/fabric.png) repeat; padding-bottom:10px"><img src="" width="223" height="122" alt="" class="leftPromoSlot" style="cursor:pointer" title="Click for Details" /></div>').prependTo('td.navleftbg');
			jQuery('.leftPromoSlot').attr('src', leftNavImage);
		}		

		var marketingPopup = jQuery('#cpwmMarketingPopup').simplePopup().hideCloseButton().makemodal();
		var isHome = (jQuery('#homeSlides').length != 0) || (jQuery('#cpwmhome').length != 0);

		console.log ("Home Page: " + isHome);
		showPop = showPop && (jQuery('.revtrax').length == 0);

		// Setup Image popup
		if (location.pathname.indexOf("/admin/") >-1 ) return;

		if (showPop) {
			jQuery ('#cpwmMarketingPopup img').attr('src', popupImage);
			marketingPopup.pop();
			jQuery.cookie(newCookie, "seen", { path: "/", expires: 7} );
		}


		// Setup Disclaimer for PROMO2 and LeftNavPromoSlot
		jQuery('#cpwm_promo2, .leftPromoSlot').click(function(){
			openDialogInfo(disclaimerID, promo2Disclaimer); // Div ID disclaimer + Disclaimers Headline
		});

		// [3] Adjust parameters for Promo Popup if applicable. See step 5 for changing Popup image.
		jQuery('#cpwmMarketingPopup').click(function(e){
			var a = window.location.href;
			var offset = jQuery(this).offset();
			var relativeX = (e.pageX - offset.left);
			var relativeY = (e.pageY - offset.top);
			var threshold = 425; // [3a] hotspot offset from top of graphic; typical value is 345; use image height (typically 425) if entire graphic is a single hotspot

			console.log ("User clicked at y-coordinate " + relativeY + " | threshold set to " + threshold);

			// if user clicks above the threshold, go to URL, else stay on this page and apply promo code
			if (relativeY > threshold) {
				window.location.href = '/category/code/109530.do?coupon=ffship0987'; // [3b] set url eg /category/code/109530.do?coupon=sunnydays
			}
			else {
				a += (a.indexOf('?')>1 ? "&":"?");
				window.location.href =  a + 'code=HELLOJULY'; // [3c] change promo source code
			}

			return false;
		});
	});
})()
