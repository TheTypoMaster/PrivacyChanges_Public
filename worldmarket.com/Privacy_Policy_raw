			  autoOpen: false,
			  closeOnEscape:true,
			  modal:false,
			  width:'500',
			  height:'400',
			  dialogClass:'modalDialogBox',
			  beforeClose: function(event,ui){jQuery.unblockUI();}
			});		
			jQuery('#dialogContainer').dialog('open');
			jQuery('#dialogContainer .popContainer').css('height','315px');
		}

