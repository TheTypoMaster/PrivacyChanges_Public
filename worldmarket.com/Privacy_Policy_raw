<script language="javascript" type="text/javascript">
var autoRedirectTimerObject;
function validateEmail(emailField,errorField)
{
		$("#"+errorField).hide();
		var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
        if (reg.test($('#'+emailField).val()) == false) 
        {
			$("#"+errorField).show();
			return false;
        }
		$("#"+errorField).hide();
        return true;
}
function closeKioskPopUp()
{
	$( "#simplepopupoverlay" ).css( "display", "none" );
	$( "#kioskpopup" ).css( "display", "none" );
}
function closeCheckoutWithAssociatePopUp()
{
	$( "#simplepopupoverlay" ).css( "display", "none" );
	window.location.href="/clearkiosksession.do";
}
function emailKioskBasket(emailField,errorField)
{
	var kioskRedirectTime=0;
	$("#"+emailField).val($.trim($("#"+emailField).val()));
	if(validateEmail(emailField,errorField))
	{
	if($("#"+emailField).val().length > 0) {
		kioskRedirectTime=0;
	$.ajax({
		type: "POST",
		url: "/basket.do?method=sendKioskEmail&r="+ Math.random(),
		data: {kioskEmailaddress: $("#"+emailField).val()},
		success: function(resultData){
				$("#saveAndSentBtn").html("Thank you. Your cart has been sent to "+$("#"+emailField).val());
				$("#saveAndSentEndSessionBtn").show();
				autoRedirectTimerObject= setInterval(autoRedirect,kioskRedirectTime);
		},
		error: function(e){
			if(e.responseText.indexOf("err.common.action.failed") !=-1 )
				{
					$("#emailBasketSentError").show();
					autoRedirectTimerObject= setInterval(autoRedirect,kioskRedirectTime);
				}
		}
	});
	}
	}
 }			
function sendKioskBasketToAssociate(emailField,errorField)
{
	var kioskRedirectTime=0;
	$("#"+emailField).val($.trim($("#"+emailField).val()));
	if(validateEmail(emailField,errorField))
	{
	if($("#"+emailField).val().length > 0) {
		kioskRedirectTime=0;
	$.ajax({
		type: "POST",
		url: "/basket.do?method=sendKioskBasketToAssociate&r="+ Math.random(),
		data: {kioskEmailaddress: $("#"+emailField).val()},
		success: function(resultData){
				$("#associateCheckout").html("Thank you! Please see an associate to complete your purchase.");
				$("#associateCheckoutBtn").hide();
				$("#checkoutAssociatePopup #toprightclose").hide();
				autoRedirectTimerObject = setInterval(autoRedirect,kioskRedirectTime);
		},
		error: function(e){
			
			if(e.responseText.indexOf("err.common.action.failed") !=-1 )
				{
					$("#endBasketAssociateError").show();
					autoRedirectTimerObject = setInterval(autoRedirect,kioskRedirectTime);
				}
		}
	});
	}
  }
 }

function autoRedirect() {
	window.clearInterval(autoRedirectTimerObject);
	window.location.href="/clearkiosksession.do";
} 
 
	initShopatronConstants('Enter City, State or ZIP','make this my store','change',14);
	getCurrentStore();

	
	if(typeof getCookie('C_TRAFFIC_LIGHT_VISIBLE') == "undefined"){
	  jQuery.ajax({url:"/ipaddress/trafficlightvisibility.do",success:function(data){
		if(data){
			setCookie('C_TRAFFIC_LIGHT_VISIBLE','true',1);
		}else{
			setCookie('C_TRAFFIC_LIGHT_VISIBLE','false',1);
		}
	  }});
	}
  </script>
</div>
