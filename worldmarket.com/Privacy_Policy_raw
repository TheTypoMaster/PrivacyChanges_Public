/* TagMan data mapping */
(function () {
	if (location.hostname.indexOf("staging.marketlive.com")==-1) return;
	console.log( "Begin TagMan data mapping" )

	if ( typeof(ml_order)==="object" && (location.pathname=="/checkout/thankyou.do" || location.pathname=="/checkout/accountsetup.do")) { // only do if the ml_order object is defined
		console.log( "\tTagMan: have ml_order object - setup order tm params" )

	// order
		window.tmParam.levrev = 				cpwm_page.order().total || "";
		window.tmParam.levordref = 	 			cpwm_page.order().id || "";
		window.tmParam.product_quantity_total = 		cpwm_page.order().totalitemcount || "";
		window.tmParam.product_id = 			cpwm_page.order().itemsSKUs.join("|");
		window.tmParam.product_quantity = 			cpwm_page.order().itemsQty.join("|");
		window.tmParam.product_price = 			cpwm_page.order().itemsPricesWithDiscount.join("|");
		window.tmParam.tm_icent =			 	cpwm_page.order().itemsPricesWithDiscountCents.join("|");
		window.tmParam.tm_iname = 				cpwm_page.order().itemNames.join("|");
		window.tmParam.product_sku = 			cpwm_page.order().itemsSKUs.join("|");
		console.log( "\tTagMan: order tm params done" )

	} // end if the ml_order object is defined
	else
	{ // not on order confirm
	//cart
		console.log( "\tTagMan: setup cart tm params" )
		window.tmParam.levrev = 				cpwm_page.cart().merchTotal || "";
		window.tmParam.product_quantity_total = 		cpwm_page.cart().itemCount || "";
		window.tmParam.product_sku = 			(cpwm_page.cart().itemSKUs && cpwm_page.cart().itemSKUs.join("|"))
									 || (cpwm_page.cart().itemNames && cpwm_page.cart().itemNames.join("|")) || "";
		window.tmParam.tm_iname = 				( cpwm_page.cart().itemNames && cpwm_page.cart().itemNames.join("|")) || "";
		window.tmParam.product_quantity = 			( cpwm_page.cart().itemQty && cpwm_page.cart().itemQty.join("|")) || "";
		console.log( "\tTagMan: cart tm params done" )
	//page
		console.log( "\tTagMan: setup page tm params" )
		window.tmParam.tm_product_image = 			cpwm_page.product().image || "";
		window.tmParam.tm_product_sku = 			cpwm_page.product().SKU || "";
		window.tmParam.tm_product_available = 		cpwm_page.product().available || "";
		window.tmParam.tm_product_name = 			escape(cpwm_page.product().name || "");
		window.tmParam.product_category = 			escape(cpwm_page.product().category || cpwm_page.breadcrumb().join(",") || "");
		window.tmParam.tm_sale_price = 			escape(cpwm_page.product().salePrice  || "");
		window.tmParam.tm_reg_price = 			escape(cpwm_page.product().regularPrice  || "");
		console.log( "\tTagMan: page tm params done" )
	} // end not on order confirm
	/* END TagMan data mapping */
	console.log( "TagMan: tm params complete" )

})();


///####### BloomSurface tracking code.  Place at foot of page. 

window.br_data = {};
try{
(function () {
	var br_data = window.br_data;
		br_data.acct_id = "5189";
		br_data.ptype = "";
		br_data.cat_id = "";
		br_data.cat = "";
		br_data.prod_id = "";
		br_data.prod_name = "";
		br_data.pstatus= "";
		br_data.sku = "";
		br_data.price = "";
		br_data.search_term = "";
		br_data.sale_price = "";
		br_data.is_conversion = "0";
		br_data.basket_value = "";
		br_data.order_id ="";
		br_data.basket={"items": []};

//	if (location.hostname.indexOf("staging.marketlive.com")==-1) return;
	console.log( "Begin BloomReach data mapping" )

	if ( typeof(ml_order)==="object" && (location.pathname=="/checkout/thankyou.do" || location.pathname=="/checkout/accountsetup.do")) { // only do if the ml_order object is defined
		console.log( "\tBR: have ml_order object - setup order params" );
		br_data.is_conversion = "1";
		br_data.basket_value = cpwm_page.order().total;
		br_data.order_id = cpwm_page.order().id;
		br_data.ptype = "other";
		var orderitemcount = cpwm_page.order().itemsSKUs.length;
		for (oi=0; oi<orderitemcount; oi++) {
			var orderItem = {
				"prod_id": cpwm_page.order().itemsCodes[oi],
				"sku": cpwm_page.order().itemsSKUs[oi],
				"name": unescape(cpwm_page.order().itemNames[oi]),
				"quantity": cpwm_page.order().itemsQty[oi],
				"price": cpwm_page.order().itemsPrices[oi],
				"mod": cpwm_page.order().itemsPrices[oi]-cpwm_page.order().itemsPricesWithDiscount[oi]			
			};
			br_data.basket.items.push(orderItem);
		}
	} else {
	//page
		br_data.ptype = cpwm_page.pageType();
		if ( br_data.ptype.match(/kit|single|dropdown|family/)!==null ) {
			br_data.ptype = "product";
		} else if ( br_data.ptype.match(/gateway|directory/)!==null ) {
			br_data.ptype = "category";
		} else if ( br_data.ptype == "search" ) {
			br_data.search_term = decodeURIComponent(cpwm_page.getQueryValue("query").replace(/\+/," "));
		}

		if(br_data.ptype=="category" || br_data.ptype=="product")
		{
			br_data.cat_id = catCode;
		}

		br_data.cat = cpwm_page.breadcrumb().join("|") || jQuery('h1#catHeader').text() || "";
		//br_data.prod_id = jQuery('#mainForm').serialize().replace(/^.*productPk=(\d+).*$/,"$1");
		if(typeof cpwm_page.product().code !="undefined")
		{
			br_data.prod_id = cpwm_page.product().code;
		}
		br_data.prod_name = escape(cpwm_page.product().name) || "";
		br_data.pstatus= cpwm_page.product().available || "";
		br_data.sku = cpwm_page.product().SKU || "";
		br_data.price = cpwm_page.product().regularPrice  || "";
		if(typeof cpwm_page.product().salePrice!="undefined" && cpwm_page.product().salePrice.length==0)
		{
			br_data.sale_price = cpwm_page.product().regularPrice  || "";
		}else{
			br_data.sale_price = cpwm_page.product().salePrice  || "";
		}
	}

	(function() { 
	var brtrk = document.createElement('script');
	brtrk.type = 'text/javascript';
	brtrk.async = true;
	brtrk.src = 'https:' == document.location.protocol ? "https://cdns.brsrvr.com/v1/br-trk-"+br_data.acct_id+".js" : "http://cdn.brcdn.com/v1/br-trk-"+br_data.acct_id+".js";
	var s = document.getElementsByTagName('script')[0];
	s.parentNode.insertBefore(brtrk, s);
	})();

})();
} catch (e) {
	console && console.log && console.log("BR pixel error: "+e.toString());
}

// add to basket event API call to BR
jQuery('input[alt="Add To Basket"]').click(function(){
	var f = jQuery(this.form).serialize();
	var prod_code = jQuery(this).attr('productcode');
	var sku = jQuery('span[itemprop="sku"]').text().trim();
	var prod_name = escape(cpwm_page.product().name);
	var price = cpwm_page.product().regularPrice;
	var sale_price = cpwm_page.product().salePrice;
	BrTrk.getTracker().logEvent('cart', 'click-add', {'prod_id': prod_code, 'sku' : sku, 'prod_color':'', 'prod_name': prod_name},{'price' : price, 'sale_price' : sale_price});
})

// add to basket event API call to BR, family page
jQuery('div.customFamily_addtobasket_button').click(function(){

	var prod_id = jQuery(this).attr('id').replace(/enabledAddToBasket_/,"");
	var prod_code = jQuery(this).attr('productcode');
	var form = jQuery('form#addToCart_'+prod_id);
	var sku = jQuery('span#selectedSkuNumber_'+prod_id).text().trim();
	var prod_name = escape(jQuery('span#selectedSkuNumber_'+prod_id).parent().prev('div.productName').text().trim());
	var price, salePrice;

	var pricing = jQuery('form#addToCart_'+prod_id+' span#productPricing');
	if (0==jQuery("div.singlePrice span.pricesale", pricing).size()) { // not on sale 
		price = jQuery('span[itemprop="price"]', pricing).html().trim().replace(/[^0-9\.\-]/g,"");
	} else { // onsale
		price = jQuery("span.pricewas", pricing).html().trim().replace(/[^0-9.\-]/g,"");
		salePrice = jQuery("span.pricesale", pricing).html().trim().replace(/[^0-9.\-]/g,"");
	}
	price = price.replace(/-.*/,"");
	salePrice = (salePrice) ? salePrice.replace(/-.*/,"") : "";

	BrTrk.getTracker().logEvent('cart', 'click-add', {'prod_id': prod_code, 'sku' : sku, 'prod_color':'', 'prod_name': prod_name},{'price' : price, 'sale_price' : salePrice});

})

/* change qty/remove/saveforLater event handlers for BloomReach */
jQuery(function(){
	var BRCartRemove = function (qtyfieldID, action,pcode) {
		if (!action) return;
		var dtl = getItemDetails(qtyfieldID,pcode);
		console.log("BRremove " + action + " " + qtyfieldID);
		BrTrk.getTracker().logEvent('cart', 'remove', 
			{'prod_id': dtl.prod_id, 'sku' : dtl.sku, 'prod_color':'', 'prod_name': dtl.prod_name},{'price' : dtl.price, 'sale_price' : dtl.salePrice});
	}
	var BRCartAdd = function (qtyfieldID, action,pCode) {
		if (!action) return;
		var dtl = getItemDetails ( qtyfieldID,pCode );
		console.log("BRadd " + action + " " + qtyfieldID );
		BrTrk.getTracker().logEvent('cart', 'click-add', 
			{'prod_id': dtl.prod_id, 'sku' : dtl.sku, 'prod_color':'', 'prod_name': dtl.prod_name},{'price' : dtl.price, 'sale_price' : dtl.salePrice});
	}

	var getItemDetails = function ( qtyfieldID,pcode ) {
		var details = { prod_id:"", sku:"", prod_name:"", price:"", salePrice:"" };
		var row = jQuery('input.qtyField#'+qtyfieldID).parents("tr:eq(0)");
		details.prod_name = jQuery('div.basketName a', row).text().trim();
		//details.prod_id = qtyfieldID.replace(/[^\d]+/, "");
		details.prod_id =pcode ;
		details.sku = jQuery('div.basketSku', row).text().trim().replace(/[^\d]/g,"");
		if (jQuery('span.salePrice', row).size()>0) {
			details.price = jQuery('span.pricewas', row).text().trim();
			details.salePrice = jQuery('span.pricesale', row).text().trim();
		} else {
			details.price = jQuery('div.regularPrice, div.basketPriceEach', row).text().trim();
		}
		/*CPWM-286 & CPWM-287*/
		if(details.salePrice!=""){
		details.salePrice  = details.salePrice.replace(/[^.\d]/g,"");
		if(details.salePrice.indexOf('.') > 0)
		details.salePrice = details.salePrice.substr(0,details.salePrice.indexOf('.') + 3);
		}
		if(details.price!=""){
		details.price  = details.price.replace(/[^.\d]/g,"");
		if(details.price.indexOf('.') > 0)
		details.price = details.price.substr(0,details.price.indexOf('.') + 3);
		}
		console.log("prod_id"+details.prod_id);		
		console.log(details.price);
		console.log(details.salePrice);
		return details;
	}

	jQuery('.tableitem div a:contains("Update"), .tableitem div a:contains("Save for Later"), .tableitem div a:contains("Remove")').click(
		function(){
			var myaction = jQuery(this).text().trim();
			var myqtyfieldID = jQuery('input', jQuery(this).parent().parent()).attr('id');
			var pCode = jQuery('input', jQuery(this).parent().parent()).attr('productcode');
			console.log(jQuery(this).text().trim() +" "+myqtyfieldID)
			if ( myqtyfieldID.match(/shopatron/i)!==null ) return; // ignore Update/Remove in Saved for Later section
			//if ( jQuery('input#'+myqtyfieldID ).val()>0 && myaction == "Update" ) return; // ignore updates where qty>0
			var itemid = myqtyfieldID.replace(/[^\d]+/, "");			
			BRCartRemove ( myqtyfieldID, myaction,pCode  );
		}
	)
	jQuery('.tableitem div a:contains("Move to cart")').click(
		function(){
			var myaction = jQuery(this).text().trim();
			var myqtyfieldID = jQuery('input', jQuery(this).parent().parent()).attr('id');
			var pCode = jQuery('input', jQuery(this).parent().parent()).attr('productcode');
			console.log(jQuery(this).text().trim() +" "+myqtyfieldID)
			var itemid = myqtyfieldID.replace(/[^\d]+/, "");
			BRCartAdd ( myqtyfieldID, myaction,pCode );
		}
	)

}); 


/*######### --- End BloomReach section --- */

jQuery(function() {

// hack to kill quickshop for iPad and browsers where window height < 500 px
if (null !== navigator.appVersion.match(/\(iPad.+?Mac OS .*/) || jQuery(window).height()<600) jQuery('div.qveThumbnail').unbind('mouseover');

// append the dialog div if it doesn't exist - needed because checkout doesn't include this.
if (jQuery('#dialogContent').size()==0) {
jQuery("body").append('<div id="dialogContainer" style="display:none;"><div class="modalDialogBox" style="width:500px;height:400px"><div class="popClose" id="dialogTitle"><a href="javascript:closeDialogInfo()"><img src="/images/worldmarket/en_us/local/localnav/pqve_close_btn.png" title="Close"></a></div><div class="popHeader">Title</div><div id="dialogContent" class="popContainer"></div></div></div>');
}

/// hack to correctly position search auto-complete dropdown
jQuery('.autocomplete-w1').parent().addClass('ACcont')

	// if on pw recov - add WME msg
	if (location.pathname.indexOf('/passwordrecovery.do')>-1 && jQuery("div.errorblock ul li").size()>0 ) jQuery("div.errorblock ul")
		.append(
			jQuery('<li/>')
			.css({color:"#6898C0" })
			.html('Looking for our World Market Explorer Program? <a style="color: #6898C0;" href="http://www.worldmarketexplorer.com">Click here!</a>')
		);


	// add GA tracking for ISPU
	//
	if (typeof(getStores)==="function") {
		var do_getStores = getStores;
		getStores = function (skuCode, storeZipCode) {
        		if (MarketLive.Reporting.googleAnalyticsUniversalEnabled) {
        		    _gaq('send', 'event', 'InStore_Availability', 'Entered_Zipcode',storeZipCode);
        		} else {
        		    _gaq.push(['_trackEvent', 'InStore_Availability', 'Entered_Zipcode',storeZipCode]);
        		}
			do_getStores(skuCode, storeZipCode);
		}
	}



});

</script>


