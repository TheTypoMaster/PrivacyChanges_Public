       <script type='text/javascript'>var isMobileEnabled = 'true';</script><script type="application/javascript">
       var mobile_redirect = function() {
	return {
		is_touch: (('ontouchstart' in window) || (navigator.msMaxTouchPoints > 0)),

		initializeRedirects : function () {
			var redirect = parseInt(this.readCookie('MobileOptOut')) != 1;
			var opt_out  = this.getUrlParam('MobileOptOut');

			if (opt_out) {
				switch (parseInt(opt_out)) {
					case 0:
						// Remove Session Opt Out
						document.cookie = "MobileOptOut=0; expires=Thu, 01-Jan-70 00:00:01 GMT; path=/;";

						redirect = true;
						break;
					case 1:
						// Session Opt Out
						document.cookie = "MobileOptOut=1; expires=0; path=/;";

						redirect = false;
						break;
					case 2:
						// Single Page Opt Out
						redirect = false;
						break;
				}
			}

			if (redirect === true && this.isMobile()) {
				var url = false;
				var check = window.location.href;

				if (check.search('www.') != -1) {
					// Production Redirect

					url = check.replace(/:\/\/www\./i, '://m.');
				} else if (check.search('stores.') == -1) {
					// Rio Redirect

					url = url.replace(
						/(\.com\/|\.ca\/)/i,
						function (match, contents, offset, s) {
							return match + "mobile/";
						}
					);
				} else {
					// Other Redirect

					var part = check.match(/:\/\/([a-z]+)\d+-(v\d+)-(\w+)\./i);

					url = check.replace(part[0] + "marketlive.com", '://' + part[1] + ".uat.bbhosted.com");
				}
				// Mobile redirect fix
				if (url && window.location.pathname == "/category/coupons.do") {
					url = url.replace("/category/coupons.do", "/offers");
				}
				if (url && window.location.pathname == "/category/company/careers.do") {
					url = url.replace("/category/company/careers.do", "/careers");
				}

				if (url && window.location.pathname == "/category/join+our+team.do" && window.location.host.search('halloween') != -1) {
					//url = "https://recruiting.adp.com/srccar/public/RTI.home?d=HCEvergreen&c=1098541";
					url = url.replace("/category/join+our+team.do", "/careers");
				}

				if (document.referrer.length != 0) {
					url += (url.indexOf('?') != -1) ? '&' : '?';
					url += 'udo_ref=' + encodeURIComponent(document.referrer);
				}

				window.location.href = url;
			}
		},

		isMobile : function () {
			var min_dim = Math.min(screen.availHeight, screen.availWidth);

			var is_mobile = (min_dim < 700);

			if (!navigator.userAgent.match(/Mobile/i)) {
				is_mobile = false
			}

			return is_mobile;
		},

		getUrlParam : function (name) {
			name = name.replace(/[\[]/,"\\\[").replace(/[\]]/,"\\\]");

			var regexS = "[\\?&]" + name + "=([^&#]*)";
			var regex = new RegExp(regexS);
			var results = regex.exec(window.location.href);
			var result = false;

			if (results && results.length >= 1) {
				result = results[1];
			}

			return result;
		},

		readCookie : function(name) {
			var result = false;

			try {
				var name_eq = name + "=";
				var cookie = document.cookie.split(';');

				for (var i = 0; i < cookie.length && result === false; i++) {
					var c = cookie[i];

					value = c.split("=");
					if (value[0] == name) {
						result = '';
						for (var j = 1; j < value.length; j ++) {
							result += (result.length == 0) ? "" : "=";
							result += value[j];
						}
					}
				}
			} catch (e) {
			}

			return result;
		}
	};
}();

mobile_redirect.initializeRedirects();</script><script language="JavaScript" type="text/javascript" src="/text/scripts/tag_mgmt.js?r=Wed Sep 02 04:46:43 PDT 2015"></script>
