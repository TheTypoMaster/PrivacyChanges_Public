				function tpGCScroll(){
					var tpGC=jQuery("#topGCSection");
					if (jQuery(document).scrollTop()>10 && !tpGC.hasClass("topGCSection")){
						tpGC.addClass("topGCSection");
					}
					else if(jQuery(document).scrollTop()< 10 && tpGC.hasClass("topGCSection")){
						tpGC.removeClass("topGCSection");
					}
				}
				function tpGCScrollM(){
					var tpGC=jQuery("#topGCSection");
					if (jQuery(document).scrollTop()>10){
						tpGC.css("top", (jQuery(document).scrollTop()-10)+"px");
					}
					else if(jQuery(document).scrollTop() < 10){
						tpGC.removeAttr("style");
					}
				}
				if(typeof uagent === 'function' && uagent()=="mobile"){
					jQuery(window).scroll(tpGCScrollM);
				} else {
					jQuery(window).scroll(tpGCScroll);
				}
