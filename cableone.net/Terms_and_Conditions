<script type="text/javascript" language="JavaScript">

var ref_host = document.referrer.substr(0,document.URL.indexOf("/",9) + 1);
var site_host = document.URL.substr(0,document.URL.indexOf("/",9) + 1);
var fromExternal = ((ref_host != site_host) && (document.URL == (site_host + "Pages/default.aspx")));

$j(document).ready(function() {
function getUrlVars() {
                var vars = {};
                var parts = window.location.href.replace(/[?&]+([^=&]+)=([^&]*)/gi, function(m, key, value) {
                    vars[key] = value;
                });
                return vars;
            }
	    var cur_system = C1.readCookie("c1_systemID");
            var setlocation = getUrlVars()["setlocation"];
           
            if (setlocation==1 && (cur_system==null||cur_system=="")) {
		
		C1.func.showSetLocation();
	
	    }
});

if (fromExternal)
{
  var cur_system = C1.readCookie("c1_location");
  if (cur_system != null)
  {
      locationIsSet();
  }
  else
  {
    //  check for old location Cookie 
    var oldSystem = C1.readCookie("CableOneSystem");
    if (oldSystem != null)
    {
  	C1.func.getOldSystem(oldSystem, function(city) {
  		C1.setCookie("c1_location",city.Name + ", " + city.State,365);
  		locationIsSet();
        });
    }
  }
}

function locationIsSet()
{
  window.location = site_host + "LI/";
}
</script>

