		<script type="text/javascript" >
		function nextAd2()
		{
		    var now = new Date();
			var i = 1*$('#i2').val();
		    var url = '/AdRetriever.asp?pos=2&i=' + i + '&ts=' + now.getTime();
		    makeHttpRequest(url, 'loadBanner2', true);
			$('#i2').val(i+1);
		}
		$(document).ready(function(){  
			$('#i2').val(0);
			nextAd2();
		});
		</script>
