    window.twttr.ready(function (twttr) {
      twttr.events.bind('follow', buttonsProcess);
      twttr.events.bind('unfollow', buttonsProcess);
    });
    function buttonsProcess(event) {
      $('.twitter-follow-button').each(function () {
        if ($(this).attr('src') != event.target.src) {
          $(this).parent().html($(this).clone());
        }
      });
