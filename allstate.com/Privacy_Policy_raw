<script type="text/javascript">  
  Global.isDrawerSticky = true;

  $(window).load(function()
  {
    Drawer.quoteBox();
    Drawer.agentCarousel('DrawerContainer');
    var myRestorePageState = Global.RestorePageState();
    if (myRestorePageState)
    {
      if (myRestorePageState == 'Drawer')
      {
        Global.ClearPageState();
        Global.ExpandDrawer();
      }
    }
    $(".drawerInner").width('943px');

    Drawer.checkAgentImages();

    if (typeof myNavPrev !== 'undefined')
    {
      Drawer.setNavigationText(myNavPrev, myNavNext);
    }
  });
