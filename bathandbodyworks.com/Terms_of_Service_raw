    <script type="text/javascript">

    var extra_attributes = {};

    if(elementHasClass(body, 'product')){
        var category = document.getElementById('categoryid').value;
        if(category != null){
            var split = category.split('(');
            if (split.length > 0){
                category = split[0];
            }
        }
        var price = '';
        var productPriceElemList = document.querySelectorAll('span.product-price');
        if(productPriceElemList.length > 0){
            var productPriceElem = productPriceElemList[0];
            var priceList = productPriceElem.querySelectorAll('.price');
            if(priceList.length > 0){
                var priceItem = priceList[0];

                if(priceItem != null){
                    price = priceItem.innerHTML.trim().replace('$', '');
                }

                if(elementHasClass(productPriceElem, 'discount')){
                    var originalValue = productPriceElem.querySelectorAll('del .value');
                    if(originalValue != null){
                        var originalPrice = originalValue[0].innerHTML.trim().replace('$', '');
                        var discount = parseFloat(originalPrice) - parseFloat(price);
                        price = originalPrice;
                        extra_attributes.product_discount = [discount.toFixed(2).toString()];
                    }
                }
            }
        }
        extra_attributes.page_product_category = category;
        extra_attributes.product_price = [parseFloat(price).toFixed(2).toString()];
    }
    else if(elementHasClass(body, 'help') && document.getElementById('helpdesk-contactform') != null){
        pageName = "contact us";
        pageType= "contact us page";
    }
    else if(elementHasClass(body, 'cart')){

        var contents = document.getElementById('cart-contents');

        //Some pages like my favorites have the same "cart" template, we need to further check for this #cart-contents to make sure it's the real cart page
        if(contents != null){
            pageType = "checkout page";
            isTealiumPage = true;
            extra_attributes.checkout_step = "cart";

            if(!isEmptyArray(productIds)){
                extra_attributes.product_id = productIds;
            }
            if(!isEmptyArray(productNames)){
                extra_attributes.product_name = productNames;
            }
            if(!isEmptyArray(quantities)){
                extra_attributes.product_quantity = quantities;
            }
            if(!isEmptyArray(skuIds)){
                extra_attributes.product_sku = skuIds;
            }
            if(!isEmptyArray(brandIds)){
                extra_attributes.product_brand = brandIds;
            }
            if(!isEmptyArray(prices)){
                extra_attributes.product_price = prices;
            }
            if(!isEmptyArray(discounts)){
                extra_attributes.product_discount = discounts;
            }
        }
        else { // In the case of my fave fragrances we still use it as tealium page but under account page type
            var favFragrances = document.querySelectorAll('div.favorite-fragrances');
            if(favFragrances != null){
                pageType = "account page";
                isTealiumPage = true;
            }
        }

    }
    else if(elementHasClass(body, 'home')){
        var containerElement = document.getElementById('container');
        if(containerElement != null && containerElement.getAttribute('class').trim() == ""){
            pageType = "home page";
            isTealiumPage = true;
        }
    }
    /*else if(elementHasClass(body, 'checkout') || elementHasClass(body, 'cart')){

        var prices = [];
        var discounts = [];
        jQueryTealium('.item-table td.price').each(function(){
            var discountedPriceElem = jQueryTealium('.discountedPrice', this);
            var price = '0';
            var discount = '0';
            if(discountedPriceElem.length > 0){
                var newPriceElem = jQueryTealium('.nowPrice', this);
                price = newPriceElem.text().trim().replace('$', '');
                var oldPrice = discountedPriceElem.text().trim().replace('$', '');
                if(price == 'FREE'){
                    price = '0';
                }
                discount = parseFloat(oldPrice) - parseFloat(price);
                discount = discount.toString();
            }
            else {
                price = jQueryTealium(this).text().trim().replace('$', '');
            }
            prices.push(price);
            discounts.push(discount);
        });
        if(prices.length > 0)
            extra_attributes.product_price = prices;
        if(discounts.length > 0)
            extra_attributes.product_discount = discounts;
    }
    */

    if(isTealiumPage){


        pageName = getTitle();

        var utag_data = {
        "logged_in_state": "anonymous",
        "has_account": "no",
        "page_type" : pageType,
        "page_name" : pageName,
        "site_currency": "USD",
        "cart_value": "0.00"
        };

        

        for(key in attributes){
            utag_data[key] = attributes[key];
        }

        for(key in extra_attributes){
            utag_data[key] = extra_attributes[key];
        }

        for(key in utag_data){
            var attrib_value = utag_data[key];
            if(typeof attrib_value == "string" && attrib_value != null){
                utag_data[key] = attrib_value.toLowerCase();
            }
        }

    }
</script>

<script type="text/javascript">

    function tealiumTest(){

        console.log("-------------------------------");
        console.log("---------TEALIUM DATA----------");
        console.log("-------------------------------");
        var propValue;
        for(var propName in utag_data) {
        propValue = utag_data[propName]

        console.log(propName,':',propValue);
        }

        console.log("-------------------------------");
        console.log("------ END TEALIUM DATA--------");
        console.log("-------------------------------");
    }

    window.onload = function() {

        var emailSubmits = document.querySelectorAll('.footerEmailSignupSubmit');
        for(i=0; i< emailSubmits.length; i++){

            var emailSubmit = emailSubmits[i];

            if(emailSubmit.addEventListener){

                emailSubmit.addEventListener("click", function(){

                    var email = this.parentNode.querySelectorAll('.footerEmailSignupInput')[0].value;
                    utag.link({
                        "event_type" : "email sign up",
                        "event_category" : "sign up",
                        "event_action" : "form submit",
                        "event_label" : "email sign up form submit",
                        "customer_email" : email
                    });
                });
            }
            else {
                emailSubmit.attachEvent("onclick", function(){

                    var email = window.event.srcElement.parentNode.querySelectorAll('.footerEmailSignupInput')[0].value;
                    utag.link({
                        "event_type" : "email sign up",
                        "event_category" : "sign up",
                        "event_action" : "form submit",
                        "event_label" : "email sign up form submit",
                        "customer_email" : email
                    });
                });
            }
        }

        var storeLocatorSubmits = document.querySelectorAll('.footerStoreLocatorSubmit');
        for(i=0; i< storeLocatorSubmits.length; i++){

            var storeLocatorSubmit = storeLocatorSubmits[i];

            if(storeLocatorSubmit.addEventListener){
                storeLocatorSubmit.addEventListener("click", function(){
                    var zipCode = this.parentNode.querySelectorAll('.footerStoreLocatorZip')[0].value;
                    utag.link({
                        "event_type" : "store locator",
                        "event_category" : "store location",
                        "event_action" : "form submit",
                        "event_label" : "store locator form submit",
                        "zip code" : zipCode
                    });
                });
            }
            else {
                storeLocatorSubmit.attachEvent("onclick", function(){
                    var zipCode = window.event.srcElement.parentNode.querySelectorAll('.footerStoreLocatorZip')[0].value;
                    utag.link({
                        "event_type" : "store locator",
                        "event_category" : "store location",
                        "event_action" : "form submit",
                        "event_label" : "store locator form submit",
                        "zip code" : zipCode
                    });
                });
            }
        }
    };

