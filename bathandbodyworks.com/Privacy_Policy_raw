<script type="text/javascript">

    if(!document.querySelectorAll){

        // IE7 support for querySelectorAll in 274 bytes. Supports multiple / grouped selectors and the attribute selector with a "for" attribute. http://www.codecouch.com/
        (function(d,s){d=document,s=d.createStyleSheet();d.querySelectorAll=function(r,c,i,j,a){a=d.all,c=[],r=r.replace(/\[for\b/gi,'[htmlFor').split(',');for(i=r.length;i--;){s.addRule(r[i],'k:v');for(j=a.length;j--;)a[j].currentStyle.k&&c.push(a[j]);s.removeRule(0)}return c}})()
    }

    function isEmptyArray(array){

        var isEmpty = true;

        for(i=0; i < array.length; i++){
            if(array[i] != ''){
                isEmpty = false;
                array[i] = array[i].toLowerCase();
            }
        }

        return isEmpty;
    }

    function elementHasClass(element, selector){
        var className = " " + selector + " ";
        var rclass = '/[\n\t\r]/g';
        if ((" " + element.className + " ").replace(rclass, " ").indexOf(className) > -1) {
            return true;
        }
        return false;
    }

    if(typeof String.prototype.trim !== 'function') {
      String.prototype.trim = function() {
        return this.replace(/^\s+|\s+$/g, '');
      };
    }

    String.prototype.endsWith = function(suffix) {
        return this.indexOf(suffix, this.length - suffix.length) !== -1;
    };

    if (typeof String.prototype.startsWith != 'function') {
      // see below for better implementation!
      String.prototype.startsWith = function (str){
        return this.indexOf(str) == 0;
      };
    }

    String.prototype.replaceAt=function(index, character) {
        return this.substr(0, index) + character + this.substr(index+character.length);
    }



    function getTitle(){

        if(location.pathname.indexOf('/checkout/index.jsp') > -1){

            var progressParam = 'null';

            if(progressParam == 'address'){
                if(typeof addressMode == undefined){
                    return "address";
                }
                else if(addressMode == "Shipping Info"){
                    return 'edit shipping address';
                }
                else {
                    return 'edit billing address';
                }
            }
            else if(progressParam == 'login'){
                return 'login';
            }
            else if(progressParam == 'modUserEmail'){
                return 'edit user email';
            }
            else if(progressParam == 'modUserPass'){
                return 'edit user passsword';
            }
            else if(progressParam == 'orderTracking'){
                return 'track your order';
            }
            else if(progressParam == 'pastPurchases'){
                return 'past purchases';
            }
            else if(progressParam == 'updateEmailPref'){
                return 'email preferences';
            }
            else if(progressParam == 'orderTrackingLogin'){
                return 'order history - login';
            }
            else if(progressParam == 'orderTrackingDetail'){
                return 'order details';
            }
        }
        else if(location.pathname.indexOf('/cart/index.jsp') > -1){
            var favFragrances = document.querySelectorAll('div.favorite-fragrances');
            if(favFragrances != null && favFragrances.length > 0){
                return "my fave fragrances";
            }
        }

        var title = document.getElementsByTagName("title");

        if(title == null){
            return '';
        }

        title = title[0].text.replace('Bath & Body Works', '').trim();

        if(title.startsWith('-')){
            title = title.replaceAt(0, ' ').trim();
        }

        if(title.endsWith('-')){
            title = title.replaceAt(title.length - 1, ' ').trim();
        }

        if(title.startsWith(':')){
            title = title.replaceAt(0, ' ').trim();
        }

        if(title.endsWith(':')){
            title = title.replaceAt(title.length - 1, ' ').trim();
        }

        return title;
    }

    var body = document.getElementsByTagName("body")[0];

    var pageName = "";
    var pageType = "";
    var searchTerm = "null";
    var searchItems = null;
    var isTealiumPage = false;


    if(elementHasClass(body, 'family')){
        pageType = "family Page";
        isTealiumPage = true;
    }
    else if(elementHasClass(body, 'category')){
        pageType = "category Page";
        isTealiumPage = true;
        attributes.page_product_category = "" ;
    }
    else if(elementHasClass(body, 'product')){
        pageType = "product detail";
        isTealiumPage = true;
        var productId = "";
        if(productId != ""){
            attributes.product_id = [productId.toLowerCase()];
        }
        var productBrand = "";
        if(productBrand != ""){
            attributes.product_brand = [productBrand.toLowerCase()];
        }
        var productSku = "";
        if(productSku != ""){
            attributes.product_sku = [productSku.toLowerCase()];
        }
        var color = " ";
        var title = "";
        if(color != "" || title != ""){
            attributes.product_name = [(color + title).toLowerCase()];
        }
        attributes.product_quantity = ["1"];
    }
    else if(elementHasClass(body, 'myAccount')){
        pageType = "account page";
        isTealiumPage = true;
    }
    else if(elementHasClass(body, 'storeLocator')){
        pageType = "store locator page";
        isTealiumPage = true;
    }
    else if(elementHasClass(body, 'help')){
        pageType = "helpdesk page";
        isTealiumPage = true;
    }
    else if(elementHasClass(body, 'checkout')){
        pageType = "checkout page";
        isTealiumPage = true;
        var checkout_step = "";

        attributes.checkout_step =  checkout_step;

        
        if(!isEmptyArray(productIds))
            attributes.product_id = productIds;
        if(!isEmptyArray(productNames))
            attributes.product_name = productNames;
        if(!isEmptyArray(quantities))
            attributes.product_quantity = quantities;
        if(!isEmptyArray(skuIds))
            attributes.product_sku = skuIds;
        if(!isEmptyArray(brandIds))
            attributes.product_brand = brandIds;
        if(!isEmptyArray(prices))
            attributes.product_price = prices;
        if(!isEmptyArray(discounts))
            attributes.product_discount = discounts;
    }
    if(searchTerm != null && searchTerm != '' && searchTerm != "null" ){
        pageType = "search page";
        isTealiumPage = true;
        searchItems = "";

        attributes.search_results = searchItems;
        attributes.search_term = searchTerm;
    }

    if(!isEmptyArray(productVariants)){
        attributes.cart_product_variant = productVariants;
    }

    if(!isEmptyArray(productVariantsSkus)){
        attributes.cart_product_sku = productVariantsSkus;
    }

</script>


    
	





