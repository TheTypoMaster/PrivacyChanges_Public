		this.bbwLargeMediumCollectionPage = function() { this.execute("_bbwLargeMediumCollectionPage"); this.createBBWCoremetricsCookie();};
		this._bbwLargeMediumCollectionPage=function()
    {
        return "cmCreatePageviewTag("
             +      "\'Medium to Large Collection:" + this.categoryName + "(" + this.categoryId + ")\', "
             +      "null, this.getBaseCategoryId(), null,null,"
			 +       "this.optData['familyAttribValue']);";

	}; 
	  this.searchResults = function() { this.execute("_searchResults"); this.createBBWCoremetricsCookie();};
	  this._searchResults = function()
    {
        return "cmCreatePageviewTag("
             +      "'SEARCH RESULTS: PAGE ' + searchData.searchPage, "
             +      "searchData.searchTerm, "
             +      "searchData.searchType, "
             +      "searchData.resultCount,null,"
			 +		"this.optData['searchAttribValue']);";
    };
        this.bbwThematicPage = function() { this.execute("_bbwThematicPage"); this.createBBWCoremetricsCookie();};
        this._bbwThematicPage = function()
    {
        return "cmCreatePageviewTag("
             +      "'BloomReach: ' + searchData.searchTerm, "
             +      "null, this.categoryName, null,null,"
             +       "this.optData['familyAttribValue']);";
    };
};

      </script>
