  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-17738953-1']);
  _gaq.push(['_trackPageview']);

  (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();

  // track coda-slider navigation
  function locationHashChanged() {
    var wl = window.location, uri = wl.pathname + wl.hash;
    _gaq.push(['_trackPageview', uri]);
    clicky.log(uri, "Home nav", "pageview");
  }
  window.onhashchange = locationHashChanged;

  // track outbound links in the following format:
  // http://foo.com/bar goes to: /outbound/foo.com/bar
  $(document).delegate("a[href*='http://']:not([href*='"+document.domain+"'])",'click', function(e) {
    _gaq.push(['_trackPageview', '/outbound/'+jQuery(this).attr('href').replace(/^https?:\/\//, "")]);
  });
</script>

