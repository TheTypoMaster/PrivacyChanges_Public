    pageTrackObj = {
    	invokePageTrackCall : function(){
            
			if(typeof(_satellite) === "object" && typeof(_satellite.track) === "function"){ 
               _satellite.track("pageview"); 
            }
		},
		updatePageTrackObj : function(){
			if(typeof(_satellite) === "object" && typeof(_satellite.track) === "function"){ 
               _satellite.track("change"); 
			} 

		}
