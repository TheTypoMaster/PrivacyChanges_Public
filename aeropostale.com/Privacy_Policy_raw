<script language="javascript">
function createCookie(name,value) {
	var date = new Date();
	date.setTime(date.getTime()+(30*24*60*60));
	var expires = "; expires="+date.toGMTString();
	document.cookie = name+"="+value+expires+"; path=/";
}
</script>



