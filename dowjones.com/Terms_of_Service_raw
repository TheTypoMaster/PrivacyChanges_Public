<script type="text/javascript">
	jQuery(document).ready(function(){
	
		//raj
		//$(".relatedItem").hover(function() {
		//$(this).attr("src","http://images.dowjones.com/company/wp-content/uploads/sites/15/2014/11/button_play_blue.png");
		//}, function() {
		//$(this).attr("src","http://images.dowjones.com/company/wp-content/uploads/sites/15/2014/11/button_play_blue.png");
		//});
		
		//end raj
		
		if(typeof(setCurrentItem) != 'undefined') {
		jQuery('.slimscroll > a.relatedItem').on('click', setCurrentItem);

		}
		jQuery('img.video-vimeo').each(function(i, e) {
			jQuery(e).css('visibility', 'hidden');
			jQuery.ajax({
				'url': jQuery(e).attr('src'),
				'dataType': 'jsonp',
				'success': function(data) {

					if(typeof(data) != 'object') {
						data = jQuery.parseJSON(data);
					}

					if(data.length) {
						e.src = data[0].thumbnail_medium;
						e.onload = function() { jQuery(e).removeAttr('style'); }
					}
				}
			});
		});
	});
</script>


