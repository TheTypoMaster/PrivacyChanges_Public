<div id="static">
	<script type="text/javascript">


var galleryActiveType = null;

function setCurrentItem(e) {
	jQuery('#current-gallery').css('overflow', 'hidden');
	
	var $currentHolder, $newHolder, isAlreadyActive = false;
	
	if(galleryActiveType == null) {
		galleryActiveType = (jQuery('#current-gallery').find('iframe').css('display') == 'none') ? 'image' : 'video';
	}
	
	$currentHolder = (galleryActiveType == 'video') ? jQuery('#current-gallery').find('iframe') : jQuery('#current-gallery').find('img');
	$currentHolder.css('display', 'none');

	if(galleryActiveType == 'image' || galleryActiveType == 'video') {
		$currentHolderCaption = jQuery('#caption');
		$currentHolderCaption.css('display', 'none');
	}

	// ABOVE - Stuff Dealing with Current Gallery Object
	
	var $relatedItem = jQuery(e.target);
	
	if($relatedItem.hasClass('video') || $relatedItem.hasClass('play_button') ) {
		console.log("in if for videos");
		$newHolder = jQuery('#current-gallery').find('iframe');
		if(galleryActiveType == 'video') {
			isAlreadyActive = true;
		}
		galleryActiveType = 'video';
	
	}
	else {
		console.log("in else for imgs");
		$newHolder = jQuery('#current-gallery').find('img');
		if(galleryActiveType == 'image') {
			isAlreadyActive = true;
		}
		galleryActiveType = 'image';

	}
	//for caption
	$newHolderCaption = jQuery('#caption');
	$newHolderCaption.css('display', 'block');

	$newHolder.css('display', 'block');

	if(!isAlreadyActive) {
		$newHolder.attr('src', 		$currentHolder.attr('src'));
		$newHolder.attr('data-img', $currentHolder.attr('data-img'));
		$newHolder.attr('class', 	$currentHolder.attr('class'));
		$newHolder.attr('alt', 		$currentHolder.attr('alt'));
		$newHolder.attr('vid_cap', 		$currentHolder.attr('vid_cap'));
		$currentHolder.removeAttr('src data-img class alt vid_cap');
	}


	var currentVideoSrc =  $newHolder.attr('src'),
		currentImgSrc = $newHolder.attr('data-img'),
		currentClass = $newHolder.attr('class'),
		currentAlt= $newHolder.attr('alt'),
		currentVidCap = $newHolder.attr('vid_cap'),
		relatedVideoSrc = $relatedItem.attr('data-img'),
		relatedImgSrc = $relatedItem.attr('src'),
		relatedClass = $relatedItem.attr('class');
		relatedAlt= $relatedItem.attr('alt');
		relatedVidCap= $relatedItem.attr('vid_cap');


	$newHolder.attr('src', relatedVideoSrc).attr('data-img', relatedImgSrc).attr('class', relatedClass).attr('alt', relatedAlt).attr('vid_cap', relatedVidCap);
	$relatedItem.attr('src', currentImgSrc).attr('data-img', currentVideoSrc).attr('class', currentClass).attr('alt', currentAlt).attr('vid_cap', currentVidCap);


   if( (relatedAlt && galleryActiveType == 'image') || (relatedVidCap && galleryActiveType == 'video') ) {
		jQuery('#caption_holder').css('visibility', 'visible');
	}
	else {
		jQuery('#caption_holder').css('visibility', 'hidden');
	}

	if(galleryActiveType == 'image') {
		$newHolderCaption.text(relatedAlt);
	}
	else {
		$newHolderCaption.text(relatedVidCap);
	}
		

}
</script>	


