    <script type="text/javascript">
        var logAssetStart = (new Date()).getTime() * 1000;
        function logAssetError(e) {
            var referer = '',
                location = '',
                type = 'unknown',
                url = 'unknown',
                data = '',
                message = 'Asset loading error';
            try {
                referer = document.referer;
            } catch (err) {}
            try {
                location = document.location.href;
            } catch (err) {}
            try {
                type = e.type;
                url = (e.target.src || e.target.href);
                message = 'Error on '
                    + e.target.tagName + ' asset ' + url
                    + ' after ' + (e.timeStamp - logAssetStart) + 'µs'
                    ;
            } catch (err) {}
            dataobj = {
                type: type,
                message: message,
                url: url,
                location: location,
                referer: referer,
                linenumber: 'unknown',
                app: 'www',
                language: 'en_US'
            }
            for (i in dataobj)
                data += i + '=' + encodeURIComponent(dataobj[i]) + '&';
            try {
                var xhr = new XMLHttpRequest();
                    xhr.open("POST", "/report/message", true);
                    xhr.setRequestHeader('Content-type', 'application/x-www-form-urlencoded');
                    xhr.send(data);
            } catch (err) {}
        }
    </script>
            
        
    
    
