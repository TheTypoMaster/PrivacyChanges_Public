<!-- Web tracking implementation -->


  <script> 

  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

  /* To enable cross domain linking, we must have an 
     array of all other related wolframalpha.com domains
     which can refer a user to this page. This prevents
     sessions from ending prematurely */ 
  function getDomainArray () {
    var currentDomain = window.location.hostname;
    var domains = ['m.wolframalpha.com',
      'www.wolframalpha.com', 'products.wolframalpha.com',
      'developer.wolframalpha.com'];
    var index = domains.indexOf(currentDomain);
    if (index > -1) {
      domains.splice(index, 1);
    }
    return domains;
  }

  /* Just incase things go wrong */ 
  ga('create', 'UA-5677369-16', 'auto', {
    'name': 'homepage',
    'allowLinker': true
  });

  ga('homepage.require', 'linker');
  ga('homepage.linker:autoLink', getDomainArray());
  ga('homepage.send', 'pageview');

  /* These three variables can be set from the global scope
     before this script is included on a page if you would like 
     to define more dimensions, send the tracker to a different
     property than the default 'UA-5677369-3', or to pass in
     alternative options for the creation of the tracker. By
     default, the tracker will be created with allowLinker: true
     and no other creation options */
  var waGoogleAnalytics_dims = waGoogleAnalytics_dims || null; 
  var waGoogleAnalytics_prop = waGoogleAnalytics_prop || null;
  var waGoogleAnalytics_createOpts = waGoogleAnalytics_createOpts || null; 
 
  var waGoogleAnalytics = (function (dims, property, createOpts) {

    /* This function will simply return the value stored for a cookie */ 
    function getCookie(cookieName) {
      var cookieString = RegExp("" + cookieName + "[^;]+").exec(document.cookie);
      return unescape(!!cookieString ? cookieString.toString().replace(/^[^=]+./, "") : "");
    }

    /* This function will take the parsed JSON from the request
       to user.json.jsp and attempt to determine the users pro
       level based on the value of various properties */ 
    function getProLevel(json) {
       if (json.needsupgrade == true) {
         return "ProTrial";
       } else if (json.hasstudentsubscription == true) {
         return "ProStudent"; 
       } else if (json.hassubscription == true) {
         return "ProFull";
       } else if (json.hasCancelled == true) {
         return "Cancelled";
       } else if (json.email != "") {
         return "Free";
       } else {
         return "None"; 
       }  
    }

    /* Begin default tracker */ 
    ga('create', 'UA-5677369-3', 'auto', {
      'allowLinker': true
    });

    ga('require', 'linker');
    ga('linker:autoLink', getDomainArray());
   
    var sessionID = getCookie('wa_ga_session');
    var date = new Date();
    var minutes = 30;
    date.setTime(date.getTime() + (minutes * 60 * 1000));

    if (sessionID == "") {
      var id = Math.floor(Math.random() * 1e20);
      document.cookie = "wa_ga_session=" + id + "; expires=" + date.toGMTString() + "; domain=.wolframalpha.com; path=/";
      sessionID = id;
    } else {
      document.cookie = "wa_ga_session=" + sessionID + "; expires=" + date.toGMTString() + "; domain=.wolframalpha.com; path=/";
    }
    
    ga('set', 'dimension2', "" + sessionID);
    var userID = getCookie('wa_pro_u') || 'unauthenticated';
    ga('set', '&uid', "" + userID);  
    ga('set', 'dimension3', "" + userID); 

    ga('send', 'pageview');   
    /* End default tracker */

    /* Send proLevel as separate event */
    var proLevel;

    var getJson = $.ajax({
      url: "//www.wolframalpha.com/input/user.json.jsp",
      xhrFields: { withCredentials: true } 
    });

    getJson.done(function(json) {
      proLevel = getProLevel(JSON.parse(json));
      var proLevel_cookie;

      if (proLevel == 'None') {
        proLevel_cookie = getCookie('wa_ga_pl'); 
        if (proLevel_cookie != '') {
          proLevel = proLevel_cookie;
        }
      } else {
        document.cookie = "wa_ga_pl=" + proLevel + "; max-age=" + (60 * 60 * 24 * 365 * 10) + 
          "; domain=.wolframalpha.com; path=/";
      }
      ga('send', 'event', 'proLevel', proLevel, {'nonInteraction': 1}); 
    });

    getJson.error(function () {
      proLevel = 'error';
      var proLevel_cookie = getCookie('wa_ga_pl');
    
      if (proLevel_cookie != '') {
        proLevel = proLevel_cookie;
      }
      ga('send', 'event', 'proLevel', proLevel, {'nonInteraction': 1}); 
    });
  })(waGoogleAnalytics_dims, waGoogleAnalytics_prop, waGoogleAnalytics_createOpts);

</script>    

