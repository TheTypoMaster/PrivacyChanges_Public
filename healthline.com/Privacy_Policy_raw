<script>

  require([
  'jquery'
  ], function( $ ){

    // activate toggle for hide/show FB comments
    $(window).load(function() {

      if( $('.fb-view-comments-bar').length ) {
        $('.fb-view-comments-bar').click(function() {
          $('.fb-view-comments-bar').toggleClass( 'open' );
          if( $('.fb-view-comments-bar').hasClass( 'open' ) ) {
            $('.fb-view-comments-bar span.fb-view-comments-arrow').removeClass( "hl-icon-right-solid-arrow" ).addClass('hl-icon-down-arrow');
            $('#fb-view-count').hide();
          } else {
            $('.fb-view-comments-bar span.fb-view-comments-arrow').removeClass( "hl-icon-down-arrow" ).addClass('hl-icon-right-solid-arrow');
            $('#fb-view-count').show();
          }
          if($('#fb-view-count').html() == '&nbsp;()') $('#fb-view-count').hide();
        });
      }

    });
  });

</script>
