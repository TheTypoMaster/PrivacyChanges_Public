<script>


        require([
          'jquery'
        ], function( $ ){

          arrayPromiseFunctions.push( function() {
            var defer = $.Deferred();

              googletag.cmd.push( function() {
                require([
                  'jquery',
                  'base/responsive-ads',
                  'base/util/ga-custom-vars',
                  'matchmedia'
                ], function( $, AppResponsiveAds ){
                  // default - show LB ad on page load
                  var isAdOnPageLoad = true;

                  
                    var breakpointsToHideAd = [];
                    var breakpointsToHideAdByPageType = [];

                    
                      breakpointsToHideAdByPageType = ['997','768','600'];
                    

                    isAdOnPageLoad =  AppResponsiveAds.getShowAdOnPageLoad("LB",breakpointsToHideAd,breakpointsToHideAdByPageType);
                  

                  //setting up ad targeting before resolove or reject a defer
                  window.HL_lbSlot = googletag.defineSlot(window.slotName,[ [768, 90], [728, 90], [320, 50], [300, 50] ],'gpt-ad-lb' )
                    .addService( googletag.pubads() );
                  window.HL_lbSlot.setTargeting( 'pos', 'lb' );
                  googletag.display( 'gpt-ad-lb' );

                  
                    if( matchMedia( 'only screen and (min-width: 1240px)' ).matches && $( '.hl-ad-wsl' ).length && $( '.ad-lb' ).hasClass( 'hidden-ad-lb' ) ) {
                      defer.reject();
                      return;
                    }
                  

                  var isHealthline = true,
                      isSlideShow = false;
                  if( !isSlideShow && matchMedia( 'only screen and (max-width: 599px)').matches && isHealthline ) {
                    window.HL_lbSlot.setCollapseEmptyDiv(true, true);
                    defer.reject();
                  } else {
                    if ( isAdOnPageLoad ){
                      log.debug( 'GPT Promise - LB expected' );
                      defer.resolve( { slotObj: HL_lbSlot, slotName: "LB" });
                    } else {
                      log.debug( 'GPT Promise - LB expected' );
                      defer.resolve( false );
                    }
                  }
                });
              });  // googletag.cmd.push

            return defer.promise();
          });

        }); // require

        
</script>
<script>

        require([
          'jquery'
        ], function( $ ){

          arrayPromiseFunctions.push( function() {
            var defer = $.Deferred();

            googletag.cmd.push( function() {
              require([
              'jquery',
              'base/responsive-ads',
              'base/util/ga-custom-vars',
              'matchmedia'
              ], function( $, AppResponsiveAds ){

                  // default - show LB ad on page load
                  var isAdOnPageLoad = true;

                  
                    var breakpointsToHideAd = [];
                    var breakpointsToHideAdByPageType = [];

                    
                      breakpointsToHideAdByPageType = ['997','768','600'];
                    

                    isAdOnPageLoad =  AppResponsiveAds.getShowAdOnPageLoad("LB",breakpointsToHideAd,breakpointsToHideAdByPageType);
                  

                    //setting up ad targeting before resolove or reject a defer
                    window.HL_lb3Slot = googletag.defineSlot(window.slotName,[ [320, 50], [300, 50] ],'gpt-ad-lb3' )

                      .addService( googletag.pubads() );
                    window.HL_lb3Slot.setTargeting( 'pos', 'lb3' );
                    googletag.display( 'gpt-ad-lb3' );

                  

                  var isHealthline = true;
                  if( matchMedia( 'only screen and (max-width: 599px)' ).matches && isHealthline ) {
                    var originalLeaderboard = $('.ad-lb');
                    googletag.pubads().addEventListener('slotRenderEnded', function(event) {
                      if (event.slot === window.HL_lb3Slot ) {
                        if( event.isEmpty ) {
                          googletag.cmd.push( function(){
                            if( window.HL_lbSlot ) {
                              window.HL_gptRefreshAds( ['mr1','mr2','mr3','wsl','wsr','pb'] );
                            }
                          });
                        } else {
                          try {
                            HL_ga_sendTrackEvent({
                              gaEventCategory : 'Any Page',
                              gaEventAction : 'Key Impression',
                              gaEventLabel: "LB3: hn.us.hl.lc.x.x.x",
                              gaEventNonInteraction: true});
                          } catch (e) {
                          // if an undefined value in array exists, do no log in GA
                          // this does not affect ad refresh
                          }
                        }

                      } else if( event.slot === window.HL_lbSlot && !event.isEmpty ) {
                        originalLeaderboard.css({height: 79, visibility: 'visible', overflow: 'hidden' });
                      }
                    });
                    if( isAdOnPageLoad ) {
                      log.debug( 'GPT Promise - LB3 expected' );
                      defer.resolve( { slotObj: HL_lb3Slot } );
                    } else {
                      defer.reject();
                    }
                  } else {
                    defer.reject();
                  }
              }); // require
            }); // googletag.cmd.push

            return defer.promise()
          });

        }); // require
      
</script>
