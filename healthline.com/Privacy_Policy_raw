        <header class="hl-title-area">
          <div itemprop="headline" class="article-header">
              <h1>Privacy Policy</h1>
            </div>
          <div class="hl-widget-sharebar c"></div>

  <div class="sharebarContainer">
  <a id="sharebarLink" class="link-sharebar" href="#overlaySharebar" rel="#overlaySharebar"></a>
