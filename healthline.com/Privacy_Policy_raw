<script type="text/javascript">
  var fireOnce = false;
  require(['base/async'], function( Async ) {

    var waitForSignal = false;

    function GPTRefreshAllAds() {
      // KIMPS
      for( var i = 0; i < adsArrayForKIMP.length; i++) {
        if( window.HL_ga_sendTrackEvent && adsArrayForKIMP[i] ) {
          try {
            HL_ga_sendTrackEvent({
              gaEventCategory : 'Any Page',
              gaEventAction : 'Key Impression',
              gaEventLabel: adsArrayForKIMP[i] + ": hn.us.hl.lc.x.x.x",
              gaEventNonInteraction: true
            });

            log.debug( 'GPT Common - KIMP called: ' + adsArrayForKIMP[i] + ': hn.us.hl.lc.x.x.x' );
          } catch (e) {
            // if an undefined value in array exists, do no log in GA
            // this does not affect ad refresh
          }
        }
      }
      // THIS IS THE ONE CALL FOR ENTIRE SITE
      googletag.cmd.push( function() {
        googletag.pubads().refresh( adsArrayToRefresh );
      });
    }

    Async( arrayPromiseFunctions, function( PromiseFunction, callback) {
      PromiseFunction().then( function( dataObj ){
        if( dataObj ){
          adsArrayToRefresh.push( dataObj.slotObj );
          adsArrayForKIMP.push( dataObj.slotName );
        } else {
          waitForSignal = true;
        }
        callback();
      }, function() {
        callback();
      });
    }, function(){
      if ( waitForSignal ) {
        $( document ).on( 'wslAdNotShow', function() {

          log.debug( 'GPT Common - wslAdNotShow triggered' );
          if( !fireOnce ) {
            GPTRefreshAllAds();
            fireOnce = true;
          }
        });
      } else {
        GPTRefreshAllAds();
      }
    });
  });
</script><script type='text/javascript'>

  // add HL_chartBeat to global space
  window.HL_chartBeat = window.HL_chartBeat || { sections: null, authors: null };

  var _sf_async_config={};
  /** CONFIGURATION START **/
  _sf_async_config.uid = 52356;
  _sf_async_config.domain = 'healthline.com';
  _sf_async_config.useCanonical = true;
  _sf_async_config.sections = HL_chartBeat.sections;  //CHANGE THIS
  _sf_async_config.authors = HL_chartBeat.authors;    //CHANGE THIS
  /** CONFIGURATION END **/
  (function(){
    function loadChartbeat() {
      window._sf_endpt=(new Date()).getTime();
      var e = document.createElement('script');
      e.setAttribute('language', 'javascript');
      e.setAttribute('type', 'text/javascript');
      e.setAttribute('src', '//static.chartbeat.com/js/chartbeat.js');
      document.body.appendChild(e);
    }
    var oldonload = window.onload;
    window.onload = (typeof window.onload != 'function') ?
      loadChartbeat : function() { oldonload(); loadChartbeat(); };
  })();
</script></body>
