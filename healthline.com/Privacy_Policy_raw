<script>


            arrayPromiseFunctions.push( function() {
              var defer = $.Deferred();

              require([ "jquery", "base/responsive-ads", "matchmedia"], function($,AppResponsiveAds){

                // determine if mr3 ad is shown on page load
                var isAdOnPageLoad = false;

                // load MR3 on new slideshow design by Default on mobile
                if ( matchMedia('only screen and (max-width: 997px)').matches && false ) {
                  isAdOnPageLoad = true;
                }

                if ( matchMedia('only screen and (min-width: 768px)').matches && matchMedia('only screen and (max-width: 997px)').matches &&
                  !false && false ) {
                  isAdOnPageLoad = false;
                }



                if( typeof window.HL_mr3Slot10 !== 'undefined' ) {
                  defer.reject();
                  return;
                }

                googletag.cmd.push(function() {
                  // need to create an MR3 slot object/array and dynmaically add individual slots
                  window.HL_mr3Slots = window.HL_mr3Slots ? window.HL_mr3Slots : [];
                  window.HL_mr3Slot10 = googletag.defineSlot(slotName, [[300, 250]],"gpt-ad-mr3-10")
                  .addService(googletag.pubads());

                  window.HL_mr3Slot10.setTargeting("pos", "mr3");
                  googletag.display("gpt-ad-mr3-10");

                  // test this
                  window.HL_mr3Slots.push( window.HL_mr3Slot10 );

                  if( isAdOnPageLoad ) {
                    log.debug( 'GPT Promise - MR3 expected' );
                    defer.resolve( { slotObj: HL_mr3Slot10, slotName: "MR3" } );
                  } else {
                    defer.reject( );
                  }
                });

              }); // require

              return defer.promise();
            });
          
</script>
