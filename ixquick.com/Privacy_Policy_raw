        jQuery(this).find('*').removeAttr('bgcolor color face style');
    });
});

function verifyChecked (form_obj) { 
    var formname = form_obj.name; 
    var orig_query = document[formname].query.value; 
    if (orig_query == "") { 
        return false; 
    } 
    form_obj.submit(); 

    return false; 
}
      
