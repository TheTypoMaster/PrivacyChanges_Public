   	jQuery(window).load(function() {
		jQuery('.accordionButton').click(function(e){
			e.preventDefault();
            var div_display = jQuery(this).next('.accordionContent').css('display');
            if (div_display == 'block') {
                jQuery(this).next('.accordionContent').slideUp();
            } else {
                jQuery('div.accordionContent').slideUp();
                jQuery(this).next('.accordionContent').slideToggle();
            }
    	});
    });

