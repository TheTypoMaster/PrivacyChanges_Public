jQuery('.external').click(function(){
        var link = jQuery(this).attr('data-link');
        var str;
        if(link.match(/facebook/i)) {
           str = "By clicking this, you will leave Ixquick.com and go to Facebook. Do you want to proceed?";
        } else if(link.match(/twitter/i)) {
           str = "By clicking this, you will leave Ixquick.com and go to Twitter. Do you want to proceed?";
        }

        r = confirm(str);
        if(r==true) { 
            document.location = link;
        } 
        return false;
    });
jQuery(window).load(function () {
    var page_url = document.URL;
    if( page_url.match("company-faq") || 
        page_url.match("protect-privacy-qa") || 
        page_url.match("preferences.pl") ) {
        if (page_url.match(/.html#q/) || page_url.match("jump_to")) {
        } else { 
          document.getElementById('query_top').focus();
        }
    } else { 
        document.getElementById('query_top').focus();
    }
});

