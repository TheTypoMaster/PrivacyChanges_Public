		function concatParamToAllUrls (referenceUrlId) {
			/* Get current url */
			var currentUrl = window.location.href;
			currentUrl = unescape(currentUrl);
			var isParamPresentInFormField = false; /* flag to check if prf OR prfh param is present in form field */
			var isParamPresentInUrl = false;  /* flag to check if prf OR prfh param is present in url */
            var isValidParam = false;  /*flag to check if prf OR prfh parameter is valid or not.*/
            var alreadyHiddenPrfBlankPresent = false;
			var paramName = "";
			var paramValue = "";

			/* Loop through each input type element and check if prf OR prfh is present or not. If yes mark flag as true */
			jQuery(":input").each(function(i){
					
				if( (jQuery(this).attr("name") == "prfh") || (jQuery(this).attr("name") == "prf") ){
					//isParamPresentInFormField = true;
					paramName  = jQuery(this).attr("name") + "=";
					paramValue = jQuery(this).attr("value");
                    if (paramValue != "") {
                         isParamPresentInFormField = true;
                    } else {
                       alreadyHiddenPrfBlankPresent = true;

                    }
				}
				
			});

			/* Is prf OR prfh present in Url. If yes then set flag as true */
			if( (currentUrl.match(/(.*?)(&|\?)(prfh)(=)(.*?)($|&)/g)) || (currentUrl.match(/(.*?)(&|\?)(prf)(=)(.*?)($|&)/g)) ){
				isParamPresentInUrl = true;
			}

            var param_name = RegExp.$3;
            var parm_opt = RegExp.$4;
            var prm_value = RegExp.$5;
            if (/^[a-zA-Z0-9/_]+$/.test(prm_value)) {
                isValidParam = true;
            }

			//alert('isParamPresentInFormField ' + isParamPresentInFormField + ' isParamPresentInUrl ' + isParamPresentInUrl);
			if( isValidParam && ((isParamPresentInUrl) || (isParamPresentInFormField)) ){
				var concatThisParam = "";

				/* Prepare param to be concatenated */
				if (isParamPresentInUrl) {
					concatThisParam = param_name + parm_opt + prm_value;
				} else if(isParamPresentInFormField){
					concatThisParam = paramName + paramValue ;
					
				}

				/* If advance search url is calling this method then concat prf OR prfh param to this url only. Else concat to all the urls */
				if (typeof referenceUrlId != "undefined") {
					jQuery('#' + referenceUrlId).attr('href', jQuery('#' + referenceUrlId).attr('href') + '&' + concatThisParam);
					
				} else {
				   /* Add prf OR prfh as form field as well if not already present. */
				   if (!isParamPresentInFormField) {
                       if (alreadyHiddenPrfBlankPresent) {
                           var myParam = concatThisParam;
                           var param_elements = myParam.split('=');
                          jQuery('input[name='+param_elements[0]+']').val(param_elements[1]);
                       } else {
					      jQuery("<input type='hidden' value='" + prm_value + "' name='" + param_name + "' id='" + param_name + "' />").prependTo(jQuery('#input_p'));
                       }
                   }
				   jQuery('.addsettings').each(function(index){ 
					   var originalUrl = jQuery(this).attr('href');
                       if (originalUrl) {
					   if ( originalUrl.match( /(.*?)(\?)(.*?)$/g ) ){
						   jQuery(this).attr('href', originalUrl + '&' + concatThisParam);
					   } else if( originalUrl.match( /(.*?)(\/)$/g ) ){
						   jQuery(this).attr('href', originalUrl + '?' + concatThisParam);
					   } else if(originalUrl.match( /(.*)(\/)(.*?)$/g ) ){
						   jQuery(this).attr('href', originalUrl + '?' + concatThisParam);
					   }
                       }
					   
				   });
			   }
			}
		} function eId(item) { 
        return document.getElementById(item);                                                                                                                                                                    
    }                                                                                                                                                                                                          
    var allCats = new Array('web','pics','video');                                                                                                                                          
                                                                                                                                                                                                               
    function hasClass(ele,cls) {                                                                                                                                                                                
        return ele.className.match(new RegExp('(\\s|^)'+cls+'(\\s|$)'));                                                                                                                             
    }                                                                                                                                                                                       
                                                                                                                                                                                                     
    function addClass(ele,cls) {                                                                                                                                                            
        if (!this.hasClass(ele,cls)) {                                                                                                                                                       
            ele.className += ' ' + cls;                                                                                                                                                   
        }                                                                                                                                                                                           
    }                                                                                                                                                                                            
                                                                                                                                                                                                    
    function removeClass(ele,cls) {                                                                                                                                               
        if (hasClass(ele,cls)) {                                                                                                                                                          
            var reg = new RegExp('(\\s|^)'+cls+'(\\s|$)');                                                                                                                                   
            ele.className=ele.className.replace(reg," ");                                                                                                                                       
        }                                                                                                                                                                                       
    }                                                                                                                                                                                              
                                                                                                                                                                                                         
    function passQueryAdv(url) {                                                                                                                                                                                 
        url += '?&cat=' + document.blah1.cat.value;                                                                                                                                                              
        url += '&query=' + escape(document.blah1.query.value);
        eId('xsearch').href = url; concatParamToAllUrls('xsearch');
    }
    
    function setCat(catr) { 
        document.blah1.cat.value = catr;
        for(var i = 0; i < allCats.length; i++) {
            removeClass(eId(allCats[i]), 'active');
        }
        addClass(eId(catr), 'active');
        eId('query_top').focus();
        if (catr != 'web') {
            //eId('enhanced').style.visibility = 'hidden';
            //eId('detail').style.visibility = 'hidden';
        } else {
            //eId('enhanced').style.visibility = 'visible';
            //eId('detail').style.visibility = 'visible';
        }
        return false;
    }
 
    function trim(str) {
       return str.replace(/^\s+|\s+$/g, '');
    }

    function get_value(c_name) {
       var current_url = location.href;
      if ( (current_url.indexOf('prfh') != -1) && (current_url.indexOf(c_name) != -1) ) {
           c_start= current_url.indexOf(c_name + "EEE");
           if (c_start!=-1)
              {
                c_start=c_start + c_name.length+3;
                c_end=current_url.indexOf("N1N",c_start);
                if (c_end==-1) c_end = current_url.length;
                return unescape(current_url.substring(c_start,c_end));
              }
         } else {
            if (document.cookie.length>0)
               {
                 c_start=document.cookie.indexOf(c_name + "EEE");
                 if (c_start!=-1)
                    {
                      c_start=c_start + c_name.length+3;
                      c_end=document.cookie.indexOf("N1N",c_start);
                      if (c_end==-1) c_end=document.cookie.length;
                      return unescape(document.cookie.substring(c_start,c_end));
                     }
                  }

         }
       return "";
     }

    function change_form_method() {
       if(get_value('enable_post_method') == '0') {
          document.blah1.method = 'GET';
       }
    }

    function verifyChecked (form_obj) {
        var formname = form_obj.name;
        var orig_query = document[formname].query.value; 
        if (orig_query == '') {
           return false;
        }
        form_obj.submit();
        return false;
    } 


        $(document).ready(function() {

            if((BrowserDetect.OS.indexOf("BB10") != -1) && (navigator.userAgent.indexOf("Mobile Safari") != -1)) MY_BROWSER = "Browser";
            if ( ((MY_BROWSER == 'Chrome') || (MY_BROWSER == 'Chromium')) && (MY_BROWSER_VERSION > 31) ) {

            actionUrl = document.blah1.action;
            document.blah1.action = actionUrl.replace(/\/search$/, '/metasearch.pl'); }
 });
        jQuery(window).load(function(){
            var additional_parm = '';
        
            var pref_args = prepare_propagation_arguments();
            additional_parm = pref_args;
            
            cpt_js_prgtr(additional_parm);
        });
        function cpt_js_prgtr(additional_parm) {
            var urlParams;
            (window.onpopstate = function () {
                var match,
                pl     = /\+/g,
                search = /([^&=]+)=?([^&]*)/g,
                decode = function (s) { try {return decodeURIComponent(s.replace(pl, " "));} catch(e){} },
                query  = window.location.search.substring(1);

                urlParams = {};
                while (match = search.exec(query)) {
                    urlParams[decode(match[1])] = decode(match[2]);
                }
            })();
            var params = new Array ('vf', 'vft', 'fm', 'fmt', 'op');
    
            params[params.length] = 'prf';
            params[params.length] = 'prfh';
        
            var cpt_prms = additional_parm;
            var cpt_h = '';
            var cpt_prms_no_hmb = '';
            jQuery.each(params, function(index, elem) {
                if (urlParams[elem] != undefined && /^[a-zA-Z0-9]+$/.test(urlParams[elem])) {
                    cpt_h += '<input value="'+urlParams[elem]+'" name="'+elem+'" type="hidden">';
                    if (cpt_prms === '') {
                        cpt_prms += elem+'='+urlParams[elem]+'&';
                    } else {
                        cpt_prms += '&'+elem+'='+urlParams[elem]+'&';
                    }   
                    if(elem != 'hmb') {
                        cpt_prms_no_hmb += elem+'='+urlParams[elem]+'&';
                    }
                }
            });
            
        cpt_prms +="hmb=1";
        cpt_h += '<input value="1" name="hmb" type="hidden">';
        
            if (cpt_prms) {
                if (jQuery('.cpt_prgtr').length) {
                    jQuery('.cpt_prgtr').each(function() {
                        var search_box_params = jQuery(this).html();
                        var pttrn = new RegExp('name=('+params.join('|')+')');
                        if(!(pttrn.test(search_box_params))) {
                            jQuery(this).prepend(cpt_h);
                        }
                    });
                }
            }           
          
            var base_url = "http://ixquick.com";
            base_url = base_url.replace('<!--KEEPTEL-->','');
            if (base_url != '') {
                base_url = base_url.match(/^https?:\/\/[^/]+/)[0];
            }
            base_url_regexp = '^('+base_url+'.+?|\\/\\w.+?)(#.*)?$';
            base_url_regexp = base_url_regexp.replace('http://', 'https?://');
            var jump_to_tag_regexp = new RegExp(base_url_regexp, 'g');
	    
            jQuery("a").each(function() {
                if (jQuery(this).hasClass('no-cpt-prgpt')) {
                    return;
                } else {
                    var old_inner_content = jQuery(this).html();
                    var href = jQuery(this).attr("href");
                    var pttrn = new RegExp('('+params.join('|')+')=');
                    if(href != undefined && !(pttrn.test(href)) && href.match(jump_to_tag_regexp)) {
                        var append_char = (href.match(/\?/)) ? "&" : "?";
                        var jump_to_tag = '';
                        var p_matches;
                        if (p_matches = jump_to_tag_regexp.exec(href)) {
                            href = p_matches[1];
                            jump_to_tag  = p_matches[p_matches.length - 1];
                        }
                        if (!jump_to_tag) {
                            jump_to_tag = '';
                        }
						if (cpt_prms || jump_to_tag) { 
                            if(jQuery('.home-wrapper').length > 0 && jQuery(this).closest('#settings_lang_div').length && !window.location.href.match(/hmb=1/)){
                                href += append_char + cpt_prms_no_hmb + jump_to_tag;
                            }
                            else{
							    href += append_char + cpt_prms + jump_to_tag;
                            }
							jQuery(this).attr("href", href);
						}
                    }
                    jQuery(this).html(old_inner_content);
                }
            });
        }
    
    
    
    function highlight () {                                                                                                                                                                                       
        var question_count = 28;                                                                                                                                                                                
        var url = location.href;                                                                                                                                                                                
                                                                                                                                                                                                              
        for (var i=1 ;i <= question_count ;i++)                                                                                                                                                                   
        {                                                                                                                                                                                                       
            var answer_id = 'q'+i+'ab'; 
            if(document.getElementById(answer_id) != null) {                                                                                                                                                  
                document.getElementById(answer_id).style.backgroundColor = '#FFFFFF';                                                                                                                            
            }                                                                                                                                                                                                   
        }                                                                                                                                                                                                        
        if ( (url.indexOf('privacy') != -1) && (document.getElementById('qgoogle_cookieab'))) {                                                                                                                  
            document.getElementById('qgoogle_cookieab').style.backgroundColor = '#FFFFFF' ;                                                                                                                       
        }                                                                                                                                                                                                        
                                                                                                                                                                                                               
        var ie = (document.all)? true: false;                                                                                                                                                                 
        if (url.indexOf('#') != -1) {                                                                                                                                                                         
            var offset = url.indexOf('#');                                                                                                                                                                       
            var tagname = url.substring(offset + 1);                                                                                                                                                            
                                                                                                                                                                                                              
            if (tagname != 'field') {                                                                                                                                                                            
                if (tagname == 'lowestprice') {                                                                                                                                                                  
                    tagname = 'phone';                                                                                                                                                                          
                }                                                                                                                                                                                               
                if (tagname == 'google_cookie') {                                                                                                                                                               
                    tagname = 'q' + tagname;                                                                                                                                                                     
                }                                                                                                                                                                                                
                tagname += 'ab';                                                     
                if (document.getElementById(tagname)) {                                                                                                                                                         
                    document.getElementById(tagname).style.backgroundColor = '#EFF3FF'
                }
        //Added to color the question part as white on the privacy q-a page.
/*
            if(url.indexOf('protect-privacy') != -1) {
                var question = url.substring(offset + 1) + 'q';
                if (document.getElementById(question)) {
                    document.getElementById(question).style.backgroundColor ='#FFFFFF';
                }
            }
*/
            } else {
                for (var i=1; i < 10; i++) {
                    var fieldname = tagname + i;
                    if (document.getElementById(fieldname)) {
                        document.getElementById(fieldname).style.backgroundColor = '#EFF3FF';
                    } else {
                        break;
                    }
                }
            }
        }
        setTimeout('highlight()',200);
    }
highlight();

jQuery(document).ready(function () {
 jQuery('#information-sharing-link').on('click',function () {
         jQuery('#info-sharing-div').show();
         jQuery('#loadingId').show();
         blockPageScrolling('info-sharing-div');
    });
 jQuery('.closeArea a, #loadingId').on('click',function () {
         jQuery('#info-sharing-div').hide();
         jQuery('#loadingId').hide();
         blockPageScrolling('info-sharing-div');
    });
 jQuery('#url_gen_link').on('click',function () {
         jQuery('#url-generator-div').show();
         jQuery('#loadingId').show();
         blockPageScrolling('url-generator-div');
    });
 jQuery('.closeArea a, #loadingId').on('click',function () {
         jQuery('#url-generator-div').hide();
         jQuery('#loadingId').hide();
         blockPageScrolling('url-generator-div');
    });
 jQuery(document).keyup(function(e) {
  if (e.keyCode == 27) {
     jQuery('#url-generator-div').hide();
     jQuery('#info-sharing-div').hide();
     blockPageScrolling('url-generator-div');
     blockPageScrolling('info-sharing-div');
     jQuery('#loadingId').hide();
  }
    });
jQuery('#loadingId').doubletap(
         /** doubletap-dblclick callback */
         function(event){
             event.preventDefault();
         },
         /** touch-click callback (touch) */
         function(event){
             jQuery('#url-generator-div').hide();
             jQuery('#info-sharing-div').hide();
             blockPageScrolling('url-generator-div');
             blockPageScrolling('info-sharing-div');
             jQuery('#loadingId').hide();
         },
         /** doubletap-dblclick delay (default is 500 ms) */
         250
    );
});

    </script>
