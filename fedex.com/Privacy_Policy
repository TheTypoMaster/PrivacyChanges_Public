<script>
               
               var fx = fx || {};
               // Helpers
               (function(fx) {
               	fx.hasClass = function(element, theClass) {
               		var pattern = new RegExp("(^|\\s+)" + theClass + "(\\s+|$)");
               		return element.className && element.className.match(pattern);
               	}
               	fx.addClass = function(element, theClass) {
               		if(fx.hasClass(element, theClass)) { return; }
               		if(element.className != "") { element.className += " "; }
               		element.className += theClass;
               	}
               	fx.removeClass = function(element, theClass) {
               		if(element.className) {
               			var pattern = new RegExp("(^|\\s+)(" + theClass + "(\\s+|$))+", "g");
               			element.className = element.className.replace(pattern, "$3");
               		}
               	}
               	fx.toggleClass = function(element, theClass) {
               		// returns true or false to indicate the presence of theClass after toggling
               		if(fx.hasClass(element, theClass)) {
               			fx.removeClass(element, theClass);
               			return false;
               		} else {
               			fx.addClass(element, theClass);
               			return true;
               		}
               	}
               	fx.getChildPosition = function(child, includeTextNodes) {
               		var nodes;
               		if(includeTextNodes) {
               			nodes = child.parentNode.childNodes;
               		} else { // if includeTextNodes is not passed, this else will execute by default
               			nodes = [];
               			var temp = child.parentNode.childNodes;
               			for(var i = 0, n = temp.length; i < n; i++) {
               				if(temp[i].nodeType != document.TEXT_NODE) {
               					nodes.push(temp[i]);
               				}
               			}
               		}
               		for(var j = nodes.length - 1; j > -1; j--) {
               			if(nodes[j] == child) {
               				return j;
               			}
               		}
               		return -1;
               	}
               }(fx));
               /***********************************************************************
                The fx_share object handles tracking logic on share links
               ***********************************************************************/
               fx_share = {
               	body: document.getElementsByTagName("body")[0],
               	prep_share: function(container) {
               		var links = container.getElementsByTagName("a");
               		for(var i = 0, n = links.length; i < n; i++) {
               			fx_share.addEvent(links[i], 'click', function(e) {
               				if(e) {
               					e.stopPropagation();
               				} else {
               					window.event.cancelBubble = true;
               				}
               				try {
               					logLinkView(this.getAttribute("data-loglink"), this.getAttribute("data-logevent"));
               				} catch(ex) { }
               				try {
               					// data-xpo can be populated with an xp1 pixel 
               					if(this.getAttribute("data-xpo").length > 0) {
               						var img = document.createElement("img");
               						img.src = this.getAttribute("data-xpo"); // this is enough to trigger the download
               					}
               				}catch(ex) { }
               			});
               		}
               		/*FB.Event.subscribe("edge.create",
               			function(response) {
               				try {
               					logLinkView("facebookLike", "event56");
               				} catch(ex) { }
               			}
               		);
               		FB.Event.subscribe("edge.remove",
               			function(response) {
               				try {
               					logLinkView("facebookUnlike", "event56");
               				} catch(ex) { }
               			}
               		);*/
               	},
               	addEvent: function(target, evt, method) {
               		if(target.addEventListener) {
               			target.addEventListener(evt, method, false);
               		} else if(target.attachEvent) {
               			target.attachEvent("on" + evt, method);
               		}
               	}
               }
               /*******************************************
                End fx_share code.
               *******************************************/
               //fx_.onLoaded(function(){
               function fx__onLoaded(){
               try{
               var facebookcontainer=document.getElementById("facebookcontainer").innerHTML;
               document.getElementById("facebookcontainer").innerHTML="";
               document.getElementById("facebookcontainer").style.display="none";
               document.getElementById("facebookreal").innerHTML= facebookcontainer;
               document.getElementById("facebookid").style.padding="0px";				
               }
               catch(err){}
               	var c =document.getElementById("content");
               	var d = c.getElementsByTagName("div");
               	for(var i=0; i < d.length; i++) {
               		if(fx.hasClass(d[i], "shareStrip")) {
               			fx_share.prep_share(d[i]);
               		}
               	}
               }
