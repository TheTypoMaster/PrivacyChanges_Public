<script type="text/javascript">
	getDomainForGA = function () {
		try {
			var domainName = "";
			var url = window.location.hostname;
			var urlparts = url.split('.');
			var i = urlparts.length;
			if (i >= 2)
				domainName =  "." + urlparts[i - 2] + "." + urlparts[i - 1];
			else
				domainName = ".progressive.com";
			return domainName;
		}
		catch (err) { };
	};

	var _gaObj = _gaObj || {};
	var _gaq = _gaq || [];
	var epm = epm || [];
