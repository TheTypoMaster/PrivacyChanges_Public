	<script language="JavaScript" type="text/javascript" src="https://www.progressive.com/content/js/domainprogressive/pgrx.min.js;pvd8911afebe3f062d"></script>
	<script language="JavaScript" type="text/javascript" src="https://www.progressive.com/content/js/domainprogressive/gaEventTracking.min.js;pvb76477e61e8acba2"></script>
	
	<script type="text/javascript">
		$(function () {
			var stateAbbr = ''.toUpperCase();

			$("#policy .back").click(function () {
				$(".content .original, #policy").toggle();
				return false;
			});
			if (stateAbbr.length > 0) {
				PrivacyFormSubmit();
			}

			$(".original.links a").bind("click", function() {
				stateAbbr = $(this).data("state");
				if (stateAbbr.length > 0) {
					PrivacyFormSubmit();
					window.scrollTo(0, 0);
				}
				return false;
			});
			

			function PrivacyFormSubmit() {
				$(".content .original, #policy").toggle();
				$.ajax({
					url: '/customer_service/privacycontent?state=' + stateAbbr,
					dataType: "text",
					type: 'GET',
					success: function (data) {
						$("#policy .details").html(data);
					}
				});
				return false;
			}
		});
	</script>


	<script type="text/javascript">
		$(window).bind("load", function () {
			var element = document.createElement("script");
			element.src = "https://preferences.truste.com/webservices/js?domain=progressive.com&type=progressive&js=2";
			$("body").append(element);
		});
	</script>




	 
