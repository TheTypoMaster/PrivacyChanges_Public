<script type="text/javascript">
  //sets the wrapping header id and classes for RIT integration



  (function(window, doc) {

    function logger() {

    };

    function updateSite(){
      if(window.location.host.indexOf('touch') > -1){ document.getElementsByTagName('html')[0].setAttribute('data-tcom-site','touch');}else{document.getElementsByTagName('html')[0].setAttribute('data-tcom-site','tcom');}
    }

    function updateHeader() {

      var globalNav = doc.getElementById('tcom-global-nav');

      var header = globalNav.parentNode;

      var tcomId = 'tcom-global-nav-header';

      var tcomClasses = 'tcom-global-nav-header tcom-global-nav-section';

      if(header.nodeName.toUpperCase() === 'HEADER') {

        var headerId = (header.getAttribute('id') || '').toLowerCase();

        var headerClass = header.getAttribute('class') || '';

        var newClasses = headerClass.toLowerCase()
                            .replace('tcom-global-nav-header', '')
                            .replace('tcom-global-nav-section', '');

        newClasses += (' '+tcomClasses);

        header.setAttribute('class', tcomClasses);

        if(headerId !== tcomId) {
          header.setAttribute('id', tcomId);
        }
        else {
          logger('already have the header id attached');
        }

      } else {
        logger('Cannot find the header tag', header);
      }
    }

    if ( document.addEventListener ) {
      document.addEventListener( "DOMContentLoaded", function(){
        document.removeEventListener( "DOMContentLoaded", arguments.callee, false);
        updateSite();
        updateHeader();
      }, false );

    // If IE event model is used
    } else if ( document.attachEvent ) {
      // ensure firing before onload
      document.attachEvent("onreadystatechange", function(){
        if ( document.readyState === "complete" ) {
          document.detachEvent( "onreadystatechange", arguments.callee );
          updateSite();
          updateHeader();
        }
      });
    }

  }(window, document));
</script>



		</header>
