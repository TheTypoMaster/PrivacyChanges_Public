<script type="text/javascript">
  //Removes wrapping footer tag, for rit integration

  (function(window, doc) {

    function logger() {

    }
    function updateFooter () {
      var globalFooter = doc.getElementById('tcom-global-nav-footer');

      var parent = globalFooter.parentNode;

      if(parent.nodeName.toUpperCase() === 'FOOTER') {

        //var tcomFooter = parent.childNodes;

        parent.parentNode.replaceChild(globalFooter, parent);

        logger('Unwrapped the TCOM footer');

      } else {
        logger('No wrapping footer element');
      }
    }

    if ( document.addEventListener ) {
      document.addEventListener( "DOMContentLoaded", function(){
        document.removeEventListener( "DOMContentLoaded", arguments.callee, false);
        updateFooter();
      }, false );

    // If IE event model is used
    } else if ( document.attachEvent ) {
      // ensure firing before onload
      document.attachEvent("onreadystatechange", function(){
        if ( document.readyState === "complete" ) {
          document.detachEvent( "onreadystatechange", arguments.callee );
          updateFooter();
        }
      });
    }

    function initadchoices(){
