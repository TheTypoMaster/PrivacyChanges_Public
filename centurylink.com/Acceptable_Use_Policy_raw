<script>
require(['centurycore','modals'],function(require){


	$('#ctam_new-customer-link').click(function(e)
		{
			e.preventDefault();

			//toggle display of new customer content in modal block
			$('#ctam_current-customer-expanded').hide();
			$('#CTAM_Dynamic_Msg').css("display","none");
			//$('#protectInfo').css("display","none");
			$('#ctam_new-customer-expanded').toggle();
		}
	);
	$('#ctam_current-customer-link').click(function(e)
		{
			e.preventDefault();

			//toggle display of new customer content in modal block
			$('#ctam_new-customer-expanded').hide();
			$('#ctam_current-customer-expanded').toggle();
			$('#CTAM_Dynamic_Msg').css("display","block");
			//$('#protectInfo').css("display","block");
		}
	);
	$('#zam_zip-code').blur(function()
		{
			var zipLength = $(this).val();
			zipLength = (zipLength ? zipLength.match(/\d/g).length : 0);

			if(zipLength == 5)
			{
				$('#zam_overlap-go').click(function()
					{
						return true;
					}
				);
			}
		}
	);
});
</script>

<script>
$(document).ready(function() {

	 $('a.disclaimer-trigger').click(function (event) {
	 
    if(this.href.indexOf('promotions-ens.html') >-1 || this.href.indexOf('pricing_details_popup.html') >-1)  {
      var disclaimerModalTitle = '<h1 style="font-family: arial ;font-style: normal;font-size:18px">CenturyLink<sup style="font-size: .7em;">&reg;</sup>&nbsp; Promotions Details</h1>';
      }
    else  {
      var disclaimerModalTitle = '<h1 style="font-family: arial ;font-style: normal;font-size:18px">CenturyLink <sup style="font-size: .7em;margin-left: -4px;">&reg;</sup>&nbsp; Product Details</h1>';
    }
	 
    $("#disclaimerModal iframe").attr("src", this.href);
		$("#disclaimerModal").dialog({
		height: 'auto',
		width: 'auto',
		title: disclaimerModalTitle,
		closeText: 'Close',
		modal: true, 
		resizable: false,  
		open: function(event, ui) {
			  $(this).parent().addClass('ui-widget-disclaimer').removeClass('ui-widget-content');
			  $(this).parent().addClass('ui-widget-disclaimer-fixed-popup');
        $('#dialogButton').blur();
			  }
		});
  event.preventDefault();
	});
});
</script>
<!-- End of WCP 130225-25 CPNI Compliance - Dynamic Message on CTAM (Ecom#2961) -->


