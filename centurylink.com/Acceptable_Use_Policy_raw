	<script type="text/javascript">
			$(document).ready(function(){		    
				$("#chat-residential-english-LQ-LCTL").click(function() {
					trackClick('ctl|rsd|global_nav|link|chat');
				});
        });
		//var ttGeoLoc = { state: null, city: null, dma: null, zipcode: null };
		require(['centurycore','cc-shortcuts','cookies','purl','cookie'],function(){
			$('#dd_navigation-change-zip-btn').click(function(){
				//remove the profile cookie when this is clicked before the zam is loaded
				//subcookiejar.crumble('profile_cookie');
				qm.ignorePreviousAuthData=true;
			});
			var zip;
			//parse the url to look for query string and pull zip override if needed
			var url=$.url();
			if(url.param('zipOverride')){
				zip=url.param('zipOverride');
			}
			else if(subcookiejar.fetch('remember_me','zipCode')){
				zip=subcookiejar.fetch('remember_me','zipCode');
			}else if(ttGeoLoc.zipcode && ttGeoLoc.zipcode!==null){
				zip=ttGeoLoc.zipcode;
			}
			$.cookie('geoloc-zip',zip);
			if(!subcookiejar.fetch('profile_cookie')){
				//only submit if the profile cookie isnt set already
				$('#zam_zip-code').val(zip);
				var ctlurl='//www.centurylink.com'
				if(ctlurl.indexOf("gamut.centurylink.com")!=-1)
				{
					initializeLocator("EBIZ","//webdmzqa2.centurylink.com");
				} else{
					initializeLocator("EBIZ",'//www.centurylink.com');
					getProfileFromZip(zip,'Y',qmz.profile);
				}
			}

			//US2450
			
				/* US2450 Live Person */
				var company=subcookiejar.fetch('profile_cookie','shoppingDomain');

				window.lpMTagConfig = lpMTagConfig || {}; lpMTagConfig.vars = lpMTagConfig.vars || [];

				lpMTagConfig.lpServer = "sales.liveperson.net";
				lpMTagConfig.lpTagSrv = lpMTagConfig.lpServer;
				lpMTagConfig.lpNumber = "86584264"; // clone# 90218425 prod#86584264
				lpMTagConfig.deploymentID = "centurylink";

				lpMTagConfig.vars.push(["page", "unit", "residential"]);
				lpMTagConfig.vars.push(["session", "language", "english"]);

				lpMTagConfig.vars.push(["page", "lpCompany", 'L'+company]); //value of lpCompany = 'LQ' OR 'LCTL'
				lpMTagConfig.vars.push(["page", "lpZipCode", zip]);
			
		});
	</script>
	
