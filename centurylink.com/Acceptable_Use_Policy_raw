	<script type="text/javascript">

		$(function(){
			$(".businessSubMenu li").live('click', function (event) {
				var $selectedSubMenuItem=$(this).text();
				$("#business").text("Business: "+$selectedSubMenuItem);  
			}); 	
		});

		function getURLParameter(name) {
			return decodeURIComponent((new RegExp('[?|&]' + name + '=' + '([^&;]+?)(&|#|;|$)').exec(location.search)||[,""])[1].replace(/\+/g, '%20'))||null;
		}

	</script>

	<script type="text/javascript">
	require(['jquery-ui','cookies','cookie'],function(){
		if (getURLParameter("PSTN")!=null || getURLParameter("DTN")!=null)
		{
		  var pstn = getURLParameter("PSTN");
		  var dtn = getURLParameter("DTN");
		  $.cookie('pstn_cookie', pstn, { expires: 30, path: '/' });
		  $.cookie('dtn_cookie', dtn, { expires: 30, path: '/' });
		}
		else if ($.cookie('pstn_cookie')!=null || $.cookie('dtn_cookie')!=null)
		{
		  var pstn = $.cookie('pstn_cookie');
		  var dtn = $.cookie('dtn_cookie');
		}
		if(pstn=="Q")
		  $("#dynamic-800-number").text("1-877-417-3983");
		else if (pstn=="C")
		  $("#dynamic-800-number").text("1-866-904-1817");
		else if (dtn=="Q")
		  $("#dynamic-800-number").text("1-888-807-4368");
		else if (dtn=="C")
		  $("#dynamic-800-number").text("1-866-962-6021");
		else
		  $("#dynamic-800-number").text("Call Us");
 	});

	</script>



