<script type="text/javascript">
//###############################################
//Begin runtime update of default TV URL to Prism if Prism DMA true or IPTV cookie set by other process
//init
//init functionality for prism nav test parameters functionality; includes condition to override live geo-targeting when using test parameters
var geonav_Prism = location.search.indexOf('geoNavBar=Prism');
var geonav_showDefault = location.search.indexOf('geoNavBar=notPrism');
var navbarGeoStatus = 'enable';
if ((geonav_Prism != -1) || (geonav_showDefault != -1)) {
	navbarGeoStatus = 'disable'; // navbarGeoStatus == 'disable' will deactive live geotargeting in the navbar runtime Prism update evaluation
}

//init geo-related nav vars
var ttNavBarSeePrismTVURL = ctl_url + '/prismtv/';
var thisTTlinkvallvl1 = 'Prism&trade; TV'
var thisTTlinkvallvl2 = 'Shop Prism&trade; TV';

//evaluate test parameter conditions
if (navbarGeoStatus == 'disable') {
	if (geonav_Prism != -1) {
		updatePrismNavURL();
		updateWirelessNavPrism();
	}
	else if (geonav_showDefault != -1) {
		//do nothing
	}
}
//evaluate live geo-targeting conditions
else if(navbarGeoStatus == 'enable') {
	if ((ttGeoLoc.prismDMAGroup != "nonPrismDMA") || isIPTVCookieChecker() ) {
		updatePrismNavURL();
		updateWirelessNavPrism();
	}
}

function updatePrismNavURL() {
		if(document.getElementById('tpl_subnav_lvl1_tv')){
			document.getElementById('tpl_subnav_lvl1_tv').innerHTML=thisTTlinkvallvl1;
			document.getElementById('tpl_subnav_lvl1_tv').href=ttNavBarSeePrismTVURL;
			
			document.getElementById('tpl_subnav_lvl1_tv').setAttribute('clicktrack','ctl|rsd|sub_nav|link|Prismtv');
			
		}
		if(document.getElementById('tpl_subnav_lvl2_tv')){
			document.getElementById('tpl_subnav_lvl2_tv').innerHTML=thisTTlinkvallvl2;
			document.getElementById('tpl_subnav_lvl2_tv').href=ttNavBarSeePrismTVURL;
			document.getElementById('tpl_subnav_lvl2_tv').setAttribute('clicktrack','ctl|rsd|sub_nav|dd|link|prismtv');
			
		}		
}

function updateWirelessNavPrism() {
	$('#tpl_subnav_lvl1_wireless').text('Wireless');
}

function isIPTVCookieChecker() {
	var cookieName="isIptv";
	var theCookie=" "+document.cookie;
	var ind=theCookie.indexOf(" "+cookieName+"=");
	if (ind==-1) { ind=theCookie.indexOf(";"+cookieName+"="); }
	var ind1=theCookie.indexOf(";",ind+1);
	if (ind1==-1) { ind1=theCookie.length; }
	var isIptvQ = unescape(theCookie.substring(ind+cookieName.length+2,ind1));
	if(isIptvQ == "true") {
		return true;
	}
	else {
		return false;
	}
}


/*  //deprecated/superseded by the runtime update code block above, including isIPTVCookieChecker(), which also incorporates geo-targeting for the TV element;
	tvNavigation() function left in code for documentation purposes

function tvNavigation() {
  //ctl_url
    var cookieName="isIptv";
	var theCookie=" "+document.cookie;
	var ind=theCookie.indexOf(" "+cookieName+"=");
	if (ind==-1) ind=theCookie.indexOf(";"+cookieName+"=");
	if (ind==-1 || cookieName=="") {
     location = "//www.centurylink.com/home/tv/";
	 }
	else {
	var ind1=theCookie.indexOf(";",ind+1);
	if (ind1==-1) ind1=theCookie.length;
	var isIptvQ = unescape(theCookie.substring(ind+cookieName.length+2,ind1));
	if(isIptvQ == "true")
      location = "//www.centurylink.com/prismtv/#index.html";
	else
	 location = "//www.centurylink.com/home/tv/";
  }
}
*/
//End runtime update of default TV URL to Prism if Prism DMA true or IPTV cookie set by other process
//###############################################




</script>
