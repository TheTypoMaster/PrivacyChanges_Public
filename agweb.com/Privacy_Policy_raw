	/* <![CDATA[ */
	var p = BaseModule.getQueryVariable("print");
	var isPrint = (p && p.toLowerCase() == "y");
	
    if (isPrint) {
        $("#HeaderMain").hide();
        $("#HeaderPrint").show(); 
    } else {
        $("#HeaderMain").show();
        $("#HeaderPrint").hide();
    };
	/* ]]> */
     
    function isLoggedIn() {
        var retVal = false;
        var options = {
            "url": "/ajax/members/ajax.aspx",
            "method": "GET",
            "async": false,
            "dataType": "json",
            "success": function (o) {
                // check the validation_response and update UI/redirect/etc
                if (o.Success) {
                    if (o.LoggedIn == 'true') {
                        retVal = true;
                    }
                    else {
                        retVal = false;
                    }
                }
                else {
                    retVal = false;
                }
            },
            "error": function () {
                retVal = false;
            },
            "data": {
                "F": "MemberLoggedIn"
            }
        };
        jQuery.ajax(options);
        return retVal;
    }

    $(document).ready(function () {
        if (isLoggedIn()) {
            $("#signup").hide();
            $("#myaccount").show();
        }
        else {
            $("#signup").show();
            $("#myaccount").hide();
        }
    });

    $('#RampSearchbutton').click(search);
    $('#RampSearchText').on('keypress', function (e) {        
        var code = (e.keyCode ? e.keyCode : e.which);
        if (code == 13) {
            e.preventDefault();
            search()
        }
    });

    function search() {
        if(encodeURIComponent($('#RampSearchText').val()) != "")
        {
            document.location.href = 'http://search.agweb.com/search?q=' + encodeURIComponent($('#RampSearchText').val());
        }
    }

</script>

			




<script type="text/javascript">
    $(document).ready(function () {
        if (window.location.href.indexOf("/mobile/") > -1) {
            $(".sf-menu").hide();
        }
    });
</script>

