var emailLightboxID="lightbox.email.05Feb2015";
$(document).ready(function(){
	if($.cookie('lightbox_track')){
                	$.removeCookie('lightbox_track');
    }

	var flyoutName = '#flyout';
	var cookieName = 'aa_noflyout';

	$("#closebox").click(function () {
		$('#flyout').stop(true, true).removeClass('visible');
		$.cookie(cookieName, 'true', { expires: 7, path: '/' });
		return false;
	});
	
	$("#closelink").click(function () {
		$('#flyout').stop(true, true).removeClass('visible');
		$.cookie(cookieName, 'true', { expires: 10000, path: '/' });
		return false;
	});
	
	$("#sign-up").click(function () {
		$.cookie('emailsignup', 'true', { expires: 10000, path: '/' });
		return true;
	});

	$(window).scroll(function() {
	var close = ($.cookie(cookieName));
	var emailsignup = ($.cookie('emailsignup'));
	
	if ($.cookie('s_lookup')) {
          var sitecat = "exclude";
    }
	var pageUrl = window.location.href;
	
 	if (pageUrl.indexOf("src=e.") > -1) {
  	var campaigncode = "exclude";
 	} else {
 	var campaigncode = null;
 	}
	if (pageUrl.indexOf("lu=") > -1) {
    var campaigncode2 = "exclude";
 	} else {
 	var campaigncode2 = null;
 	}
 	if ($(window).width() < 750) {
 	var mobile = "mobile";
 	} else {
 	var mobile = null;
 	}
 	var isMobile = window.matchMedia("only screen and (max-width: 750px)");
    if (isMobile.matches) {
 	var mobile = "mobile";
 	} else {
 	var mobile = null;
 	}
		if(close == null && campaigncode == null && campaigncode2 == null && sitecat == null && mobile == null && emailsignup == null){
			if($(document).scrollTop() > 100 ) {
				$(flyoutName).addClass('visible');
				if($.cookie('lightbox_track') == null ){
                	$.cookie('lightbox_track', 'true');
                	_satellite.track("emailLightboxOpen");
                }              
			}
			else {
				$(flyoutName).removeClass('visible');
			}
		}
		
	});

	// dynamicFormWrapper('#dynamicform-sign-up-enews');//initialize multistep form process

	// pulls "name" attribute from input field"
	$('#sign-up-enews-flyover').addClass('gonnavalidateit').accessibleValidate({
		rules: { // full list of methods to test against: http://docs.jquery.com/Plugins/Validation#List_of_built-in_Validation_methods
			"cons_email": {
				required: true,
				email: true
			},
			cons_first_name: "required",
			cons_last_name: "required",
			"cons_state": {required: false}
		},
		defaults: {
			errorClass: "notvalid",
			validClass: "valid"
		},
		messages: { // messages print as "label text": "error message". if labels aren't used refer to showErrors: function(errorMap, errorList), line 175 in jquer.validate.custom.js
			"cons_email": "*",
			"cons_first_name": "*",
			"cons_last_name": "*",
			"cons_state": "*"
		}
	});
	
});

