							<script type="text/javascript">
var laDate = new Date();
var semaineArray = new Array("Dimanche","Lundi","Mardi","Mercredi","Jeudi","Vendredi","Samedi");
var semaine = semaineArray[laDate.getDay()]; 
var leJour = laDate.getDate();
var leMoisArray = new Array("janvier","février","mars","avril","mai","juin","juillet","août","septembre","octobre","novembre","décembre");
var leMois = leMoisArray[laDate.getMonth()]; 
var annee = laDate.getFullYear();
var fullDate = semaine+"&nbsp;"+leJour+"&nbsp;"+leMois+"&nbsp;"+annee
document.write("<p>"+fullDate+"</p>");
</script>
<script type="text/javascript">
//clear les search box on click
function clickclear(thisfield, defaulttext,attr_name) {
	if (thisfield.value == defaulttext) {
		thisfield.value = "" ;
		thisfield.name = attr_name ;
	}
}
function clickrecall(thisfield, defaulttext) {
	if (thisfield.value == "") {
		thisfield.value = defaulttext ;
		thisfield.name = "" ;
	}
}
</script>
<form action="http://recherche.lapresse.ca/cyberpresse/search/theme/lapresse/">
	<input class="champsRecherche" type="text" value="Rechercher" onfocus="clickclear(this, 'Rechercher','q')" onblur="clickrecall(this,'Rechercher')" />
    <input class="btnSubmit" type="submit" value="&nbsp;" />
</form>                        </div>
                    </div>
