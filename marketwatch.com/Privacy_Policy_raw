      <!-- Only set uid, domain and path once -->
        var _sf_async_config = _sf_async_config || {};
        _sf_async_config.uid = 17409;
        _sf_async_config.domain = "marketwatch.com";

        var cl = $('link[rel=canonical]');
        if(cl != undefined && cl.length > 0) {
        _sf_async_config.useCanonical = true;
        }
        else {
        _sf_async_config.useCanonical = false;
        _sf_async_config.path = '/support/privacy.asp';
        }
      var _sf_async_config = _sf_async_config || {};
        _sf_async_config.sections = '';
        if (window.dji_config) { window.dji_config.sections = ''; }
        _sf_async_config.authors = '';
        if (window.dji_config) { window.dji_config.authors = ''; }
        _sf_async_config.zone = 'default';
      (function(){
      function loadChartbeat() {
      window._sf_endpt=(new Date()).getTime();
      var e = document.createElement('script');
      e.setAttribute('language', 'javascript');
      e.setAttribute('type', 'text/javascript');
      e.setAttribute('src',
      (("https:" == document.location.protocol) ? "https://a248.e.akamai.net/chartbeat.download.akamai.com/102508/" : "http://static.chartbeat.com/") +
      "js/chartbeat_pub.js");
      document.body.appendChild(e);
      }
      var oldonload = window.onload;
      window.onload = (typeof window.onload != 'function') ?
      function() {
      loadChartbeat();
      loadDJInsightsHooks();
      } :
      function() {
      oldonload();
      loadChartbeat();
      loadDJInsightsHooks();
      }
      })();
    </script>

