function timepoint(name) {
    var d = new Date();

    var h = d.getHours();
    var m = d.getMinutes();
    var s = d.getSeconds() + (d.getMilliseconds() / 1000);

    var timeString = ( (h < 10)? '0' + h : h ) + ':'
                   + ( (m < 10)? '0' + m : m ) + ':'
                   + ( (s < 10)? '0' + s : s );

    var pre = document.createElement('pre');
    pre.innerHTML = timeString + ":	" + name;
    document.body.appendChild(pre);
    pre = null;
}
