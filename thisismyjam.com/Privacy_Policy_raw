            // stupid facebook http://stackoverflow.com/questions/7131909/facebook-callback-appends-to-return-url
            if (window.location.hash == '#_=_')window.location.hash = '';

            window.fbAsyncInit = function() {

                FB.Event.subscribe('auth.statusChange', function(response) {
                    if (response.status === 'connected') {
                      $.ajax({
                        type: "POST",
                        url: '/ajax/fbs',
                        data: {u:response.authResponse.userID,t:response.authResponse.accessToken},
                        dataType: 'json'
                      });
                    } else if (response.status === 'not_authorized') {
                      // the user is logged in to Facebook, 
                      // but has not authenticated your app
                    } else {
                      // the user isn't logged in to Facebook.
                    }
                })

                logger.add('fbAsyncInit: before FB init');

                FB.init({
                    appId      : "210605368997076",
                    version    : 'v2.2',
                    channelURL : 'www.thisismyjam.com/channel.php', // Channel File
                    status     : true, // check login status
                    cookie     : true, // enable cookies to allow the server to access the session
                    oauth      : true, // enable OAuth 2.0
                    xfbml      : true  // parse XML
                });

                window.FB_API_READY = true; //init flag

                logger.add('fbAsyncInit: window.FB_API_READY');
            };

              logger.add('Before self-evaluating FB function');

              // Load the SDK Asynchronously
              (function(d){
                  var js, id = 'facebook-jssdk'; if (d.getElementById(id)) {return;}
                  js = d.createElement('script'); js.id = id; js.async = true;
                  js.src = "//connect.facebook.com/en_US/sdk.js";
                  d.getElementsByTagName('head')[0].appendChild(js);

                  logger.add('FB facebook-jssdk appended');

              }(document));

              // Load in twitter SDK
              window.twttr = (function (d,s,id) {
                    var t, js, fjs = d.getElementsByTagName(s)[0];
                    if (d.getElementById(id)) return;
                    js=d.createElement(s);
                    js.id=id;
                    js.src="//platform.twitter.com/widgets.js"; fjs.parentNode.insertBefore(js, fjs);
                    return window.twttr || (t = { _e: [], ready: function(f){ t._e.push(f); } });
                }(document, "script", "twitter-wjs"));

