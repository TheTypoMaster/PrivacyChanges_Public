<script type="text/javascript">

window.sexyPreloadAnImg = function(urlArr){

  if(!(urlArr instanceof Array))
    urlArr = [urlArr]; // ensure array

  if(typeof(urlArr) === "object"){
    for(var i=0;i<urlArr.length;i++){
      var b = new Image();
          b.src = urlArr[i];
      b.onload = function(){
        return this;
      }
    }
  }
}

/* BEGIN SETUP OF VARIOUS PLAYBACK METHODS */

window.forceHTML5 = null;
var smTimeouts = 0;
soundManager.url = '/includes/swf/';
soundManager.flashVersion = 9; // optional: shiny features (default = 8)
soundManager.useFlashBlock = true;
soundManager.useHighPerformance = true; // keep flash on screen, boost performance
soundManager.wmode = 'transparent'; // transparent SWF, if possible
soundManager.useFastPolling = true; // increased JS callback frequency
soundManager.useHTML5Audio = true;
if (window.forceHTML5)
    soundManager.preferFlash = false;
else
    soundManager.preferFlash = true;
soundManager.flashLoadTimeout = 1000;
soundManager.debugMode = false;
soundManager.consoleOnly = true;
soundManager.ontimeout(function(status) {

    if(soundManager.isReady || soundManager.flashIsMissing)
      return;

    console.log('SoundManager initialisation error:', status.error);

    if(status.error.type == 'FLASHBLOCK'){
        soundManager.flashIsMissing = true;

        if(!Modernizr.video || !(Modernizr.audio && Modernizr.audio.mp3)) {
            JamError.show('Oops, it looks like you have Flash disabled. Some jams might not play.');
        }
        else {
            soundManager.preferFlash = false;
            soundManager.flashLoadTimeout = 0;
            soundManager.reboot();
            $('#sm2-container').hide();
            if(!window.ytPlayer.isReady)
                window.ytPlayer.forceHTML5();
        }
    } else {
        smTimeouts++;
        //console.log(soundManager.flashLoadTimeout * smTimeouts);
        soundManager.flashLoadTimeout = soundManager.flashLoadTimeout * smTimeouts; // Keep bumpin.
    }
});

window.ytPlayer = new YoutubePlayer('ytPlayer', $("#ytContainer"), true); // TODO - remove me when YT has patched things?
                                                                        //  'true' was: window.forceHTML5
                                                                        
window.vimeoPlayer = new VimeoPlayer($("#vimeoContainer"), !window.hasFlash()); // TODO: figure out if we need any forceHTML5 here.
                                                                                //       seems like no, but opportunity to consolidate?

// insert html5 video api script tag, everyone gets it
var tag = document.createElement('script');
tag.src = "http://www.youtube.com/player_api";
var firstScriptTag = document.getElementsByTagName('script')[0];
firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);


$(function() {
    window.player.init();
});

if(window.isIOS())
    window.iosVideoPlaybackHandler = new IOSVideoPlaybackHandler();

window.youtubePlaybackHandler = window.isIOS() ?
    window.iosVideoPlaybackHandler : new YoutubePlaybackHandler();
window.vimeoPlaybackHandler = window.isIOS() ?
    window.iosVideoPlaybackHandler : new VimeoPlaybackHandler();
window.audioPlaybackHandler = new AudioPlaybackHandler();
window.player = new Player();
window.player.muted = 0;
window.player.volume = 100;
window.playerView = new PlayerView();
window.playlistBag = new PlaylistBag();

$(function(){

    sexyPreloadAnImg(["/includes/image/ajax-loader.gif"]);

    window.soundManager.onready(function() {
            window.soundManager.isReady = true;
    });

    // soundmanager not quite ready yet
    window.soundManager.onerror = function(e) {
        console.log('SoundManager error:', e);
        if(e.type == 'INIT_TIMEOUT')
            JamError.show('Oops, we had trouble setting up our music player. Please refresh the page, or try disabling any ad or flash blocking extensions if you have them.');
    }

});


function asset(url) {
    
    var cnames = [ "static01.thisismyjam.com","static02.thisismyjam.com","static03.thisismyjam.com","static04.thisismyjam.com" ];
    var s3topDir = "";

    return 'http://' + cnames[Math.floor(Math.random() * cnames.length)] +
        s3topDir + url;

    }

function bg(path) {

    var version = 1;

    var base = basename(path);
    var dir = dirname(path);
    if(base.match(/^default/)) {
        base = base.replace(/(default_.*)\.jpg/, '$1_v' +
                            version + '.jpg');
        path = dir + '/' + base;
    }

    return path;
}

window.JS_VERSION = '5a2515f729fd61d89a37fd00dcc058d811723e3c';

// THIS NEEDS TO BE IN THE FOOTER, AS IT DEPENDS ON PHP, AND IT SHOULD
// ALWAYS, ALWAYS WORK

/**
  * Custom error logging. Sends error to server.
 */
window.onerror = function(error, url, line)
{
    // only send to server 1% of the time.
    if(Math.random() * 100 > 0)
        return;

    // using old fashioned xhr just in case there are
    // errors within jquery

    // no way i could remember how to do this. took if from
    // http://www.jibbering.com/2002/4/httprequest.html

    var xmlhttp=false;
    /*@cc_on @*/
    /*@if (@_jscript_version >= 5)
    // JScript gives us Conditional compilation, we can cope with old IE versions.
    // and security blocked creation of the objects.
    try {
    xmlhttp = new ActiveXObject("Msxml2.XMLHTTP");
    } catch (e) {
    try {
    xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
    } catch (E) {
    xmlhttp = false;
    }
    }
    @end @*/
    if (!xmlhttp && typeof XMLHttpRequest!='undefined') {
          try {
                xmlhttp = new XMLHttpRequest();
          } catch (e) {
                xmlhttp=false;
          }
    }
    if (!xmlhttp && window.createRequest) {
          try {
                xmlhttp = window.createRequest();
          } catch (err) {
                xmlhttp=false;
          }
    }

    if(xmlhttp) {
        var params =
            "error=" + error + "&" +
            "url=" + url + "&" +
            "line=" + line;

        xmlhttp.open("POST", 'http://' + window.location.host +
                     '/error/js');
        xmlhttp.setRequestHeader("Content-type",
                              "application/x-www-form-urlencoded");
        xmlhttp.setRequestHeader("Content-length", params.length);
        xmlhttp.setRequestHeader("Connection", "close");

        xmlhttp.send(params);
    }
};



</script>


  
    
        <div id="largeScreen"></div>

  </body>
</html>
