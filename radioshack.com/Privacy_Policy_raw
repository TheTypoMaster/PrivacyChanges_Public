<script>
    $(document).ready(function() {
        $(document).on('click', '#home a, #home area', function (e) {
            e.preventDefault();
            e.stopPropagation();
            e.stopImmediatePropagation();
            var url = $(this).attr('href');
            if (url) {
                ga('bb.send', 'event', 'homecms', 'click', url, {
                    'hitCallback': function () {
                        document.location = url;
                    }
                });
            }
        })
    })
</script>        </div>
