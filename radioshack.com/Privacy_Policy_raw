  window.bb_vid = window.bb_vid || (function(d,n){
    function guidv4() {return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g,function(c){var r=Math.random()*16|0,v=c=='x'?r:r&0x3|0x8;return v.toString(16);});}
    function get() {return decodeURIComponent(d.cookie.replace(new RegExp('(?:(?:^|.*;)\\s*'+encodeURIComponent(n).replace(/[\-\.\+\*]/g,'\\$&')+'\\s*\\=\\s*([^;]*).*$)|^.*$'),'$1')) || null;}
    function set(v,e,p) {return d.cookie=n+'='+(v || guidv4())+'; expires='+(e || (new Date(Date.now()+1000*60*60*24*365)).toUTCString())+'; path='+(p || '/')+';';}
    function update() {return set(get());}
    return {get: get, set: set, update: update};
  }(document,'bb_vid'));
  bb_vid.update();
</script>
