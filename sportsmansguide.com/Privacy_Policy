    <script type="text/javascript">
        //page intialization
        $(document).ready(function () {

            $("#mini-cart").hover(function () { var cartSummary = $('#cartSummary'); if ($(cartSummary).html() == "") { updateCartSummary(); } else { $('#cartSummary').show(); } }, function () { $('#cartSummary').hide(); });
            $('#cartSummary').hide();

            $("#cartSummary").click(function (e) {
                var target = $(e.target);
                if (target.is("a") || target.is("img"))
                    return; // tried stopPropagation

                $("#cartSummary").fadeOut("slow");
            });
        });

        function addClub(clubId, pitchId, refreshCart) {
            if (!refreshCart)
                refreshCart = false;
            removeClubTrialMessage();
            return DoClub('joinclub', clubId, pitchId, refreshCart);
        }

        function renewClub(clubId, pitchId, refreshCart) {
            if (!refreshCart)
                refreshCart = false;
            removeClubTrialMessage();
            return DoClub('renewclub', clubId, pitchId, refreshCart);
        }

        function DoClub(action, clubId, pitchId, refreshCart) {
            return AjaxJsonPostWrapper.ajaxRoute('club', action, { clubId: clubId, pitchId: pitchId, refreshCart: refreshCart }, function (response) {
                if (response.success) {
                    if (response.messages != null && response.messages != "") {
                        Gorilla.util.messages.addMessages(response.messages, 'success');
                    }
                } else {
                    //set messages
                    if (response.messages) {
                        Gorilla.util.messages.addMessages(response.messages, 'error');
                    }
                }
                if (response.cartHtml && response.cartHtml != "") {
                    setCartQuantityText();
                    $('#cartContainer').replaceWith(response.cartHtml);
                    $('#cartSummary').html("");
                    Gorilla.util.initForms();
                }
                if (response.miniCartHtml && response.miniCartHtml !== "") {
                    window.scrollTo(0, 0);
                    //update mini cart html
                    setCartQuantityText();
                    $('#cartSummary').html(response.miniCartHtml);
                    //show dropdown and then hide after 1.5s
                    $('.customer-nav').DropIt('showMenu', $('#mini-cart .drop-down'));
                    window.setTimeout($.proxy(function () {
                        $('.customer-nav').DropIt('hideMenu', $('#mini-cart .drop-down'));
                    }, this), 5000);
                }
            });
        }

        function setCartQuantityText() {
            var displayValue = "0";
            var cookieData = decodeURIComponent(document.cookie.replace(new RegExp("(?:(?:^|.*;)\\s*" + encodeURIComponent("crtclb").replace(/[\-\.\+\*]/g, "\\$&") + "\\s*\\=\\s*([^;]*).*$)|^.*$"), "$1")) || null;
            if (cookieData != null) {
                var cookieValue = cookieData.replace(new RegExp("(?:(?:^|.*;)\\s*" + encodeURIComponent("I").replace(/[\-\.\+\*]/g, "\\$&") + "\\s*\\=\\s*([^;]*).*$)|^.*$"), "$1") || null;
                if (cookieValue != null)
                    displayValue = cookieValue.split('&')[0];
            }
            $('#cartQtyText').text(displayValue);
        }

        function getCategoryMenu(btn, dept) {
            if ($("#k").autocomplete("instance") != undefined) {
                $("#k").autocomplete("close");
            }
            var flyOut = $(btn).siblings('.fly-out');

            if ($(flyOut).html().indexOf('Loading...') > -1) {
                return AjaxJsonPostWrapper.ajaxRoute('sitemap', 'getcategorymenu', { deptId: dept }, function (result) {
                    $(flyOut).html("");
                    $(flyOut).append(result);
                });
            }
        }

        function closeModalWindow(obj) {
            if (($moby = $(obj).closest('.moby-container')).length) {
                $moby.find('.moby-close').trigger('click').off('click');
            }
        }
        function intlShoppingModal(trigger) {
            return AjaxJsonPostWrapper.ajaxRouteNonAsync('main', 'intlshoppingmodal', {}, function (result) {
                $('#intl-shopping').replaceWith(result);
                $('#hidden-intl-shopping-trigger').val(trigger.id) //Store this id so only the link that opens the modal will be closed.
                Gorilla.util.initForms();
            });
        }

        function acceptTrialClub(couponCode, clubId) {
            AjaxJsonPostWrapper.ajaxRouteNonAsync('club', 'trialclubacceptance', { couponCode: couponCode, clubId: clubId }, function (response) {
                $('#trialMain').html(response.html);
                if (response.miniCartHtml && response.miniCartHtml !== "") {
                    window.scrollTo(0, 0);
                    //update mini cart html
                    setCartQuantityText();
                    $('#cartSummary').html(response.miniCartHtml);
                    //show dropdown and then hide after 1.5s
                    $('.customer-nav').DropIt('showMenu', $('#mini-cart .drop-down'));
                    window.setTimeout($.proxy(function () {
                        $('.customer-nav').DropIt('hideMenu', $('#mini-cart .drop-down'));
                    }, this), 5000);
                }
            });
        }

        $(document).ready(function () {
            $('#sign-up').on('click', function () {
                if ($('#email').val() == "" ||
                    !$('#email').valid()) {
                    return;
                }
                var currAddress = $('#email').val();
                AjaxJsonPostWrapper.ajaxRoute('customerservice', 'EmailSignupHomePage', { emailAddress: currAddress }, function (result) {
                    $('#thankyouemail').toggle();
                    $('#email').val("");
                });
                return false;
            });
        });

        //always make sure cart qty is correct (mostly for when a user used back button into page
        setCartQuantityText();

    </script>



