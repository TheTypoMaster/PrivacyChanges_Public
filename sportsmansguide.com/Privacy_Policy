<script type="text/javascript">

    $(document).ready(function () {
        AjaxJsonPostWrapper.ajaxRoute("Main", "getfooterlinks", {}, function (result) {
            if (result.customerService) {
                $('#CustomerServiceFooterLinks').append(result.customerService);
            }
            if (result.shoppingResources) {
                $('#ShoppingResourcesFooterLinks').append(result.shoppingResources);
            }
            if (result.guideOutdoors) {
                $('#GuideOutdoorsFooterLinks').append(result.guideOutdoors);
            }
            if (result.ourCompany) {
                $('#OurCompanyFooterLinks').append(result.ourCompany);
            }
            if (result.buyersClub) {
                $('#BuyersClubFooterLinks').append(result.buyersClub);
            }
        });
    });
</script>

    
