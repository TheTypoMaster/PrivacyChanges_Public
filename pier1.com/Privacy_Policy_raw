					var classNames = $('#menu-panel #home-decor').attr('class').split(' ');
			        $.each(classNames, function (i, className) {
			        	if (new RegExp('^subpanel-col-').test(className)) {
							try {
				        		var parts = className.split('-');
				        		var oldColumnCount = parts.pop();
				        		var newColumnCount = parseInt(oldColumnCount) + 1;
				        		var resizedClassName = 'subpanel-col-' + newColumnCount;
				        		$('#menu-panel #home-decor').removeClass(className).addClass(resizedClassName).append('<div id="nav-promo_home-decor"></div>');
