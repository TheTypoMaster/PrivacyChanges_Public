	(function(){
		var links = document.getElementsByTagName('link'),
			exists = false,
			url = encodeURI("http://demandware.edgesuite.net/aaid_prd/on/demandware.static/-/Sites-pier1_us-Library/default/v1440922445081/css/global-text-banner-150830.css");

