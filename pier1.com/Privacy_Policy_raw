			<script type="text/javascript">
				if ($('#topNavCategoryPromoTemplate_outdoor').length) {
					var classNames = $('#menu-panel #outdoor').attr('class').split(' ');
			        $.each(classNames, function (i, className) {
			        	if (new RegExp('^subpanel-col-').test(className)) {
							try {
				        		var parts = className.split('-');
				        		var oldColumnCount = parts.pop();
				        		var newColumnCount = parseInt(oldColumnCount) + 1;
				        		var resizedClassName = 'subpanel-col-' + newColumnCount;
				        		$('#menu-panel #outdoor').removeClass(className).addClass(resizedClassName).append('<div id="nav-promo_outdoor"></div>');
				        		$('#nav-promo_outdoor').loadTemplate('#topNavCategoryPromoTemplate_outdoor', null);
							} catch(ex) { }
			        		return false;
			        	}
			        });
				}
			</script>
