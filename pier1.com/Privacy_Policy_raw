<script>
$(document).ready(function(){
    if ( app && app.customer ) {
        if (app.customer.previousDevice && app.customer.previousDevice == 'mobile') {
            $('#switch-view').show();
        }
    }
});
    
// Apply sale and New Classes to description text on sale and new tiles
    $( "span.lnd-desc:contains('New')" ).addClass('New');
    $( "span.lnd-desc:contains('%')" ).addClass('sale');
    $( "span.lnd-desc:contains('$')" ).addClass('sale');
    $( "span.lnd-desc:contains('Sale')" ).addClass('sale');
    $( "span.lnd-desc:contains('Clearance')" ).addClass('sale');
    
   // Dynamic Tile Script
   $("li.dynamic").hide('fast');

   $('.TopImage').each(function () {
      var $ti = $(this);
      $.ajax({
         url: $ti.closest('a').attr('href') + '&sz=1&source=quicklook&format=ajax',
         dataType: 'html',
         success: function (response) {

            var $image = $(' .product-image a img', response).eq(0),
               src = $image.attr('src');
            if (!src) return false;
            src = src.replace(/(.*)sw=\d*&sh=\d*/, '$1sw=' + pis + '&sh=' + pis);
            $('<img src="' + src + '">').addClass('tile-border').appendTo($ti).parent().addClass('stay');
            $(".TopImage.stay").parent().parent().show('fast');
         }
      });
   });
   // End Dynamic Tile Script

   // Mobile IMAX
   if ( $('.pt_product').length && $.cookie('mimax-storenum')) {
      $('span', '.product-number').each(function () {
         var skuArr = $.trim($(this).text()).match(/\d+/),
            sku = skuArr[0] || '';

         if (!sku) return;
         $(this).html('<a href="http://mimax/#sku=' + sku + '">' + sku + '</a>');
      });
   }
   // End Mobile IMAX

   // Don't know what this is about.
   function formatDate(d) {
         function addZero(n) {
            return n < 10 ? '0' + n : '' + n;
         }

         return d.getFullYear() + "-" + addZero(d.getMonth() + 1) + "-" + addZero(d.getDate()) + " " +
            addZero(d.getHours()) + ":" + addZero(d.getMinutes()) + ":" + addZero(d.getSeconds());
      }
      // End - Don't know what this is about.

   // E-mail Sign-up
   if (document.forms["EmailFooter"] || document.forms["EmailCapture"]) {
       
      // Form validation and Ajax submission for e-mail sign-up FOOTER
      function validatefooterForm() {
         var x = document.forms["EmailFooter"]["EMAIL_ADDRESS_"].value;
         var atpos = x.indexOf("@");
         var dotpos = x.lastIndexOf(".");
         if (atpos < 1 || dotpos < atpos + 2 || dotpos + 2 >= x.length) {
            $('#footeremailsign > #EMAIL_ADDRESS_').css('border', '2px solid red');
            return false;
         }
      }
       
      // Form validation and Ajax submission for e-mail sign-up modal
      function validateemailForm() {
        var x = document.forms["EmailCapture"]["EMAIL_ADDRESS_"].value;
        var atpos = x.indexOf("@");
        var dotpos = x.lastIndexOf(".");
        if (atpos < 1 || dotpos < atpos + 2 || dotpos + 2 >= x.length) {
            $('.takeover-address').css('border', '2px solid red').val('PLEASE PROVIDE AN EMAIL!!');
            return false;
        }
      }

      $('form[name="EmailFooter"]').submit(function (event) {
         $.ajax({
            type: "POST",
            url: $(this).attr('action'),
            data: $(this).serialize(),
            complete: function () {
               $('#footeremailsign').replaceWith('Thanks for signing up! Watch your email for a 10% off offer in as soon as 7 days.');
            }
         });
         return false;
      });
       
      $('form[name="EmailCapture"]').submit(function (event) {
        event.preventDefault();
         $.ajax({
            type: "POST",
            url: $(this).attr('action'),
            data: $(this).serialize(),
            complete: function () {
                $('#takeover-sign-submit, .takeover-address, .takeover-go-away').hide();
                $('.head-title').html('Thanks!').css({});
                $('.sub-title').html('Watch your email for a 10% off<br>offer in as soon as 7 days.').css({});
            }
        });
    });

      var emaildate = new Date();
      if (document.forms["EmailCapture"]) document.forms["EmailCapture"]["ONLINE_CUSTOMER_DATE"].value = formatDate(emaildate);
      if (document.forms["EmailFooter"]) document.forms["EmailFooter"]["ONLINE_CUSTOMER_DATE"].value = formatDate(emaildate);
   }
    
   // End E-mail Sign-up

   // Show or Hide Mobile Toggle Switch
   if (app && app.customer && app.customer.previousDevice == 'mobile') {
      $('#switch-view').css('display', 'inline-block');
   }
   // End Show or Hide Mobile Toggle Switch

   //5.10.2013 - Function to open a page in a modal window
   $('.modalLink').click(function (e) {
      e.preventDefault();
      var opts = $.extend(true, {}, app.dialog.settings, {
         width: 500,
         height: 'auto',
         draggable: false
      });
      app.dialog.open({
         url: this.href,
         options: opts
      });
   });
   // End 5.10.2013 - Function to open a page in a modal window
    
  
    
    
</script>

