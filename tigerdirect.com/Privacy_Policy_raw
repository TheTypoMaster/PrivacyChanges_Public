<script language="JavaScript" type="text/JavaScript">
    function getElement(elementName) {
        if (document.getElementById && document.getElementById(elementName)) {
            return document.getElementById(elementName);
        }
        else if (document.all && document.all(elementName)) {
            return document.all(elementName);
        }
        else if (document.layers && document.layers[elementName]) {
            return document.layers[elementName];
        } else {
            return false;
        }
    }

    function CountdownTimer2(mon, dy, yr, hr, min) {
        curr_date = new Date();
        target_date = new Date(yr, mon - 1, dy, hr, min, 00);

        GMTOffset = curr_date.getTimezoneOffset() - 300  // 240 is the EST Offset

        diff = new Date(target_date - curr_date - (GMTOffset * 60000));

        if (diff > 0) {
            days = (diff - (diff % 86400000)) / 86400000;
            hours = (diff - (diff % 3600000)) / 3600000 - days * 24;
            minutes = (diff - (diff % 60000)) / 60000 - hours * 60 - days * 1440;
            seconds = (diff - (diff % 1000)) / 1000 - minutes * 60 - hours * 3600 - days * 86400;
            if (days < 10) { days2 = "0" } else { days2 = "" }
            if (hours < 10) { hours2 = "0" } else { hours2 = "" }
            if (minutes < 10) { minutes2 = "0" } else { minutes2 = "" }
            if (seconds < 10) { seconds2 = "0" } else { seconds2 = "" }

            output = "Time Left: " + days2 + days + " : " + hours2 + hours + " : " + minutes2 + minutes + " : " + seconds2 + seconds;
            document.getElementById("countdown").innerHTML = output;
            setTimeout("CountdownTimer2(" + mon + "," + dy + "," + yr + "," + hr + "," + min + ")", "1000");
        }
        else {
            output = "";
        }
    }
			
			
</script>
<script language="JavaScript" type="text/JavaScript">
		function GetLeftPos(){
			var dp
			var winW = 0, winH = 0;
			if (parseInt(navigator.appVersion)>3) {
			if (navigator.appName=="Netscape") {
			 winW = window.innerWidth;
			 }
			 if (navigator.appName.indexOf("Microsoft")!=-1) {
			  winW = document.body.offsetWidth;
			 }
			}
			//dp = (winW/2)+383;   //Old Coordinates for centered website
			
				dp = 780;
			  
			return dp;
		}
		
		function RefreshNav(){
			document.getElementById("layer1").style.left = GetLeftPos();
		}

		position = "<div id=\"layer1\">"


		document.write(position)
		
		function echeck(str) {
		
				var at="@";
				var dot=".";
				var lat=str.indexOf(at);
				var lstr=str.length;
				var ldot=str.indexOf(dot);
				if (str.indexOf(at)==-1){
				   alert("Invalid E-mail ID");
				   return false;
				}
		
				if (str.indexOf(at)==-1 || str.indexOf(at)==0 || str.indexOf(at)==lstr){
				   alert("Invalid E-mail ID");
				   return false;
				}
		
				if (str.indexOf(dot)==-1 || str.indexOf(dot)==0 || str.indexOf(dot)==lstr){
					alert("Invalid E-mail ID");
					return false;
				}
		
				 if (str.indexOf(at,(lat+1))!=-1){
					alert("Invalid E-mail ID");
					return false;
				 }
		
				 if (str.substring(lat-1,lat)==dot || str.substring(lat+1,lat+2)==dot){
					alert("Invalid E-mail ID");
					return false;
				 }
		
				 if (str.indexOf(dot,(lat+2))==-1){
					alert("Invalid E-mail ID");
					return false;
				 }
				
				 if (str.indexOf(" ")!=-1){
					alert("Invalid E-mail ID");
					return false;
				 }
		
				 return true;
			}
		
		function checksubmit()
		{
		   if (document.sweep.email.value == "")
		   {
			  alert("Please enter your email address")
			  document.sweep.email.focus();
			  return false;
		   }
		   if (document.sweep.fullname.value == "")
		   {
			  alert("Please enter your name")      
			  document.sweep.fullname.focus();
			  return false;
		   }
		   if (document.sweep.zipcode.value == "")
		   {
			  alert("Please enter your zip code");
			  document.sweep.zipcode.focus();
			  return false;
		   }   
		   return echeck(document.sweep.email.value);
		}
		
		function MM_openBrWindow(theURL,winName,features) { 
		  window.open(theURL,winName,features);
		}
		function selOpt(obj, val) {
		   for (var i=0; i<obj.options.length; i++) {
			  if (obj.options[i].value==val) {
					obj.options[i].selected = true;
			  }
		   }
		}
		function process() {
		   return checksubmit();
		}
</script>

