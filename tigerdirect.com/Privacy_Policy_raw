// [ Form handling ]
function validateFormAjax_mast(formName, $clicked, formSuccess){
	// [ Elements ]
	var $form	= $('#' + formName);
	
	// [ Fields ]
	var $form_fullname		= $form.find('.form_input-fullName');
	var $form_email			= $form.find('.form_input-email');
	var $form_zipCode		= $form.find('.form_input-zipCode');
	var $form_mobilePhone	= $form.find('.form_input-mobileNumber');
	
	// [ Field default values ]
	var form_fullname_error		= 'Enter your full name';
	var form_email_error		= 'Enter a valid email';
	var form_zipCode_error		= 'Enter a valid zip code';
	var form_mobilePhone_error	= 'Enter your mobile number';
	
	// [ Validation and reporting variables ]
	var form_errors = 0;
	
	// [ Disable the button ]
	$clicked.defText = $clicked.text(); // [ Store the button's default value ]
	$clicked.prop({
			disabled : 'disabled'
		})
		.children('span')
		.text('Processing');
	
	// [ Check for existing fields ]
	if ($form_fullname.length) {
		// [ Check full name ]
		if ($form_fullname.val() == null || $form_fullname.val() == '' || $form_fullname.val() == $form_fullname_error) {
			// [ Missing or invalid ]
			form_errors ++;
			$form_fullname.val(form_fullname_error);
			form_errorStyler($form_fullname);
		}
	}
	
	if ($form_email.length) {
		// [ Check email ]
		if ($form_email.val() == null || $form_email.val() == '' || $form_email.val() == form_email_error || check_email($form_email.val()) == false) {
			// [ Missing or invalid ]
			form_errors ++;
			$form_email.val(form_email_error);
			form_errorStyler($form_email);
		}
	}
	
	if ($form_zipCode.length) {
		// [ Check zip code ]
		if ($form_zipCode.val() == null || $form_zipCode.val() == '' || $form_zipCode.val() == form_zipCode_error || check_zipCode($form_zipCode.val()) == false) {
			// [ Missing or invalid ]
			form_errors ++;
			$form_zipCode.val(form_zipCode_error);
			form_errorStyler($form_zipCode);
		}
	}
	
	if ($form_mobilePhone.length) {
		// [ Check mobile phone number ]
		if ($form_mobilePhone.val() == null || $form_mobilePhone.val() == '' || $form_mobilePhone.val() == form_mobilePhone_error || check_phone($form_mobilePhone.val()) == false) {
			// [ Missing or invalid ]
			form_errors ++;
			$form_mobilePhone.val(form_mobilePhone_error);
			form_errorStyler($form_mobilePhone);
		}
	}
	
	if (form_errors > 0) {
		// [ Enable the button for re-submission ]
		$clicked.prop({
			disabled	: ''
		})
		.children('span')
		.html($clicked.defText);
			return;
	} else {
		if ($form_mobilePhone.length) {
			$form_mobilePhone.val('1' + $form_mobilePhone.val());
		}
		submitTheForm_mast($form, $clicked, formSuccess);
	}
}
 
function submitTheForm_mast($theForm, $clicked, formSuccess) {	
	// [ Submit the form information or reset if error ]
	$.post($theForm.attr('action'), $theForm.serialize(), function(data){
		// [ Successful ]
		formSuccess();
	}).fail(function(xhr, status, error) {
		alert("An error has occured. Please reload the page and try again.");
	});

}

function form_errorStyler($formElement) {
	var errorClassName = 'form_error';
	
	$formElement.addClass(errorClassName);
	$formElement.focus(function(e) {
		if ($formElement.hasClass(errorClassName)) {
			$formElement.removeClass('form_error');
			$formElement.val('');
		}
	});
}

function check_zipCode(str) {
	var regex_us = new RegExp(/(^\d{5}$)|(^\d{5}-\d{4}$)/);
    var regex_ca = new RegExp(/^[ABCEGHJKLMNPRSTVXY]\d[ABCEGHJKLMNPRSTVWXYZ]( )?\d[ABCEGHJKLMNPRSTVWXYZ]\d$/i);
	
    if (regex_ca.test(str) || regex_us.test(str)) {
		// [ Successful ]
        return true;
	} else {
		// [ Invalid ]
		return false;
	}
		
}

function check_phone(str) {
	var stripped = str.replace(/[\(\)\.\-\ ]/g, '');     
	
	if (isNaN(parseInt(stripped))) {
		// [ Make sure it's all numbers ]
		return false;
	} else {
		// [ Successful ]
		return true;
	}
}

function check_email(str) {
	var at="@";
	var dot=".";
	var lat=str.indexOf(at);
	var lstr=str.length;
	var ldot=str.indexOf(dot);
	if (str.indexOf(at)==-1){
	   //alert("Invalid E-mail ID");
	   return false;
	}

	if (str.indexOf(at)==-1 || str.indexOf(at)==0 || str.indexOf(at)==lstr){
	   //alert("Invalid E-mail ID");
	   return false;
	}

	if (str.indexOf(dot)==-1 || str.indexOf(dot)==0 || str.indexOf(dot)==lstr){
		//alert("Invalid E-mail ID");
		return false;
	}

	 if (str.indexOf(at,(lat+1))!=-1){
		//alert("Invalid E-mail ID");
		return false;
	 }

	 if (str.substring(lat-1,lat)==dot || str.substring(lat+1,lat+2)==dot){
		//alert("Invalid E-mail ID");
		return false;
	 }

	 if (str.indexOf(dot,(lat+2))==-1){
		//alert("Invalid E-mail ID");
		return false;
	 }
	
	 if (str.indexOf(" ")!=-1){
		//alert("Invalid E-mail ID");
		return false;
	 }

	 return true;
}

-->
</script>


