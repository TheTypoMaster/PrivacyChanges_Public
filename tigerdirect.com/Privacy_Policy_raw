<script type="text/javascript">

google.load('search', '1');
var completionObject;

function onLoad() {
	var cse_id = '016974591766332755208:or7nnpuoulc';
	completionObject = google.search.CustomSearchControl.attachAutoCompletionWithOptions(cse_id, document.getElementById("tigerv2_searchform"), "searchFRM");
	
	$("#tigerv2_searchform").blur(function(){
	  if (completionObject.getInputQuery() === ''){
		 completionObject.prefillQuery('Search by Keyword or Item #');
	  };
	});
}
//if completionObject not loaded before focus
$("#tigerv2_searchform").focus(function(){
	if (!completionObject) {
		this.value = '';
	} else if (completionObject.getInputQuery() === 'Search by Keyword or Item #'){
	 completionObject.prefillQuery('');
  };
});
google.setOnLoadCallback(onLoad);
	
</script>


