<script type="text/javascript">
var _shoprunner_com = new Object();
(function(){
    _shoprunner_com.version = 2.0;
    _shoprunner_com.enabled = true;
    _shoprunner_com.retailerID = "TIGERDIRECT";
    _shoprunner_com.environmentID = 2;
    _shoprunner_com.sendOrderConfirm = true;

    var sr_cartid  = ($.cookie('CartId')==undefined) ? "" : $.cookie('CartId');
	var sr_srccode = ($.cookie('SRCCODE')==undefined) ? "" : $.cookie('SRCCODE');  
	var sr_session = ($.cookie('SessionId')==undefined) ? "" : $.cookie('SessionId'); 

    _shoprunner_com.loginValidationURL = function(token, success, failure){
	  try{
		$.ajax("https://orders.tigerdirect.com/ShopRunner/Login?cartId=" + sr_cartid + "&srcCode=" + sr_srccode + "&sessionId=" + sr_session + "&srtoken=" + token,
			{dataType : "jsonp"})
			.done(function(data){
				
				if(data && data.cartId && data.cartDb){
					$.cookie("CartSave", "CartId=" + data.cartId + "&Cart=" + data.cartDb, { expires: 5, path: '/' });
					$.cookie("CartId", data.cartId, { path: '/' });
				}
				
				success();
			})
			.fail(function(){
				failure();
			});
		  }
		  catch(e)
		  {}
	}

	_shoprunner_com.checkout = new Object();
	_shoprunner_com.checkout.enabled = false;
	_shoprunner_com.checkout.singleProductBuyNowEnabled = false;
	_shoprunner_com.checkout.cartBuyNowEnabled = false;
	_shoprunner_com.checkout.partnerAPIEndPoint = '';

	_shoprunner_com.partner_contentURL = "/javascript/ShopRunner";

	_shoprunner_com.prefix = window.parent.document.location.protocol + "//";
    	if (_shoprunner_com.enabled) {
       		switch (_shoprunner_com.environmentID) {
            		case 1:
                	_shoprunner_com.sr_jsContentURL = _shoprunner_com.prefix + "staging-content.shoprunner.com/staging";
                	break;
            		case 2:
                	_shoprunner_com.sr_jsContentURL = _shoprunner_com.prefix + "content.shoprunner.com";
                	break;
            	default:
                	_shoprunner_com.sr_jsContentURL = _shoprunner_com.prefix + "staging-content.shoprunner.com/staging";
                	break
        	}
    	}

	var a = document.createElement("link");
        a.href = "/javascript/ShopRunner/ShopRunner.min.css?v=8";
        a.type = "text/css";
        a.rel = "stylesheet";
        document.getElementsByTagName("head")[0].appendChild(a);

    	_shoprunner_com.docReady = false;

    	$(function() {
        	_shoprunner_com.docReady = true;
        	if (typeof (sr_$) !== "undefined") {

			   sr_$.delayRun();
			   
			   if (typeof sr_$ !== "undefined" && typeof _gaq !== "undefined" && typeof _gaq.push === "function") {

					sr_$.events.addListener("onSignOut", function() {
						_gaq.push(["_trackEvent", "ShopRunner", "Logout", "unknown"]);
					});
					
					// detect login
					var isSignedIn = sr_$.member.signed_in;
					sr_$.events.addListener("onRefresh", function() {
						if (!isSignedIn && sr_$.member.signed_in) {
							_gaq.push(["_trackEvent", "ShopRunner", "Login", "unknown"]);
						}
					});
				}
			   
        	}
    	});		
	
}())
