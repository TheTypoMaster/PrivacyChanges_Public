    function openBMLWindow(url,height) {
        var iwidth=500, iheight=height;
        {
            popUpWin =window.open(url,'NewWindow','toolbar=0,location=0,directories=0,status=0,menubar=0,scrollbars=1,resizable=0,width='+(iwidth+15)+',height='+(iheight+40));
        }
    }
    function openPayPalWindow(url,height) {
        var iwidth=850, iheight=height;
        {
            popUpWin =window.open(url,'NewWindow','toolbar=0,location=0,directories=0,status=0,menubar=0,scrollbars=1,resizable=0,width='+(iwidth+15)+',height='+(iheight+40));
        }
    }
    function openInvestWin(url) {
        popupWin = window.open(url,'affiliate','height=600,width=850,fullscreen=no,menubar=no,location=no,resizable=yes,scrollbars=yes,titlebar=yes,toolbar=no');
    }
</SCRIPT>




