	function refresh_tooltips(){
	$('.tooltips').each(function(){
	  $(this).qtip({
	        content: $(this).attr('alt'),
	        position: {
	            my: 'top middle', 
	            at: 'bottom middle'
	        },
	        style: { 
	            tip: true,
	            classes: 'ui-tooltip-mt'
	        }
	  });
	})
	}
	refresh_tooltips();
</script>
<script type="text/javascript">
	jQuery(document).ready(function(){
		$('.mt-container').css('min-height', $(window).height()-470);
		$.waypoints.settings = { scrollThrottle: 200 };
		
		$('.mt-container').waypoint(function(event, direction) {
	   			if (direction === 'down') {
					$('#mt-scroll_to_top').fadeIn();
		 		} else {
					$('#mt-scroll_to_top').fadeOut();
		   		}
			event.stopPropagation();
	 	}, { offset: 0} );
		
	});
	
</script>

