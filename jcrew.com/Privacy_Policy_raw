<script>
function isEmailValid(formObj) {
        var formValid=true;
        var errorMsg='';
        var erorDiv=document.getElementById('erorMsg');
        if(formObj.emailFlag.checked==true){
        if(!validateEmail(formObj.emailAddress.value)){errorMsg='Please enter a valid email address';formValid=false;}}
        if(errorMsg!='')erorDiv.innerHTML=errorMsg;
        return formValid;
}
