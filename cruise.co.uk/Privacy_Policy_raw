<script type="text/javascript">
    
        $(document).ready(function() {
                    if ($(window).width() > 1260) {
				$("body").addClass("bodyFarRight");
                ajaxLoader.load("/API-TPL/v3/900/FarRightNavigation//", "FarRightNavigation-2", false, function(){
                    eysysAjaxLoader.setFarRightNavReady(true);
                });     
        } else {
                    $("#sidebar_farright").attr("id", "");
                    $("#FarRightNavigation-2 > div").css("border", "none");
                    $("#FarRightNavigation-2 > div").hide();
                    eysysAjaxLoader.centerMainBody();
    }
 });
</script>
