<script type="text/javascript">

jQuery(window).ready(function() {

    if ((omni.strCurrentPageName && omni.strCurrentPageName.indexOf("Factory") > -1) || (window.location.href.indexOf("cid=48620") > -1)){ //show only on Factory pages

        var bannerContainer = jQuery('.topNavGFOLContainer'),
