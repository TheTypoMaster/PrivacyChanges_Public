<script type="text/javascript">
if (window.location.pathname.indexOf('shopping_bag.do') > 0) {
  var wcdContinuitySnackBar ='';
  jQuery.fn.wcdContinuityTest = {
    init: function(){
      // check if user is logged in
      var wcdContinuityPersonalizationData = personalizationService.model.personalizationData;
      if (wcdContinuityPersonalizationData == null || wcdContinuityPersonalizationData.personalizationInfoV1.userContext.isAnonymousUser == "true") {
        jQuery(function() {
          jQuery.fn.wcdContinuityUnrecognized.init();
        });
      } else {
        // no action taken
      }
    }
  }
  var curBreakpoint = '';

  jQuery.fn.wcdContinuityUnrecognized = {
    message: 'Save & access your bag from any device by signing in.',
    button: 'SAVE BAG',
    close: '',
    init: function(){
      // check if bag is not empty
      var bagContainer = jQuery('.activeBagContainer .ng-isolate-scope')[0];
      var bagCount = jQuery(bagContainer).attr('count');
      if (bagCount != 0) {
        // get first item
        var bagItems = jQuery('.lineItemsContainer div.ng-scope[ng-repeat="item in items"]');
        var firstBagItem = bagItems[0];
        setTimeout(jQuery.fn.wcdContinuitySnackbarSet.init(firstBagItem), 1500);
        /*
        var nua = navigator.userAgent;
        var is_android = ((nua.indexOf('Mozilla/5.0') > -1 && nua.indexOf('Android ') > -1 &&     nua.indexOf('AppleWebKit') > -1) && !(nua.indexOf('Chrome') > -1));
        if (is_android == true) {
          jQuery(body).css({ 'background-color': 'yellow' });
        }
        */
      }
    }
  }

  jQuery.fn.wcdContinuityRecognized = {
    message: 'Save & access your bag from any device by signing in.',
    button: 'SAVE BAG',
    close: '',
    init: function(){
      
    }
  }

  jQuery.fn.wcdContinuitySnackbarSet = {
    largeInsert: '<div class="continuity_message_bar continuity_message_full_size"><div class="snackbar_wrap"><div class="continuity_graphic"></div><div class="closeHitArea"><button class="xGrayIcon"></button></div><div class="continuity_block"><div class="continuity_text heading-d_universal">' + jQuery.fn.wcdContinuityUnrecognized.message + '</div><button class="continuity_button label-a_universal">' + jQuery.fn.wcdContinuityUnrecognized.button + '</button></div></div></div>',
    smallInsert: '<div class="continuity_message_bar continuity_message_small_size"><div class="snackbar_wrap"><div class="continuity_graphic"></div><div class="closeHitArea"><button class="xGrayIcon"></button></div><div class="continuity_block"><div class="continuity_text heading-d_universal">' + jQuery.fn.wcdContinuityUnrecognized.message + '</div></div><button class="continuity_button label-a_universal">' + jQuery.fn.wcdContinuityUnrecognized.button + '</button></div></div>',
    init: function(firstBagItem) {
      // check breakpoint
      var curBreakpoint = responsiveService.model.currentBreakpoint;
      if (curBreakpoint != null) {
        if (curBreakpoint.name == 'xlarge' || curBreakpoint.name == 'large') {
          var continuityInsert = jQuery.fn.wcdContinuitySnackbarSet.largeInsert;
          var continuitySnackbar = '.continuity_message_full_size';
          jQuery(firstBagItem).after(continuityInsert);
          wcdContinuitySnackbar = continuitySnackbar;
        } else {
          var continuityInsert = jQuery.fn.wcdContinuitySnackbarSet.smallInsert;
          var continuitySnackbar = '.continuity_message_small_size';
          jQuery(firstBagItem).after(continuityInsert);
          wcdContinuitySnackbar = continuitySnackbar;
        }
      } else {
        if (clientBrowser != undefined) {
          // fallback code if responsiveService is not loading
          // check to see if IE 9 or below
          if (clientBrowser.isIE == true && clientBrowser.isIE10Up == false) {
            if(jQuery(window).width() < 768) {
              var continuityInsert = jQuery.fn.wcdContinuitySnackbarSet.smallInsert;
              var continuitySnackbar = '.continuity_message_small_size';
              jQuery(firstBagItem).after(continuityInsert);
              wcdContinuitySnackbar = continuitySnackbar;
            } else {
              var continuityInsert = jQuery.fn.wcdContinuitySnackbarSet.largeInsert;
              var continuitySnackbar = '.continuity_message_full_size';
              jQuery(firstBagItem).after(continuityInsert);
              wcdContinuitySnackbar = continuitySnackbar;
            }
          } else {
            if (window.matchMedia("(min-width: 768px)").matches) {
              var continuityInsert = jQuery.fn.wcdContinuitySnackbarSet.largeInsert;
              var continuitySnackbar = '.continuity_message_full_size';
              jQuery(firstBagItem).after(continuityInsert);
              wcdContinuitySnackbar = continuitySnackbar;
            } else {
              var continuityInsert = jQuery.fn.wcdContinuitySnackbarSet.smallInsert;
              var continuitySnackbar = '.continuity_message_small_size';
              jQuery(firstBagItem).after(continuityInsert);
              wcdContinuitySnackbar = continuitySnackbar;
            }
          }
        } else {
          if(jQuery(window).width() > 768) {
            var continuityInsert = jQuery.fn.wcdContinuitySnackbarSet.largeInsert;
            var continuitySnackbar = '.continuity_message_full_size';
            jQuery(firstBagItem).after(continuityInsert);
            wcdContinuitySnackbar = continuitySnackbar;
          } else {
            var continuityInsert = jQuery.fn.wcdContinuitySnackbarSet.smallInsert;
            var continuitySnackbar = '.continuity_message_small_size';
            jQuery(firstBagItem).after(continuityInsert);
            wcdContinuitySnackbar = continuitySnackbar;
          }
        }
      }
      setTimeout(jQuery.fn.wcdContinuitySnackbarFadeIn.init, 1500);
    }
  }

  jQuery.fn.wcdContinuitySnackbarFadeIn = {
    init: function() {
      if (wcdContinuitySnackbar.indexOf('full') > -1) {
        jQuery('.continuity_message_full_size').css({
          height: '175px'
        });
        // set iframe
        setTimeout(jQuery.fn.wcdContinuityIconAnimation.init, 400)
      } else {
        jQuery('.continuity_message_small_size').css({
          height: '138px'
        });
      }
      // add click functionality to buttons
      jQuery('.closeHitArea').click(function(){
        jQuery.fn.wcdContinuitySnackbarFadeOut.init(wcdContinuitySnackbar);
      });
      jQuery('.continuity_button').click(function(){
        gap.SigninOverlay.openModal();
      });
      jQuery('.continuity_graphic').click(function(){
        jQuery.fn.wcdContinuityIconAnimation.init;
      });
    }
  }

  jQuery.fn.wcdContinuitySnackbarFadeOut = {
    init: function(continuitySnackbar) {
      if (continuitySnackbar.indexOf('full') > -1) {
        jQuery(continuitySnackbar).css({
          height: '0px',
          'box-shadow': 'none'
        });
      } else {
        jQuery(continuitySnackbar).hide();
      }
      

    }
  }

  jQuery.fn.wcdContinuityIconAnimation = {
    init: function(){
      var wcdContinuityAnimationIframe = '<iframe id="continuity_animation" src="/Asset_Archive/GPWeb/content/0009/997/635/assets/ContinuityAnimation.html" width="150" height="106" />';
      jQuery('.continuity_graphic').html('');
      jQuery('.continuity_graphic').append(wcdContinuityAnimationIframe);
    }
  }
  
}


