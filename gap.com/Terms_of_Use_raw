			// Toddler/Infant Boy categories
			tboyCategoriesHideFacet : new Array('76759', '76754', '65281', '65283', '85104', '81112', '85064', '70127', '63897', '77431', '17850', '70173', '84824', '84491', '82572', '82569', '80386', '80390', '80396', '42558', '8788', '7142', '74004', '7139', '78580', '7143', '7185', '80899', '76754', '84336', '86610', '81589', '55773', '65261', '70127', '63897', '77431', '17850', '76759', '50728', '65236', '84823', '8777', '83376', '85214', '85213', '35792', '6395', '6392', '88871', '89335', '89348', '84830', '89903', '88920', '89697', '36691', '17851', '91809', '71988', '95575', '95598', '95496', '95679', '95682', '95684', '95689', '95692', '95693', '95697', '93306', '93305', '93304', '95184', '91491', '95445', '91808', '82562', '84674', '89902', '95423', '96777', '96773', '96457', '93308', '6365', '97213', '97211', '93309', '97795', '97792', '97798', '97801', '97804', '91809', '71988', '98441', '94883', '98250', '98095', '78037', '99339', '99648', '99656', '99670', '98567', '98570', '98573', '99838', '97714', '1000238', '1000638', '1000641', '1000645', '1000649', '1000653', '1000656', '1000658', '1000661', '98121', '1001212', '1001576', '1001579', '1002230', '1002430', '1002256', '1002440', '1002445', '1002337', '1002338', '1002339', '1002345', '1001384', '1001460', '1001385', '1001459', '1002449', '1003472', '1003341', '1003223', '1004898', '1004822', '1005561', '1005649', '1005946', '1005933', '1005763', '1005903', '1005883', '1005903', '1007093', '1007162', '1007164', '1007026', '1007812', '1007814', '1007812', '1007814', '1008323', '1008262', '1007815', '1008477', '1008893', '1008894', '1009184', '1009496', '1009362', '1009218', '95690', '1009612', '1009614', '1009993', '1009994', '1009996', '1010126', '1010127', '1010128', '6359', '1010287', '1010290', '1010419', '8787', '9175', '1011148', '1011147', '1011273', '1011151', '1011194', '1002438', '1002260', '35855', '1009219', '1011504', '1012102', '1012105', '1012439', '1011292', '1012992', '1013109', '1013110', '1012