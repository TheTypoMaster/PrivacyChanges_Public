	(function() {
		if (gidLib.getCookie("s_pers") == "") {
			var p = gidLib.setCookieParamsHelper("s_pers");
			gidLib.setCookie("s_pers", null, p.exp, p.path, p.domain, p.isSecure);
		}
	})();
