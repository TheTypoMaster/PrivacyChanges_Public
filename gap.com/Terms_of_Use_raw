jQuery.wcdSharedOverride_viewPortSetter = function() {
  var wcdViewPortNavigator = navigator.userAgent;
  if( wcdViewPortNavigator.indexOf("iPad") > -1 ) {
    document.write('<meta name = "viewport" content = "width = 1024">');
  }
}
