            taConfig: {
                        serviceRequestMask:14,
                        On:true,
                        MinChar:3,
                        MaxItems:10,
                        Path:"http://suggest.expedia.com/hint/es/v1/ac",
                        Locale:"en_US",
                        abTest:"842.1|808.0",
                        version: 0,
                        lob: "ACTIVITIES",
                        closeText:"Close",
                        contiueText:"Continue typing to refine search"
            },

            errorMessages: {
                destination: "Please complete the highlighted fields below.",
                startDate: dateFormatErr,
                endDate: dateFormatErr,
                bookingDateRange:"Your start date/time must occur before your end date/time. Please check your dates.",
