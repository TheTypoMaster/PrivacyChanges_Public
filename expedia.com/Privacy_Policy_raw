<script type="text/javascript">

    $(function() {
        $("input:radio[id=cruiseType]")[0].checked = true;
        initControls();
        publishing.wizard.lobforms.cruise({
            defaultDurationDays: 1,
            minPurchaseDate: publishing.util.formattedDate('DD/MM/YYYY', 1),
            maxPurchaseDate: publishing.util.formattedDate('DD/MM/YYYY', 331),
            errorMessages: {
                destination: ""
                }
        });
    });

    $("input[id=cruiseType]").change(initControls);
    $("#cruise-ds").change(updateControls);
    $("#cruise-drd").change(updateControls);
    $("#cruise-cl").change(updateControls);
    $("#cruise-dr").change(updateControls);
    $("#cruise-dp").change(updateControls);

    var rootUrl = "/cruiseSearchCriteria/getFilteredOptions?";

    function initControls(){
        updateDropDownLists(true);
    }

    function updateControls(){
        updateDropDownLists(false)
    }

    function updateDropDownLists(init) {
        var targetUrl = rootUrl;

        targetUrl += "aDestination=";
        if (init == false) {
            targetUrl += $("#cruise-ds").val();
        }
        targetUrl += "&aSubDestination=";
        targetUrl += "&aDeparturePort=";
        if (init == false) {
            targetUrl += $("#cruise-dp").val();
        }
        targetUrl += "&aDuration=";
        if (init == false) {
            targetUrl += $("#cruise-dr").val();
        }
        targetUrl += "&aMonth=";
        if (init == false) {
            targetUrl += $("#cruise-drd").val();
        }
        targetUrl += "&aMonthTo=";
        targetUrl += "&aDay=";
        targetUrl += "&aDayTo=";
        if ($("#cruise-drd").val() != ""){
            targetUrl += "&aDayRange=999";
        } else {
            targetUrl += "&aDayRange=";
        }
        targetUrl += "&aDayRangeDay=";
        targetUrl += "&aCruiseLine=";
        if (init == false) {
            targetUrl += $("#cruise-cl").val();
        }
        targetUrl += "&aShip=";
        targetUrl += "&aPromotion=";
        targetUrl += "&aWebID=5859";                                    
        targetUrl += "&aProductScope=";
        targetUrl += "&aCultureName=en-CA";                             
        targetUrl += "&aCategory=" + $("input[id=cruiseType]:checked").val();
        targetUrl += "&aSubdomain=cruises";                             
        targetUrl += "&aNumOfAdult=" + $("#cruise-numAdults").val();
        targetUrl += "&aNumOfChild=" + $("#cruise-numChildren").val();

        $.ajax({
            url: targetUrl,
            type: "GET",
            dataType: "json",
            success: function(data) {
                PopulateLists(data);
            },
        });
    }

    function PopulateLists(data){
        PopulateDDL($("#cruise-ds"), data.destination);
        PopulateDDL($("#cruise-drd"), data.month);
        PopulateDDL($("#cruise-cl"), data.cruiseline);
        PopulateDDL($("#cruise-dr"), data.duration);
        PopulateDDL($("#cruise-dp"), data.departureport);
    }

    function PopulateDDL(ddl, data) {
        var group = "";
        var x = 0;

        var newOptions = "";
        for (var i=0; i<data.length; i++){
            if (data[i].group != null && data[i].group != ""){
                if (group != data[i].group) {
                    if (group != ""){
                        newOptions += "</optgroup>";
                    }
                    group = data[i].group;
                    newOptions += "<optgroup label=\"" + group + "\">";
                }
            }

            if (data[i].text != "") {
                newOptions += "<option value=\"" + data[i].value + "\"";
                if(data[i].selected == "1"){
                    newOptions += " selected=\"selected\"";
                }
                newOptions += ">";
                if (data[i].value != ""){
                    newOptions += data[i].text + "</option>";
                } else {
                    newOptions += "Show all" + "</option>";
                }
            }
        }
        if (group != ""){
            newOptions += "</optgroup>";
        }
        ddl.html(newOptions);
    }
</script>

                                    </div>
                                    
