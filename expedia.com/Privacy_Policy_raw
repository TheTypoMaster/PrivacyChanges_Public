<script type="text/javascript"> 

function generatedVenList(discode){

		var generatedVenList = "";
		
        var venList = {AC: "ACE Rent A Car", AD:"Advantage Rent-A-Car", AL:"Alamo Rent A Car", ZI:"Avis", ZD:"Budget", ZR:"Dollar Rent A Car", EY:"Economy Rent a Car", ET:"Enterprise", EP:"Europcar", FX:"Fox Rental Cars", ZE:"Hertz", MW:"Midway Car Rental", ZL:"National Car Rental", NU:"N√º Car", ZA:"Payless", SX:"Sixt", ZT:"Thrifty Car Rental", FF:"Firefly Car Rental", SV:"U-Save"};
		var corpDiscodeVenList = {ZI:"Avis", ZD:"Budget", ZR:"Dollar Rent A Car", ET:"Enterprise", EP:"Europcar", ZE:"Hertz", FF:"Firefly Car Rental", ZA:"Payless", ZT:"Thrifty Car Rental", AC: "ACE Rent A Car", AD:"Advantage Rent-A-Car", AL:"Alamo Rent A Car", EY:"Economy Rent a Car", ZL:"National Car Rental", NU:"N√º Car", SX:"Sixt"};
		var specDisCodeVenList = {ZI:"Avis", ZD:"Budget", ZR:"Dollar Rent A Car", ET:"Enterprise", EP:"Europcar", ZE:"Hertz", FF:"Firefly Car Rental", ZA:"Payless", ZT:"Thrifty Car Rental", AC: "ACE Rent A Car", AD:"Advantage Rent-A-Car", AL:"Alamo Rent A Car", EY:"Economy Rent a Car", FX:"Fox Rental Cars", ZL:"National Car Rental", NU:"N√º Car", SX:"Sixt", SV:"U-Save"};

		if(discode==1)
		{
			generatedVenList = '<option value="">-- Select from the list --</option>' + generatedOptionList(corpDiscodeVenList);
		}
		else if(discode==2)		
		{		
			generatedVenList = '<option value="">-- Select from the list --</option>' + generatedOptionList(specDisCodeVenList);
		}		
		else
		{						
			generatedVenList = '<option value="">No Preference</option>' + generatedOptionList(venList);
		}

		return generatedVenList;
}

function generatedOptionList(list)
{
	var options ="";
	
	$.each(list, function(key, value){
		options += "<option value=\""+key+"\">"+value+"</option>";
	});
	return options;

}


$(function(){
	
	var discode = $("#discountCode").find('[name=discode]').val();
	var vendor = $("#rentalCompany").find('[name=Renter]').val();
	$("#rentalCompany").find('select[name=Renter]').html(generatedVenList(discode));

	$("#discountCode").bind('change',function(){
		
		var discode = $("#discountCode").find('[name=discode]').val();
		var vendor = $("#rentalCompany").find('[name=Renter]').val();
		
		if($("#discountCode").find('[name=discode]').val()==1)
		{		
			$("#rentalCompany").find('select[name=Renter]').html(generatedVenList(discode));
			if(vendor!=null && vendor!=""){
			$("#rentalCompany option[value='"+vendor+"']").attr("selected","selected");
			}
			onCarVendorChangeCallback(discode,vendor);
		}
		else if($("#discountCode").find('[name=discode]').val()==2)
		{		
			$("#rentalCompany").find('select[name=Renter]').html(generatedVenList(discode));
			if(vendor!=null && vendor!=""){
			$("#rentalCompany option[value='"+vendor+"']").attr("selected","selected");
			}
			onCarVendorChangeCallback(discode,vendor);
		}
		else
		{			
			$("#rentalCompany").find('select[name=Renter]').html(generatedVenList(discode));
			if(vendor!=null && vendor!=""){
			$("#rentalCompany option[value='"+vendor+"']").attr("selected","selected");
			}
			onCarVendorChangeCallback(discode,vendor);
		}
	});

	$("#rentalCompany").bind('change',function(){
		
		var discode = $("#discountCode").find('[name=discode]').val();
		var vendor = $("#rentalCompany").find('[name=Renter]').val();
		
		onCarVendorChangeCallback(discode,vendor);
	});	
	
	onCarVendorChangeCallback(discode,vendor);
	
	//reset all discount code columns if they are disabled.
	$("#disRateCode").find('input[name=RateCode]').removeAttr("disabled");
	$("#disCouponCode").find('input[name=CouponCode]').removeAttr("disabled");
	$("#disOtherCode").find('input[name=OtherCode]').removeAttr("disabled");
	$("#disCorpCode").find('input[name=CorpDiscNum]').removeAttr("disabled");
	
})

function onCarVendorChangeCallback(discode,vendor) 
	{
		// reset all discount code fields to hidden
		$("#disRateCode").hide();
		$("#disCouponCode").hide();
		$("#disOtherCode").hide();
		$("#disCorpCode").hide();
				

		var discountConfig = getDiscountCodeConfig(vendor);
		
		if (discountConfig && discode != 0)
		{
			if (discode == 2)
			{
				if (discountConfig.rateCodeLbl)
				{
					var label = $("#disRateCodelb").html(discountConfig.rateCodeLbl);
					
					$("#disRateCode").show();
					$("#disRateCodelb").show();
				}
				if (discountConfig.couponCodeLbl)
				{
					var label = $("#disCouponCodelb").html(discountConfig.couponCodeLbl);
					
					$("#disCouponCode").show();
					$("#disCouponCodelb").show();
				}
				if (discountConfig.otherCodeLbl)
				{
					var label = $("#disOtherCodelb").html(discountConfig.otherCodeLbl);
					
					$("#disOtherCode").show();
					$("#disOtherCodelb").show();
				}
			}
			else if (discountConfig.corpCodeLbl)
			{
				var label = $("#disCorpCodelb").html(discountConfig.corpCodeLbl);
				
				$("#disCorpCode").show();
				$("#disCorpCodelb").show();
			}
		}
	}
	
var discountCodeConfig = null;
function getDiscountCodeConfig(vendor){

    if (discountCodeConfig == null)
    {
    	var rc="Rate code:";
    	var cc="Coupon code:";
    	var cd="Corporate Discount #:";
    	
        discountCodeConfig = {
            'AC' : {rateCodeLbl: rc, couponCodeLbl: cc,  corpCodeLbl: "Corporate Discount #:"},
            'AD' : {rateCodeLbl: rc, couponCodeLbl: cc,  corpCodeLbl: "FASTSTART Org ID:"},
            'AL' : {rateCodeLbl: rc, couponCodeLbl: cc, otherCodeLbl: "Alamo Insiders #:", corpCodeLbl: cd},
            'ZI' : {rateCodeLbl: rc, couponCodeLbl: cc, otherCodeLbl: "Wizard #:", corpCodeLbl: "AWD #:"},
            'ZD' : {couponCodeLbl: cc, otherCodeLbl: "Fast Break #:", corpCodeLbl: "BCD #:"},
            'ZR' : {rateCodeLbl: rc, couponCodeLbl: cc, otherCodeLbl: "Dollar Express #:", corpCodeLbl: cd},
            'EY' : {couponCodeLbl: cc, corpCodeLbl: cd},
            'ET' : {couponCodeLbl: cc, otherCodeLbl: "Enterprise Plus #:", corpCodeLbl: "Customer #:"},
            'EP' : {rateCodeLbl: rc, couponCodeLbl: cc, otherCodeLbl: "Privilege #:", corpCodeLbl: "Contract #:"},
            'FX' : {rateCodeLbl: rc, couponCodeLbl: cc},
            'ZE' : {couponCodeLbl: cc, otherCodeLbl: "Hertz #1 Club:", corpCodeLbl: "CDP:"},
            'ZL' : {couponCodeLbl: cc, otherCodeLbl: "Emerald Club ID:", corpCodeLbl: "National Contract ID:"},
            'NU' : {rateCodeLbl: rc, couponCodeLbl: cc, corpCodeLbl: "Corporate Account:"},
            'ZA' : {rateCodeLbl: rc, couponCodeLbl: cc, otherCodeLbl: "Payless Perks #:", corpCodeLbl: cd},
            'SX' : {rateCodeLbl: rc, couponCodeLbl: cc, corpCodeLbl: cd},
            'ZT' : {rateCodeLbl: rc, couponCodeLbl: cc, otherCodeLbl: "Blue Chip #:", corpCodeLbl: cd},
            'SV' : {rateCodeLbl: rc, couponCodeLbl: cc}
        };
    }
    return discountCodeConfig[vendor];
}

</script>
	    
	    


