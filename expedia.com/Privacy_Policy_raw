        <script type="text/javascript">
        $('#carSpecialEquipmentCheckboxes :input[type=checkbox]').bind( 'change', function() 
        {
            var checkedBoxes = $("#carSpecialEquipmentCheckboxes :input[type=checkbox]:checked");
            var uncheckedBoxes = $("#carSpecialEquipmentCheckboxes :input[type=checkbox]:not(:checked)");
            var maxMessage = $('#max6EquipOptionsMessage');            
                
            if ( checkedBoxes.length == 6 && uncheckedBoxes.length > 0)
            {
                uncheckedBoxes.each(function() 
                {
                    $(this).attr('disabled', 'disabled'); // disable checkbox
                    $(this).next().css("color", "#666");  // visibly disable label for checkbox
                });
                maxMessage.show();
            }
            else if ( checkedBoxes.length == 5 && uncheckedBoxes.length > 1 )
            {
                uncheckedBoxes.each(function() 
                {
                    $(this).removeAttr('disabled');
                    $(this).next().css("color", "");
                });
                maxMessage.hide();
            }
        });
        </script>

