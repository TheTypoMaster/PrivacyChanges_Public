      PJMedia.ExternalLinker = (function($){
        'use strict';

        function openExternalLinksInNewWindow() {
          var hostRegExp = new RegExp(location.host);
          jQuery('a').each(function() {
            if (this.host !== '' && !hostRegExp.test(this.host)) {
              jQuery(this).attr('target', '_blank');
            }
          });
        }

        return {
          openExternalLinksInNewWindow: openExternalLinksInNewWindow
        } 

      }(jQuery));
       
