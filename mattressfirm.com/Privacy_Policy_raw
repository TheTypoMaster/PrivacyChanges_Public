		function redirectHome() {
			document.location.href = "/";
		}

		function getSilverCookie() {
			var value = "; " + document.cookie;
			var parts = value.split("; " + "silverPopUp" + "=");
			if (parts.length == 2) return parts.pop().split(";").shift();
		}

		function existsSilverPopEmailCookie() {
			var value = document.cookie;
			return value.indexOf("silverpopEmail") > -1;
		}

		function putSilverCookie() {
			var d = new Date();
			d.setDate(d.getDate() + 7);
			var n = d.toUTCString();
			document.cookie = "silverPopUp=true; expires=" + n + "; path=/";
		}

		function showSilverPopUp() {
			if ((window.location.host == 'beds.mattressfirm.com') || (getSilverCookie() == 'true') || window.location.pathname.indexOf("shopping-cart") > -1
				|| window.location.pathname.indexOf("checkout") > -1) {
				return false;
			}
			else {
				return true;
			}
		}

		function SilverPopOnLoad(thisobject) {
			if (typeof ewt != 'undefined' && ewt && ewt.setIFrameSrc)
				ewt.setIFrameSrc(thisobject, 'http://mattressfirm.mkt6537.com/sitepopup_final/SitePopUp_Final');
			else if (thisobject.src != 'http://mattressfirm.mkt6537.com/sitepopup_final/SitePopUp_Final')
				thisobject.src = 'http://mattressfirm.mkt6537.com/sitepopup_final/SitePopUp_Final';
		}

		function SilverPopSecureOnLoad(thisobject) {
			if (typeof ewt != 'undefined' && ewt && ewt.setIFrameSrc)
				ewt.setIFrameSrc(thisobject, 'http://mattressfirm.mkt6537.com/sitepop_gatedemailpg/SitePopUp_Final');
			else if (thisobject.src != 'http://mattressfirm.mkt6537.com/sitepop_gatedemailpg/SitePopUp_Final')
				thisobject.src = 'http://mattressfirm.mkt6537.com/sitepop_gatedemailpg/SitePopUp_Final';

			//The second Iframe Load implies that they have clicked the submit button
			if (pageLoadCnt > 1) {
				$j("#secureCloseButton").css('display', 'none');
				$j('#SilverPopUP').css('display', 'none');
				putSecureCookie();
				setTimeout(function () { hideSecureSilverPopUp() }, 2000);
				pageLoadCnt = 0;
			}

			pageLoadCnt++;

		}

		function hideSilverPopUp() {
			$j('#SilverPopUP').css('display', 'none');
		}

		function hideSecureSilverPopUp() {
			$j('#SilverPopSecure').css('display', 'none');
			canLoadSilverPopPopup = false;
		}

		function putSecureCookie(val) {
			var d = new Date();
			d.setDate(d.getDate() + 7);
			var n = d.toUTCString();
			document.cookie = "secureSilverPopUp=; expires=" + n + "; path=/";
		}

