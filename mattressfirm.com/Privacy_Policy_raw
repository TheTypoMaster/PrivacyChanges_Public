<script type="text/javascript">
	var url = document.location.protocol + "//api.vee24.com/api/Availability/?token=c9e014fc-1092-427d-b881-66ab4632e92f&callback=availabilityAnswer";

	var script = document.createElement("script");
	script.setAttribute("src", url);
	script.setAttribute("type", "application/javascript");
	document.body.appendChild(script);

	function availabilityAnswer(answer) {
		if (answer.video > 0) {
			jQuery(".videochat-button").css('cursor', 'pointer');
			jQuery(".videochat-container-OFF").attr('class', 'videochat-container');
			jQuery(".videochat-text").html("<span class='green-text'>Online now!</span><br>Click to video chat.");
		}
		else
			jQuery(".videochat-button").removeAttr("onclick")
	}
</script>

<script type="text/javascript">
	var $j = jQuery.noConflict();
	// $j is now an alias to the jQuery function; creating the new alias is optional.

	function fnTrapKD(event, caller) {
		if (event.keyCode == 13) {
			event.preventDefault();
			event.returnValue = false;
			event.cancel = true;
			SliSearch(caller);
			return false;
		}
		return true;
	}

	function SliSearchById(id) {
		SliSearch(document.getElementById(id));
		return false;
	}

	function SliSearch(caller) {
		var queryString;
		caller.name = caller.value;
		$j("meta[itemprop=target]").attr("content", "http://beds.mattressfirm.com/search?w={" + caller.value + "}")
		if (caller.value != 'Type here to search') {
			queryString = "w=" + caller.value;
		}
		if (caller.value != '') {
			document.location.href = 'http://beds.mattressfirm.com/search?' + queryString;
		}
	}

	function OpenPopup(time) {
		setTimeout(function () { document.getElementById('wehaveanswers').style.display = 'block'; }, time);
	}

	$j('#grayScreen').click(function () {
		HidePopups();
	});

	function HidePopups() {
		document.getElementById("grayScreen").style.display = "none";
		document.getElementById('customercare').style.display = 'none';
	}

	function showCallMessage() {
		$j('#modalPopupBackground').show();
		$j('#customerCouponBox').show();
		return false;
	}

	jQuery(document).ready(function () {
		//SCRIPT TO KNOW CURRENT BROWSER AND VERSION
		var sayswho = (function () {
			var ua = navigator.userAgent, tem,
			M = ua.match(/(opera|chrome|safari|firefox|msie|trident(?=\/))\/?\s*(\d+)/i) || [];
			if (/trident/i.test(M[1])) {
				tem = /\brv[ :]+(\d+)/g.exec(ua) || [];
				return 'IE ' + (tem[1] || '');
			}
			if (M[1] === 'Chrome') {
				tem = ua.match(/\b(OPR|Edge)\/(\d+)/);
				if (tem != null) return tem.slice(1).join(' ').replace('OPR', 'Opera');
			}
			M = M[2] ? [M[1], M[2]] : [navigator.appName, navigator.appVersion, '-?'];
			if ((tem = ua.match(/version\/(\d+)/i)) != null) M.splice(1, 1, tem[1]);
			return M.join(' ');
		})();

		if (sayswho == "Chrome 31" || sayswho == "Chrome 32" || sayswho == "Chrome 33" || sayswho == "MSIE 9")
			$j('#customercare').css('left', '25%');

		var SubtotalTD = document.getElementById('SubtotalTD');
		var showPopup = document.getElementById('placeholderheader_0_showPopUp');
		var PopupFG = $j('#adddedDarkBG, #addedPopUp');
		var PopupBG = $j('#adddedDarkBG');

		if (showPopup.value == "show") {
			PopupFG.animate({ 'opacity': '.50' }, 300, 'linear');
			PopupBG.animate({ 'opacity': '1.00' }, 300, 'linear');
			PopupFG.css('display', 'block');

			/* Center Box */
			var widthW = $j(window).width();
			var heightW = $j(window).height();

			var widthBox = PopupBG.outerWidth();
			var heightBox = PopupBG.outerHeight();

			var centerW = (widthW - widthBox) / 2;
			var centerH = (heightW - heightBox) / 2;

			PopupBG.css('right', '12%');
			PopupBG.css('left', '-12%');
			PopupBG.css('top', '1%');
			PopupBG.css('width', '100%');
			PopupBG.css('height', '100%');

			$j('.continueShop').click(function () {
				close_box();
			});

			/* Close Box */
			$j('.close').click(function () {
				close_box();
			});

			PopupBG.click(function () {
				close_box();
				__doPostBack('', '');
			});

			function close_box() {
				PopupFG.animate({ 'opacity': '0' }, 300, 'linear', function () {
					PopupFG.css('display', 'none');
				});
			}
			if (SubtotalTD != null) {
				SubtotalTD.style.paddingLeft = "0px";
				showPopup.value = "hidden";
			}
		}
		else {
			if (SubtotalTD != null) {
				SubtotalTD.style.height = "60px";
				SubtotalTD.style.paddingLeft = "130px";
			}
		}
		setParentURL();

		//--Since we are using Dynamic Footer from Sitecore, set the PPC Code Display through Javascript -->
		if ($j('#ppcCustomerCode') != null)
			$j('#ppcCustomerCode').html(formatPPCCode(myJScriptPPCCode));

	});

	var queryStringParams = new Array();
	var parentURL;

	//WORKAROUND TO ENABLE LOCAL, STG & PROD TESTING ; USED IN SILVERPOP BACKEND PROCESSING
	function setParentURL() {
		var thisSubDomain = getSubdomain();
		var domain = thisSubDomain != "" ? '.mattressfirm.com' : 'mattressfirm'
		parentURL = 'http://' + thisSubDomain + domain;
		//alert(parentURL); sidney
		console.log('parentURL: ' + parentURL);
	}

	// Here "addEventListener" is for standards-compliant web browsers and "attachEvent" is for IE Browsers.
	var eventMethod = window.addEventListener ? "addEventListener" : "attachEvent";
	var eventer = window[eventMethod];

	// Now...
	// if
	//    "attachEvent", then we need to select "onmessage" as the event.
	// if
	//    "addEventListener", then we need to select "message" as the event

	var messageEvent = eventMethod == "attachEvent" ? "onmessage" : "message";
	var AjaxSubmissionURL;
	// Listen to message from child IFrame window
	eventer(messageEvent, function (e) {
		var messageData = e.data;
		var resultArray = "";
		var command;
		var commandParam;

		//alert(messageData);
		console.log('messageData: ' + messageData);

		if (messageData.indexOf(',') != -1) {
			resultArray = messageData.split(",");
			command = resultArray[0];
			commandParam = resultArray[1];
			//alert( resultArray );
			console.log('resultArray: ' + resultArray);

			switch (command) {
				case 'linkEmailToVisitorKey':
					var myEmail = commandParam;
					//AjaxSubmissionURL = parentURL + '/Layout/Layouts/AddSilverpopRecipient.aspx?em=' + myEmail;
					AjaxSubmissionURL = parentURL + '/Layout/Layouts/AddSilverpopRecipient.aspx?action=add&em=' + myEmail;
					//alert( AjaxSubmissionURL );
					console.log('AjaxSubmissionURL: ' + AjaxSubmissionURL);
					addRecipientByAjax();
					break;
			}
		}

	}, false);

	function addRecipientByAjax() {
		// Assign handlers immediately after making the request,
		// and remember the jqxhr object for this request
		var jqxhr = $j.get(AjaxSubmissionURL, function () {
			//alert( "success" );
		})
		  .done(function (data) {
		  	//alert( "second success" );
		  	//alert( "Data Loaded: " + data );
		  	console.log("RETURN DATA: " + data)
		  })
		  .fail(function () {
		  	//alert( "error" );
		  	console.log("error");
		  })
		  .always(function () {
		  	//alert( "finished" );
		  });

		// Perform other work here ...

		// Set another completion function for the request above
		jqxhr.always(function () {
			// alert( "second finished" );
		});

	}

	function getSubdomain() {
		var regexParse = new RegExp('[a-z\-0-9]{2,63}\.[a-z\.]{2,5}$');
		var urlParts = regexParse.exec(window.location.hostname);
		return window.location.hostname.replace(urlParts[0], '').slice(0, -1);
	}

	//EXTRACT THE PARAMETERS FROM QUERYSTRING
	function buildQSParamArray(thisLocation) {
		//var query = window.location.search.substring(1);
		var query = thisLocation.substring(thisLocation.indexOf('?') + 1);
		//alert( 'query : ' + query );
		var parms = query.split('&');
		for (var i = 0; i < parms.length; i++) {
			var pos = parms[i].indexOf('=');

			if (pos > 0) {
				var key = parms[i].substring(0, pos);
				var val = parms[i].substring(pos + 1);
				queryStringParams[key] = val;
			}
		}
	}

	// setParentURL();
</script>

