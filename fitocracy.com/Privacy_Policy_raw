$(function(){
  
  //change sets no user
  _kmtee(['record', 'Logged out visit']);
  $(".set_nouser").show();

  $("#usernav li img").click(function(){
    if($("#global_notifications").is(':hidden')){
      $("#global_notifications").show();
    }else{
      $("#global_notifications").hide();
    }
    return false;
  });

  $('#global_notifications').click(function(e) {
    e.stopPropagation();
  });

  $(document).click(function() {
    $('#global_notifications').hide();
  });

  $('.clear_global_notification').click(function(e){
    e.preventDefault();
    var id=$(this).attr('id');
    var href=$(this).attr('href');
    $.get('/clear_notification/'+id, function(results){
      window.location = href;
    });
  });

  //Bind modal events
  bind_register_button();
  bind_login_button();
  bind_password_reset_button();
  fb_start(fb_app_id_oauth);
  if(is_register){
    base_register_modal();
  }

  //auto-show the login modal if the login page and is a redirect
  if(window.location.pathname == "/accounts/login/" && window.location.search.match(/next=(.+?)(&|$)/)){
    if(unescape(window.location.search).match(/hero_payment\/\?gift=/)){
      $(".register-button").trigger("click");
    }
    else{
      $(".login-button").trigger("click");
    }
  }
  // auto-show the pwreset modal if anchor
  if(window.location.pathname=== "/reset/password/"||window.location.hash === "#pwreset"){
    $(".forgot-password-link").trigger("click");
  }
  
});
