<script>
// update cart quantity javascript function (PMO::140492)
function updateCartQty(quantity) {
	var quantity = quantity - 0;
	var retVal = false;
	if (quantity < 0) {
		alert("Please select a quantity");
		retVal = false;
	} else {
		document.getElementById("cartQtyId").innerHTML = quantity;
		retVal = true;
	}
	return retVal;
}
function newWindow(url) {
	theWindow = window.open(url,'mapWin','width=299,height=256,scrollbars=no,resizable=no');
	theWindow.focus();
}
// Section Code for Mboxes
var section = "";
</script>

