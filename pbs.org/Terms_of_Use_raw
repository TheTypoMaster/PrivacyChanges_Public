      <script type="text/javascript" language="javascript">
      function initMenu() {
        $("ul#bento-corp-nav li:has(ul)").css("background", "#dee6e3 url(http://www.pbs.org/about/media/images/cm_down_grey.png) no-repeat right top");
        $("ul#bento-corp-nav li ul li:has(ul) ").css("background", "#89a1a9 url(http://www.pbs.org/about/media/images/cm_down_white.png) no-repeat right top");
       /*
      $("ul#bento-corp-nav  li:has(ul)").toggle(
          function() {
            $(this).css("background", "#fff url(http://www.pbs.org/about/media/images/cm_down_black.png) no-repeat right top");            
            },
          function() {
            $(this).css("background", "#dee6e3 url(http://www.pbs.org/about/media/images/cm_down_grey.png) no-repeat right top");
            });
     */
        $("ul#bento-corp-nav li:has(ul)").click(
          function(ev) {
             if (ev.target.nodeName != "A") {
              if ($(this).is("ul#bento-corp-nav > li")) {
                if  ($(this).is(".open_category")) {
                   $(this).removeClass("open_category");
                    $(this).css("background", "#dee6e3 url(http://www.pbs.org/about/media/images/cm_down_grey.png) no-repeat right top");
                 } else {
                  $(this).addClass("open_category");
                   $(this).css("background", "#fff url(http://www.pbs.org/about/media/images/cm_down_black.png) no-repeat right top");
                 }
              }
              $(this).children("ul").slideToggle(300);
              $(this).siblings().children("ul").slideUp("slow");
              $(this).siblings(":has(ul)").css("background", "#dee6e3 url(http://www.pbs.org/about/media/images/cm_down_grey.png) no-repeat right top");
              ev.stopPropagation();
              }
              });
      
        }
      $(document).ready(function() {initMenu();});
          
      </script>
