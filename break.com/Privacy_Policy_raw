		<script>
			/*
			-------------------------------------------------------
				set vsCookie, i.e. visit started cookie.
				Varnish will detect existance of this and the uem cookie ('dtCookie') set by dynatrace
				 and send the small portion (specified in vcl file) of traffic to app01 (for dynatrace).
				AMk - 08/09/2013
			-------------------------------------------------------
			*/
			if(typeof $.cookie('vsCookie') === 'undefined')
			{
				$.cookie('vsCookie', 1);
			}

			// This does not seems to be used anywhere
			/*var isCO = { setCookie: function (value, expiration) {
				jQuery.cookie('isCO', value, { expires: expiration, path: '/', domain: BUI.globals.ENV_URLS.DOTBREAK });
			},
				getCookie: function () {
					return jQuery.cookie('isCO');
				}
			}*/

			// If CPC traffic then set a session cookie to disalbe impression and click tracking
			if (window.location.toString().indexOf('utm_medium=') !== -1) {
				$.cookie('noStats', 1);
			}

			// Page Init try ASAP and if fail wait for DOMReady
			try {
				window.BUI.page.Init(); // do not pass data.pageType BUI.page.js will get the value from <head data-page-type="..">
			} catch (ee) {
				$(document).ready(function () {
					window.BUI.page.Init();
				});
			}
