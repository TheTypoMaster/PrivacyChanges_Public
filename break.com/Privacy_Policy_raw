			BUI.ads.normalizeAdArray = function (actual) {
				'use strict';
				
				var newArray = new Array();
					
				for (var i = 0; i < actual.length; i++) {
					if (actual[i]) {
						newArray.push(actual[i].toLowerCase().replace(/\s/g, '-'));
					}
				}
				
				return newArray;
			}

			
			BUI.ads.getAdZone = function (type) {
				'use strict';

				var zone,
					swapAdZone = BUI.utils.getURLParameter('swapAdZone'),
					pageType = {
						'channel' : 'channels',
						'cdp' : 'vid_contentdetail',
						'cdp-gallery' : 'gallery_contentdetail',
						'cdp-image' : 'pictures_individual',
						'cdp-playlist' : 'playlist_contentdetail',
						'cdp-article' : 'blog_contentdetail',
						'cdp-series' : 'series_contentdetail',
						'cdp-quiz' : 'quiz_contentdetail',
						'just-submitted' : '',
						'categories' : 'category',
						'topics' : 'topics',
						'topics-landing' : 'topics',
						'gallery_hub' : 'gallery_hub',
						'default' : 'uncategorized'
					},
					interstitial = {
						'home' : 'inter_stitial_home',
						'popular' : 'inter_stitial_popular',
						'channelindex' : 'inter_stitial_channelindex',
						'channel' : 'inter_stitial_channels',
						'channels' : 'inter_stitial_channels',
						'cdp' : 'inter_stitial_vid_contentdetail',
						'cdp-gallery' : 'inter_stitial_gallery_contentdetail',
						'cdp-image' : 'inter_stitial_fullscreen_contentdetail',
						'cdp-playlist' : 'inter_stitial_playlist_contentdetail',
						'cdp-article' : 'inter_stitial_blog_contentdetail',
						'cdp-series' : 'inter_stitial_series_contentdetail',
						'cdp-quiz' : 'inter_stitial_quiz_contentdetail',
						'gallery_hub' : 'inter_stitial_gallery_hub',
						'just-submitted' : 'inter_stitial_just_submitted',
						'categories' : 'inter_stitial_category',
						'search' : 'inter_stitial_search_results',
						'movies' : 'inter_stitial_movies',
						'topics' : 'inter_stitial_topics',
						'topics-landing' : 'inter_stitial_topics',
						'default' : 'inter_stitial_uncategorized'
					},
					cdpPromo = {
						'cdp' : 'vid_contentdetail_cdp_promo',
						'cdp-gallery' : 'gallery_contentdetail_cdp_promo',
						'cdp-playlist' : 'playlist_contentdetail_cdp_promo',
						'cdp-article' : 'blog_contentdetail_cdp_promo',
						'cdp-quiz' : 'quiz_contentdetail_cdp_promo'
					},
					promotedVideo = {
						'cdp-gallery' : 'gallery_contentdetail_promoted_video',
						'gallery_hub' : 'gallery_hub_promoted_video',
						'cdp-article' : 'blog_contentdetail_promoted_video',
						'cdp-quiz' : 'quiz_contentdetail_promoted_video',
						'just-submitted' : 'just_submitted_promoted_video',
						'search' : 'search_results_promoted_video',
						'categories' : 'category_promoted_video'
					},
					belowTheFold = {
						'home' : 'home_blf',
						'cdp' : 'vid_contentdetail_blf_300',
						'gallery_hub' : 'gallery_contentdetail_blf_300',
						'cdp-gallery' : 'gallery_contentdetail_blf_300',
						'cdp-playlist' : 'playlist_contentdetail_blf_300',
						'cdp-article' : 'blog_contentdetail_blf_300',
						'cdp-quiz' : 'quiz_contentdetail_blf_300',
						'cdp-series' : 'series_contentdetail_blf_300',
						'movies' : 'movie_contentdetail_blf_300',
						'default' : 'uncategorized_blf_300'
					},
					breakingNews = 'breaking_news';
					
				switch (type) {
					case 'page_type':
						zone = pageType;
						break;
					case 'interstitial':
						zone = interstitial;
						break;
					case 'cdp_promo':
						zone = cdpPromo;
						break;
					case 'promoted_video':
						zone = promotedVideo;
						break;
					case 'below_the_fold':
						zone = belowTheFold;
						break;
					case 'breaking_news':
						zone = breakingNews;
						break;
					default:
						zone = pageType;
						break;
				}

				
				if ( (swapAdZone && type === 'cdp_promo') || (swapAdZone && type === 'below_the_fold') ) {
					return zone[BUI.ads.props.pageType] || BUI.ads.props.pageType;
				}
				
				
				return (swapAdZone) ? swapAdZone : zone[BUI.ads.props.pageType] || BUI.ads.props.pageType;
			}
			
			var googletag = googletag || {},
				apexTagKeyVals = GetApexTagKeyVals().slice(4, apexTagKeyVals.lastIndexOf(';')) + ''.split(';mar='),
				_hostname,
				_hostname_split,
				_domain,
				_trkr = __BRK.get(),
				_sourcedomain = '',
				_brkref = '',
				_userstate = isLoggedInLocal();
				
			BUI.ads.props.pageType = 'legal';
			
			googletag.cmd = googletag.cmd || [];

			if (typeof ord === 'undefined') {
				ord = Math.ceil(Math.random() * 10000000000000000);
			}
			
			if (document.location.pathname.indexOf('/videos/newest/just-submitted') > -1) {
				BUI.ads.props.pageType = 'just-submitted';
			}
			
			if (document.location.pathname.indexOf('/pictures/') > -1) {
				// make distinction between gallery CDP and galleries HUB
				if (BUI.page.pageType === 'cdp-gallery') {
					BUI.ads.props.pageType = 'cdp-gallery';
				} else {
					BUI.ads.props.pageType = 'gallery_hub';
				}
				
			}

			
			if (typeof document.referrer.split('/')[2] != 'undefined') {
				_sourcedomain = document.referrer.split('/')[2];
			}

			if (typeof _trkr.brkref != 'undefined') {
				_brkref = _trkr.brkref;
			}

			if (BUI.utils.getURLParameter('brkref')) {
				_brkref = BUI.utils.getURLParameter('brkref');
			}

			
			BUI.ads.props.utmc = BUI.utils.getURLParameter('utm_content') || jQuery.cookie('brk.utm_content');
			if (BUI.ads.props.utmc && !jQuery.cookie('brk.utm_content')) {
				_hostname = window.location.hostname;
				_hostname_split = _hostname.split('.');
				
				_hostname_split.shift();
				_domain = _hostname_split.join('.');
				jQuery.cookie('brk.utm_content', BUI.ads.props.utmc, { path: '/', domain: _domain});
			}
			
			
			
			if (window.location.pathname.indexOf('/movies') === 0 && window.location.pathname.indexOf('/movies-') === -1) {
				
					
				
				
				BUI.ads.props.pageType = 'movies';
			}
			
			
			if (BUI.ads.props.pageType === 'movies') {
				
				BUI.ads.props.movieCategories = BUI.ads.normalizeAdArray(''.split(';'));
				
				// Extract the movie category from the URL when not supplied by the back end.
				if (BUI.ads.props.movieCategories.length === 0) {
					var moviesPagePath = window.location.pathname.replace('/movies/','');
					
					if (moviesPagePath === '/movies') {
						BUI.ads.props.movieCategories = [];
					} else if (moviesPagePath.indexOf('/') === -1) {
						BUI.ads.props.movieCategories.push(moviesPagePath.toLowerCase());
					}
					
				}
			}
			
			
			var adZone = BUI.ads.getAdZone('page_type'),
				intAdZone = BUI.ads.getAdZone('interstitial'),
				promoZone = BUI.ads.getAdZone('cdp_promo'),
				blfZone = BUI.ads.getAdZone('below_the_fold'),
				pvAdZone = BUI.ads.getAdZone('promoted_video'),
				bnAdZone = BUI.ads.getAdZone('breaking_news'),
				homeBelowFoldZone = BUI.ads.getAdZone('below_the_fold');
		</script>

		
		

