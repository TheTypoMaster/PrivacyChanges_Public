<script type="text/javascript">
 $(function(){
   var $navTabs = $(".navTabs")
   $navTabs.tabs(".navPanes > .navPane",{"tabs":"li","initialIndex":"0","event":"mouseup"})

   // STORE API IN GLOBAL SCOPE //
   HDC_CONTROL.NavTabs.API = $navTabs.data("tabs");

   // Here we are forcing the chage & forcing the flyout to close 
   $navTabs.find("li").each(function(index){
     var $this = $(this);
     $this.on("mouseenter",function(){ 
       HDC_CONTROL.NavTabs.switchToTab(index);
     });
     $this.on("mouseleave",function(){ HDC_CONTROL.NavTabs.stopTabSwitch(); })
   })
   $navTabs.on("mouseleave",function(){ HDC_CONTROL.NavTabs.stopTabSwitch(); })
 });
 HDC_CONTROL.overlapTabs();
</script>
