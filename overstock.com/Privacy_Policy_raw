function show_more(cat_id, count) {
    var more_link = document.getElementById('more_link_' + cat_id);
		more_link.style.display = 'none';
    var i;
//alert(count);
    for (i=3; i<=count; i=i+1) { 
        var show_link = document.getElementById('more_link_' + cat_id + i);
		    show_link.style.display = 'block';
    }
}
</script>
