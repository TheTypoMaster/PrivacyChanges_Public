                                  swy.header.ProfileAdapter = (function () {
                                      var api = {
                                          init: function () {
                                              this.headerMyCardCountEl = doc.getElementById('headerMyCardCount');
                                              this.headerMyListCountEl = doc.getElementById('headerMyListCount');
                                              this.headerRewardsAvailableCt = doc.getElementById('headerRewardsAvailable');
                                              this.headerRewardsAvailableEl = doc.getElementById('headerRewardsAvailable');

                                              profile.on('cardcountchange', this.onUpdateCount, this, {el: this.headerMyCardCountEl});
                                              profile.on('listcountchange', this.onUpdateCount, this, {el: this.headerMyListCountEl});
                                              profile.on('rewardpointschange', this.onUpdateRewards, this, {el: this.headerRewardsAvailableEl, ct: this.headerRewardsAvailableCt});

                                              profile.getCardCount({success:this.onUpdateCount,scope:this,el:this.headerMyCardCountEl});
                                              profile.getListCount({success:this.onUpdateCount,scope:this,el:this.headerMyListCountEl});
                                              profile.getRewardPoints({success:this.onUpdateRewards,scope:this,el: this.headerRewardsAvailableEl, ct: this.headerRewardsAvailableCt});

                                              return this;
                                          },
                                          onUpdateCount: function (count, opts) {
                                              if( opts && opts.el ) {
                                                  $(opts.el).text(' (' + count + ')');
                                              }
                                          },
                                          onUpdateRewards: function (rewards, opts) {
                                              if (opts && opts.el && opts.ct ) {
                                                  var rewardsAvailable = rewards ? rewards.rewardsAvailable : false,
                                                          $el = $(opts.el), $ct = $(opts.ct);
                                                  rewardsAvailable = parseInt(rewardsAvailable);
                                                  $zeroGasEl = document.getElementById("headerZeroGasRewardsLi");
                                                  $hasGasEl  = document.getElementById("headerHasGasRewardsLi");
                                                  if (!isNaN(rewardsAvailable) && rewardsAvailable !== 0) {
                                                  	  document.getElementById('headerRewardsAvailable').innerHTML=" ("+rewardsAvailable+") ";
                                                      $($hasGasEl).show();
                                                      $($zeroGasEl).hide();
                                                      //$ct.show();
                                                  }
                                                  else {
                                                  	var zeroGasEl=document.getElementById("headerHasGasRewards");                     	
                                                    $($zeroGasEl).show();
                                                  	$($hasGasEl).hide();
                                                      //$ct.hide();
                                                  }
                                              }
                                          }
                                      };
                                      return api.init();
                                  })();
