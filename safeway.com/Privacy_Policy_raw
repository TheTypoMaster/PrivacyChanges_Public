  <script type="text/javascript">
     // If IE is rendering the page in IE7 document mode, then the header needs
     // to be adjusted such that the last nav item is not floated. First locate
     // globalui_header_nav2, but if not found, locate globalui_header_middle.
     // Once a proper element is found the nofloat class is applied.
    if (7==document.documentMode)
        (function() {
            var $ = document.getElementById, E = $('globalui_header_nav2');
            if (E || (E=$('globalui_header_middle'))) E.className += ' nofloat';
        })();
