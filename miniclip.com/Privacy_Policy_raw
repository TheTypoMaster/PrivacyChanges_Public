        <script>
                (function () {
					
                        var name = 'mpu';
                 
                                var target = document.getElementById('dfp-' + name), ad = document.getElementById('container-' + name);
                                if (ad !== null && target !== null) {
									if($.browser.msie && $.browser.version <= 8) {
									document.write = function(html) {
										$('#dfp-mpu').append(html);
									 };
								 }
                                        target.appendChild(ad); ad.style.display = 'block';
                                        target.style.display = 'block';
                                        googletag.display('dfp-' + name);
                                }
                       
                })();
        </script>
</div>

