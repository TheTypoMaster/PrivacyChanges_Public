<script type="text/javascript">

var newSearchCategory = 0;
var newSearchShow = 0;
function newNavSelectCat(id){
	newSearchCategory = id;
	var txt = jQuery('#nnav_cat_' + id).html();
	jQuery('#nnav_search_label').html('<' + 'table style="width: 100%;" cellspacing="0" cellpadding="0"' + '><' + 'tr><' + 'td>' + txt + '<' + '/td' + '><' + 'td ' + 'style="vertical-align: middle;" valign="middle" align="right"' + '><' + 'span class="nnav sp-down_arrow_blue" id="nnav_search_arrow"><' + '/span' + '><' + '/td><' + '/tr><' + '/table>');
	dsmetrics.track(new DSMActivity('search-nav-change-category', '', dsmetrics.ActivityObjType.NotSet, 0, dsmetrics.ActivityActionType.Click, [new DSMOtherInfo('cat_name', txt)]));
}


function newNavDoSearch(){
	if($j('.nnav_search_input').val() == ''){
		alert('You need to enter a Search Term first.');
 $j('.nnav_search_input').val('').focus();
}
else
{
    var sto  = $j('.nnav_search_input').val();
    if('search all documents' !=sto)
    {
        var stM  = sto.replace('-','~');
        stM = $j.trim(stM);
	    var url = '/search/' + stM + '?catid=' + newSearchCategory;



    url += '';


	    dsmetrics.track(new DSMActivity('search-nav-do-search', '', dsmetrics.ActivityObjType.NotSet, 0, dsmetrics.ActivityActionType.Click, [new DSMOtherInfo('search_term', stM )]));
	    window.location = url;
    }
    else
    {
        $j('.nnav_search_input').val('').focus();
        $j('.nnav_search_input').css({color:'#444'});
    }
}
}

    jQuery(document).ready(function() {

    var nWidth = jQuery('.nav-inner').width();
nWidth = nWidth * 1;
jQuery('.nnav_repeating').width(nWidth - 11);
//autocomplete
$j('.nnav_search_input').autocomplete('/search/AutoComplete.ashx', { 
			minChars    : 4,
			max         : 8,
			delay       : 50,
			width       : '325px',
			selectFirst : false,
			cacheLength : 0
		});

//label on textbox
jQuery('#txtSearch').click( function() {
		if(jQuery('#txtSearch').val() == 'search all documents'){
			jQuery('#txtSearch').val('');
            $j('.nnav_search_input').css({color:'#444'});
		}
	});
if($j('.nnav_search_input').val() =='search all documents')
{
    $j('.nnav_search_input').css({color:'#cccccc'});
}

//search category  
jQuery('#popnav_searchcat').click( function() {
		if(newSearchShow == 1){
			jQuery('#popnav_searchcat_extra').slideUp();
			newSearchShow = 0;
			jQuery('#nnav_search_arrow').removeClass('sp-up_arrow_blue');
			jQuery('#nnav_search_arrow').addClass('sp-down_arrow_blue');
		}
		else{
			jQuery('#popnav_searchcat_extra').slideDown();
			newSearchShow = 1;
			jQuery('#nnav_search_arrow').removeClass('sp-down_arrow_blue');
			jQuery('#nnav_search_arrow').addClass('sp-up_arrow_blue');
		}
	});
$j(document).click(function(event) {
    var target = $j(event.target);
    if (target.parents('#popnav_searchcat').length == 0) {
			jQuery('#popnav_searchcat_extra').slideUp();
			newSearchShow = 0;
			jQuery('#nnav_search_arrow').removeClass('sp-up_arrow_blue');
			jQuery('#nnav_search_arrow').addClass('sp-down_arrow_blue');
    }
});

