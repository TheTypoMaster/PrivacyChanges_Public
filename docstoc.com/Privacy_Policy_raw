//documents  
document.getElementById('popnav_documents').onmouseover=function() {
		this.className='nnav_hover fleft';
		document.getElementById('popnav_documents_extra').style.display = 'block';
	};
document.getElementById('popnav_documents').onmouseout=function() {
		this.className='fleft';
		document.getElementById('popnav_documents_extra').style.display = 'none';
	};

//resources  
document.getElementById('popnav_resources').onmouseover=function() {
		this.className='nnav_hover fleft';
		document.getElementById('popnav_resources_extra').style.display = 'block';
	};
document.getElementById('popnav_resources').onmouseout=function() {
		this.className='fleft';
		document.getElementById('popnav_resources_extra').style.display = 'none';
	};

//upload  
document.getElementById('popnav_upload').onmouseover=function() {
		this.className='nnav_hover fleft';
		document.getElementById('popnav_upload_extra').style.display = 'block';
	};
document.getElementById('popnav_upload').onmouseout=function() {
		this.className='fleft';
		document.getElementById('popnav_upload_extra').style.display = 'none';
	};

    });
</script>


				
