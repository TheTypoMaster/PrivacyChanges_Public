			<script type="text/javascript">

$(document).ready(function(){
  $(".feedback_button").click(function(){
	document.cookie = "noaafeedback=true; expires=Thu, 31 Dec 2020 20:20:20 GMC";
  });
});

$(document).ready(function(){
  if (document.cookie.indexOf("noaafeedback") >= 0) {
	
	$(".feedback_tab").hide();							// HIDE feedback TAB
	$(".feedback_box").hide();							// HIDE feedback BOX
	$(".feedback_overlay").hide();						// HIDE feedback OVERLAY BACKGROUND
	
	//alert("The cookie exists!");
  } else {

var number = 1 + Math.floor(Math.random() * 20);			//CREATE RANDOM NUMBER

	$(".feedback_box").hide();							// HIDE feedback BOX
	$(".feedback_overlay").hide();						// HIDE feedback OVERLAY BACKGROUND

	$('#storenumber').text(number); 					// SHOW NUMBER IN DIV
		
	if(number == 4) {									// IF NUMBER EQUALS 4
		$(".feedback_box").fadeIn(300);					// THEN SHOW feedback TAB
		$(".feedback_overlay").fadeIn(300);
	} else {
		$(".feedback_box").hide();						// OR ELSE HIDE feedback TAB
		$(".feedback_overlay").hide();
	} 
	  
  }
});

$(document).ready(function(){
  $(".feedback_show").click(function(){					// CLICK TO SHOW feedback BOX
	$(".feedback_box").fadeIn(300).show();
	$(".feedback_overlay").fadeIn(300).show();
  });
});

$(document).ready(function(){
  $(".feedback_box .feedback_close").click(function(){	// CLICK CLOSE BUTTON TO HIDE feedback BOX AND OVERLAY BACKGROUND
	$(".feedback_box").fadeOut(300).hide();
	$(".feedback_overlay").fadeOut(300).hide();
  });
});

$(document).ready(function(){
  $(".feedback_overlay").click(function(){				// CLICK OVERLAY BACKGROUND TO HIDE feedback BOX
	$(".feedback_box").fadeOut(300).hide();
	$(".feedback_overlay").fadeOut(300).hide();
  });
});
	
	$(document).keyup(function(e) {

		if (e.keyCode == 27) {
			$(".feedback_box").fadeOut(300).hide();
			$(".feedback_overlay").fadeOut(300).hide();		
 		}
		
	});

</script>

