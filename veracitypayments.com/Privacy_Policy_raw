    <script>



        // Get Twitter share popup

        function twitterShare(url, title, descr, winWidth, winHeight) {
            var winWidth  = 520;
            var winHeight = 350;

            var winTop = (screen.height / 2) - (winHeight / 2);

            var winLeft = (screen.width / 2) - (winWidth / 2);            

            

            var title   = encodeURIComponent(jQuery('title').html());

            var descr   = encodeURIComponent(jQuery('meta[name=description]').attr("content"));

            var url     = encodeURIComponent(window.location.href);

            window.open('https://twitter.com/share?title=' + title + '&text=' + title +' '+ descr + '&url=' + url, 'sharer', 'top=' + winTop + ',left=' + winLeft + ',toolbar=0,status=0,width=' + winWidth + ',height=' + winHeight);            

        }



        // Get G Plus share popup

        function gplusShare(url, title, descr, winWidth, winHeight) {
            var winWidth  = 520;
            var winHeight = 350;
            var winTop = (screen.height / 2) - (winHeight / 2);

            var winLeft = (screen.width / 2) - (winWidth / 2);            

            

            var title   = encodeURIComponent(jQuery('title').html());

            var descr   = encodeURIComponent(jQuery('meta[name=description]').attr("content"));

            var url     = encodeURIComponent(window.location.href);

            window.open('https://plus.google.com/share?url=url=' + url, 'sharer', 'top=' + winTop + ',left=' + winLeft + ',toolbar=0,status=0,width=' + winWidth + ',height=' + winHeight);            

        }



        // Get Linkedin share popup

        function linkedinShare(url, title, descr, winWidth, winHeight) {
            var winWidth  = 520;
            var winHeight = 350;
            var winTop = (screen.height / 2) - (winHeight / 2);

            var winLeft = (screen.width / 2) - (winWidth / 2);                   

            var url     = encodeURIComponent(window.location.href);

            window.open('https://www.linkedin.com/cws/share?url=url=' + url, 'sharer', 'top=' + winTop + ',left=' + winLeft + ',toolbar=0,status=0,width=' + winWidth + ',height=' + winHeight);            

        }



        // News print script    



        function PrintElem(elem){

            Popup(jQuery(elem).html());

        }



        function Popup(data){

            var mywindow = window.open('', 'my div', 'height=600,width=775');

            mywindow.document.write('<link rel="stylesheet" href="http://192.163.235.19/~vanco/wp-content/themes/vanco/css/bootstrap.min.css?ver=3.0.3" type="text/css" /><link rel="stylesheet" href="http://192.163.235.19/~vanco/wp-content/themes/vanco/css/custom.css?ver=4.0.1" type="text/css" /><link rel="stylesheet" href="http://192.163.235.19/~vanco/wp-content/themes/vanco/css/media_screens.css?ver=3.0.3" type="text/css" />');

            mywindow.document.write('<html><head><title>Vanco News</title>');

            mywindow.document.write('</head><body >');

            mywindow.document.write(data);

            mywindow.document.write('</body></html>');

            mywindow.print();

            mywindow.close();

            return true;

        }



        



    </script>

