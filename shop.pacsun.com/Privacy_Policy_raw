			gigya.accounts.showScreenSet({
				screenSet: 'Login-web',
		        onAfterSubmit: loginErrorMessaging
	        });
		}
		
		function displayRegistration() {
			gigya.accounts.showScreenSet({
				screenSet: 'Login-web',
				startScreen:'gigya-register-screen',
		        onAfterSubmit: loginErrorMessaging
	        });
		}
		
