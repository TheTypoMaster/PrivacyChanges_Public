<script  type="text/javascript">
var userName = getCookie("DP_USER_NAME");
var userState = getCookie("DP_USER_STATE");
var favCount = getCookie("DP_USER_FAVCOUNT");
var rewarsPoints = getCookie("rewardsPointsCookie");
var country = 'US';
if(country == null || country == '' || country == 'undefined'){
country = 'US';
}
var context = document.getElementById('context').value;
if (favCount == undefined){favCount=0;}
var favCountHtml = '';
if(favCount==0)
{
favCountHtml = 'no saved items'
} else {
favCountHtml = 'saved items ('+favCount+')'
}
if (userName != undefined && userState == 1) {
$('#hotState #usrName').html('Hello &nbsp;' + userName);
$('#hotState #myfavoriteLinkHot span').html(favCountHtml);
$('#hotState').show();
$('#coldState').hide();
if(country == null || country == '' )
{
country = "US";
}
$('#hotState').find('#countryFlag').attr("src",context+'/images/flags/'+country+'.gif');
} else {
$('#hotState #usrName').html('');
$('#coldState #myfavoriteLink span').html(favCountHtml);
$('#coldState').show();
$('#hotState').hide();
if(country == null || country == '' )
{
country = "US";
}
$('#coldState').find('#countryFlag').attr("src",context+'/images/flags/'+country+'.gif');
}
if (rewarsPoints != undefined && rewarsPoints != null && rewarsPoints != '""' && rewarsPoints != "" && rewarsPoints != 'null') {
$('#hotState #rewardPoints').html(rewarsPoints+ ' rewards points');
}
if (country == 'US') $('.shippingModal').hide();
</script>


