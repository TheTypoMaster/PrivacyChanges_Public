}else{
var embeddedCm31 = "-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+typeOfCheckoutCm31+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-";
var renderTagCMJCP =renTagcmJcpt+'-_-'+renTagcmJcpc+'-_-'+'-_-'+orderVid+'-_-'+renTagtopDime+'-_-'+renTagtopDimeVal+'-_-'+currentDime+'-_-'+currentDimeVal+'-_-'+dimeCombo+'-_-'+dimeComboVal+'-_-'+renTagPageNumber+'-_-'+sortType+'-_-'+'-_-'+'-_-'+'-_-'+'-_-'+cmRedirectTerm+'-_-'+embeddedCm31+mmcParamArray[0]+'-_-'+mmcParamArray[1]+'-_-'+mmcParamArray[2]+'-_-'+mmcParamArray[3];;
}
var renderSortType = '-_-'+'-_-'+'-_-'+sortType+'-_-'+'-_-'+'-_-'+'-_-'+renTagcmJcpmmc+'-_-'+'-_-'+renTagcmJcpt+'-_-'+renTagcmJcpc+'-_-'+'-_-'+'-_-'+'-_-'+orderVid;
//document.getElementById("pageValue").value = renderTagCategId;
var val='';
if(pageType == 'storeLocator' && val != ""){
var first28EmptyAttributes = "-_-"+"-_-"+"-_-"+orderVid+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-"+"-_-";
var pipeSeperatedAtt29Values='';
var att30Value ='';
var storeType='';
//JCP store defult option
if (storeType == 'jcpStore'){
var selected = new Array();
if(!selected .length){
pipeSeperatedAtt29Values="Stores";
}
else if (selected .length > 1){
pipeSeperatedAtt29Values=selected.join("|");
}
else{
pipeSeperatedAtt29Values=selected;
}
}
if (storeType == 'jcpServices'){
pipeSeperatedAtt29Values="Pickup_Return";
}
if (storeType == 'jcpOutlet'){
pipeSeperatedAtt29Values="Outlet";
}
var itIsNumber = /^\d{5}$/.test(val);
if(itIsNumber){
att30Value=val;
}
else{
att30Value=val.replace(/\s/g, "_")
}
cmCreatePageviewTag(renderTagPageId, renderTagCategId, renderTagSearchTerm, renderTagSearchResults, first28EmptyAttributes+pipeSeperatedAtt29Values+"-_-"+att30Value, renderSortType);
updateCommonTagDataLayerProperties(renderTagCategId,renderTagPageId);
}
else{
cmCreatePageviewTag(renderTagPageId, renderTagCategId, renderTagSearchTerm, renderTagSearchResults, renderTagCMJCP, renderSortType);
updateCommonTagDataLayerProperties(renderTagCategId,renderTagPageId,renderTagSearchTerm);
}
if(elementTag != 'null' && elementTag == 'true'){
cmCreateElementTag('','');
}
</script>
