		jQuery.ready();
         $jq5=jQuery.noConflict();
         var hashurl = window.location.hash;
         if (hashurl.substring(1, 5).toLowerCase() == 'play') {
         hashurl = hashurl.replace("%20", " ");
         var hashvid = hashurl.substring(hashurl.indexOf(' ')+1, hashurl.length);
         $jq5("a[rel='#mediaContainer']").each(function() {
         href = $jq5(this).attr("href");
         if (href.indexOf(hashvid) != -1) {
         $jq5(this).overlay().load();
         }
         })
         }
	</script>
<!-- CORP Overlay jQuery Entry End -->
	
	
