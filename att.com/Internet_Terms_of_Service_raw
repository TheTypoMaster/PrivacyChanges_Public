    var serviceRequestCheck =""; 
       	{ 
    	if(URL.indexOf("all")  !== -1 || URL.indexOf("bundles")  !== -1 ) 
    	{ 
    		serviceRequestCheck= "All"; 
    	} 
    	else if(URL.indexOf("internet") !== -1) 
    	{ 
    		serviceRequestCheck="Internet"; 
    	} 
    	else if(URL.indexOf("tv") !== -1) 
    	{ 
    		serviceRequestCheck="TV"; 
    	} 
    	else if(URL.indexOf("u-verse") !== -1) 
    	{ 
    		serviceRequestCheck="U-Verse"; 
    	} 
    	else if(URL.indexOf("home-phone") !== -1) 
    	{ 
    		serviceRequestCheck="Home-Phone"; 
    	} 
    	else 
    	{ 
    		serviceRequestCheck="Unknown"; 
    	} 
    	} 
    	window.localStorage.setItem('serviceRequestCheck', serviceRequestCheck);  
