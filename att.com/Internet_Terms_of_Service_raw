                	if (sessionStorage['desktop-CAEvent-Back'] == 'true') { 
                		// back/cancel, so set both StatusCode and SuccessFlag to -1 
                		jQuery('<meta/>', {name: 'DCSext.wtStatusCode', content: '-1'}).appendTo('head'); 
                        jQuery('<meta/>', {name: 'DCSext.wtSuccessFlag', content: '-1'}).appendTo('head'); 
                        sessionStorage.removeItem('desktop-CAEvent-Back'); 
                	}  
                	 else if ((/out-of-footprint/).test(window.location.href)) { 
                	jQuery('<meta/>', {name: 'DCSext.wtStatusCode', content: 'CA_ERR_004'}).appendTo('head'); 
                        jQuery('<meta/>', {name: 'DCSext.wtSuccessFlag', content: '0'}).appendTo('head'); 
                	} 
                	else { 
                		// not back/cancel, and we already know no errors, so indicate success 
                		jQuery('<meta/>', {name: 'DCSext.wtStatusCode', content: '0'}).appendTo('head'); 
                        jQuery('<meta/>', {name: 'DCSext.wtSuccessFlag', content: '1'}).appendTo('head'); 
                	} 
                if (typeof sessionStorage['desktop-wtEvent'] !== undefined) { 
                	sessionStorage.removeItem('desktop-wtEvent'); 
                } 
                jQuery('<meta/>', {name: 'DCSext.wtQualCriteria', content: 'unknown'}).appendTo('head'); 
                jQuery('<meta/>', {name: 'DCSext.wtDwellingType', content: ''}).appendTo('head'); 
                jQuery('<meta/>', {name: 'DCSext.wtConnectedComm', content: ''}).appendTo('head'); 
                jQuery('<meta/>', {name: 'DCSext.wtELUID', content: 'unknown'}).appendTo('head'); 
                jQuery('<meta/>', {name: 'DCSext.wtAddressID', content: ''}).appendTo('head'); 
                jQuery('<meta/>', {name: 'DCSext.wtUverseCtn', content: ''}).appendTo('head'); 
                jQuery('<meta/>', {name: 'DCSext.wtUverseAvailResult', content: ''}).appendTo('head'); 
                jQuery('<meta/>', {name: 'DCSext.wtDSLAvailResult', content: ''}).appendTo('head'); 
                jQuery('<meta/>', {name: 'DCSext.wtAccessLnAvailResult', content: ''}).appendTo('head'); 
                jQuery('<meta/>', {name: 'DCSext.wtSysRespCode', content: ''}).appendTo('head'); 
                jQuery('<meta/>', {name: 'DCSext.wtSysTransID', content: ''}).appendTo('head'); 
                jQuery('<meta/>', {name: 'DCSext.wtServiceTypeAvail', content: 'IPTV~N|HSIA~N|VOIP~N'}).appendTo('head'); 
                jQuery('<meta/>', {name: 'DCSext.wtServiceRequestCheck', content: sessionStorage['desktop-wtServiceRequestCheck']}).appendTo('head'); 
                jQuery('<meta/>', {name: 'DCSext.wtEligResult', content: wtEligResult}).appendTo('head'); 
                jQuery('<meta/>', {name: 'DCSext.wtUserResp', content: sessionStorage['desktop-wtUserResp']}).appendTo('head'); 
                jQuery('meta[name="DCSext.wtCustType"]').attr('content',sessionStorage['desktop-wtCustType']); 
            	jQuery('<meta/>', {name: 'DCSext.wtMigrationTargetProdElig', content: ''}).appendTo('head'); 
                jQuery('<meta/>', {name: 'DCSext.wtFCCCenterTrialStatus', content: ''}).appendTo('head'); 
				jQuery('<meta/>', {name: 'DCSext.wtWHPEligibility', content: 'false'}).appendTo('head'); 
                jQuery('<meta/>', {name: 'DCSext.wtSourceAppName', content: 'MyAttSalesWireline'}).appendTo('head'); 
                if (typeof sessionStorage['desktop-wtCQClickId'] != 'undefined') { 
                    jQuery('<meta/>', {name: 'DCSext.wtCQClickId', content: sessionStorage['desktop-wtCQClickId']}).appendTo('head'); 
                } 
                if (v_pathName.indexOf('notify-me') > 0) { 
                    jQuery('<meta/>', {name: 'DCSext.wtPN', content: 'HRock NotifyMe Pg'}).appendTo('head'); 
                } 
                sessionStorage['desktop-CAEvent'] = 'false'; 
            } 
            var CADesktopData = []; 
            CADesktopData.push('DCSext.wtEvent'); CADesktopData.push('HRock_Check_Avail_Submit');	// TODO: always Submit event?  Can't be right, can it? 
            CADesktopData.push('DCSext.wtSuccessFlag'); CADesktopData.push('1');					// TODO: always success value of 1?  Can't be right, can it? 
            CADesktopData.push('DCSext.wtQualCriteria'); CADesktopData.push('unknown'); 
            CADesktopData.push('DCSext.wtDwellingType'); CADesktopData.push(''); 
            var connCommFlag = ''=='N' ? 'false' : '' =='Y' ? 'true' : '' ; 
            CADesktopData.push('DCSext.wtConnectedComm'); CADesktopData.push(connCommFlag); 
            CADesktopData.push('DCSext.wtELUID'); CADesktopData.push('unknown'); 
            CADesktopData.push('DCSext.wtAddressID'); CADesktopData.push(''); 
