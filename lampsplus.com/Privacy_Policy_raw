<!-- Process header-footer template -->
<script>
function chainScripts(obj, n, promise) {
    // Load the script at obj[n]
    if (n < obj.length) {
        var jqo = jQuery(obj[n]),
           src = jqo.attr('src');
        if (src) {
            src = (src.indexOf("/") == 0 || src.indexOf("http") == 0) ? src : "/" + src;
            // Modify google conversion js for async version
            // See: https://developers.google.com/adwords-remarketing-tag/asynchronous/
            if(src.indexOf("www.googleadservices.com/pagead/conversion.js") > -1) {
                src = "//www.googleadservices.com/pagead/conversion_async.js";
            }
            
            jQuery.ajax({
                url: src,
                dataType: 'script',
                success: function() {
                    chainScripts(obj, ++n, promise);
                    jQuery('head').append("<!-- script src " + src + " loaded dynamically -->");
                },
                error: function(xhr, ajaxOptions, thrownError) {
                    chainScripts(obj, ++n, promise);
                },
                async: false
            });
        } else {
        	var s = document.createElement("script");
            s.type = jqo.attr('type');
            s.text = jqo.text();
            document.body.insertBefore(s, document.body.firstChild);
            chainScripts(obj, ++n, promise);
        }
    }
    // Finished, resolve the promise if exists
    else if(promise) {
    	promise.resolve();
    }
}

function parseHeaderFooter(response) {
	// Parse string of HTML page
	var htmlString = response.split('<head>')[1];
		// head prep
	var split = htmlString.split('/head>');
	var head = "<div>" + split[0] + "/div>";
	var headArray = jQuery(head);
			// div containing non script elements from head
	var hNonScriptContainer = jQuery(headArray[0]);
			// array of script elements from head
	var hScripts = headArray.slice(1,headArray.length);

		// body prep
	var body = split[1];
	var split = body.split('<body');
	var body = "<div" + split[split.length-1].replace("</body>","").replace("</html>","") + "</div>";
	var bodyArray = jQuery(body);
			// <div> containing content of body, no <script>
	var bContent = jQuery(bodyArray[0]);
			// array of <script> from <body>
	var bScripts = bodyArray.slice(1,bodyArray.length);

	// Append meta
	hNonScriptContainer.find('meta:not([property=og\\:url]').each(function () {
	    jQuery('head').append(this);
	});

	// Append CSS, append HTML after global CSS is loaded
	var globalCSSIdentifierString = '[href^=\\/bundles]';
		// ASYNC: Global CSS
	var globalCSS = hNonScriptContainer.find('link' + globalCSSIdentifierString).eq(0);
	var globalCSSUrl = jQuery(globalCSS).attr('href');
	var request = jQuery.ajax({
		url: globalCSSUrl,
		success: function(r) {
			var globalStyleSheet = jQuery('<style type="text/css" />');
			globalStyleSheet.text(r);
			jQuery('head').append(globalStyleSheet);
		}
	});
			// DONE: Append HTML and load body JavaScript
	request.done(function() {
		var lpHeaderHTML = bContent.find('.lpHeader').html();
		var lpFooterHTML = bContent.find('.lpFooter').html() + bContent.find('.coreMetricsSupport').html();

		var lpHeader = jQuery('.lpHeader'),
            lpFooter = jQuery('.lpFooter');

		lpHeader.html(lpHeaderHTML).height('auto');
		lpFooter.html(lpFooterHTML).height('auto');
        
        	// ASYNC: Append JavaScript from head
			// Let body scripts know head scripts are done loading
		var hScriptPromise = new jQuery.Deferred;
		chainScripts(hScripts,0, hScriptPromise);

			// ASYNC: Append JavaScript from body when head scripts are done
        var bScriptPromise = new jQuery.Deferred;
		hScriptPromise.done(function() {
			chainScripts(bScripts,0, bScriptPromise);
		});
        
            // Trigger Google AdWords Remarketing Tag after body scripts
        bScriptPromise.done(function() {
            window.google_trackConversion({
              google_conversion_id: window.google_conversion_id, 
              google_custom_params: window.google_custom_params,
              google_remarketing_only: window.google_remarketing_only
            });
        });

				// Patch HTML
		            // Prevent DropDownSplash floats from hanging
	    jQuery('.CategoryDropDowns').each(function() {
	        jQuery('.DropDownSplash', this).eq(0).addClass('first');
	    });
	                // Sign In: Correct embedded return url
		if(jQuery('#hdrSignIn').length){            
	        var originalLinkArray = jQuery('#hdrSignIn').attr('href').split('?'),
	            originalLink = originalLinkArray[0],
	            originalQueryArray = originalLinkArray[1].split('&');
	        for(i=0; i<originalQueryArray.length; i++){
	            if(originalQueryArray[i].indexOf('ReturnUrl=') + 1){
	                originalQueryArray[i] = 'ReturnUrl=' + encodeURIComponent(window.location.pathname + '?' + new Date().getTime());
	            }
	        }
	        var newLink = originalLink + '?' + originalQueryArray.join('&');
	        jQuery('#hdrSignIn').attr('href', newLink);
	    }
	});
		// Other CSS
	var otherCSS = hNonScriptContainer.find('link[rel=stylesheet]:not(' + globalCSSIdentifierString + ')');
	otherCSS.each(function() {
		jQuery('head').append(this);
	});

}
function parseError() {
    jQuery('body').prepend('<div><p>Header and Footer template will not load.</p></div>');
}

jQuery(function () {
    // Give body 'ie7' class if IE 7
    if(navigator.userAgent.indexOf("MSIE 7.0") >= 0) {
        jQuery('body').addClass('ie7');
    }
    
    // Prepare Header and Footer Containers
    var lpHeader = jQuery('<div class="lpHeader" />').height(169),
        lpFooter = jQuery('<div class="lpFooter" />').height(279);

	if( jQuery('.admin-bar').length ){
        jQuery('.header-fragments-footer').after(lpHeader);
    } else {
        jQuery('body').prepend(lpHeader);
    }
    
    jQuery('body').append(lpFooter);

    // Load Header and Footer
    if( !jQuery('.lpHeader').children().length && !jQuery('.lpFooter').children().length ){
        jQuery.ajax({
            url: '/templates/blog/header-footer.aspx?currenturl=' + encodeURIComponent(location.href) + '&' + new Date().getTime(),
            success: parseHeaderFooter,
            error: parseError
        });
    }    
});
</script>
