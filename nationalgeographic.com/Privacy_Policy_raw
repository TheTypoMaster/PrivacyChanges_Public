<script type="text/javascript">
window.fbAsyncInit = function() {
    FB.init({
        appId        :     '130939060280023',
        status       :     true, // check login status
        cookie       :     true, // enable cookies to allow the server to access the session
        xfbml        :     true,  // parse XFBML
        channelUrl   :     'http://images.nationalgeographic.com/wpf/sites/common/inc/channel.html'
    });
};

(function() {
    var e = document.createElement('script');
    e.src = document.location.protocol + '//connect.facebook.net/en_US/all.js';
    e.async = true;
    document.getElementById('fb-root').appendChild(e);
}());
</script>


    <div id="gigya_login_div"></div>





<script src="//images.nationalgeographic.com/wpf/sites/common/j/jquery.dfp.min-cb1414755310.js"></script>
<script>
    /* Initializes the ad slots.
     * Arguments:
     *  - loadSecondaryAdBox: boolean. 
     *    -- `true`: the function will load/reload only the secondary ad slot (after-ad).
     *    -- other value than `true`: the function will load all the ad slots.
     */ 
    function initAds(loadSecondaryAdBox) {
        var $units;
        $('.adunit').each(function() {
            var $element = $(this);
            var $parent = $element.closest('.advertisement');
            if ($parent.length == 0 || $parent.css('display') !== 'none') {
                $element.addClass('visible');
            }
        });
        
        if (loadSecondaryAdBox) {
            $('div.secondary_ad').removeClass('hidden');
            $units = $('#headerboard .adunit.ad-after');
        } else {
            $('div.secondary_ad').addClass('hidden');
            $units = $('.adunit.visible:not(.ad-after)');
        }
        
        $units.each(function(i, unit) {
            $(unit).data().afterLoadAdreadyExecuted = false;
        });
        $units.dfp({
            dfpID: '2994',
            collapseEmptyDivs: true,
            enableSingleRequest: false,

            // Callback which is run after the render of all ads.
            afterEachAdLoaded: function (adUnit, event) {
                if (adUnit.data().afterLoadAdreadyExecuted === false) {
                    var slotWidth = event.size[0];
                    if (adUnit.is('.ad-after') && adUnit.closest('#headerboard').length > 0) {
                        if (slotWidth > 0) {
                            $('div.adunit.leaderboard:not(.hidden)').css('float', 'left');
                        }
                    } else {
                        if (adUnit.is('.leaderboard')) {
                            if (slotWidth <= 728) {
                                initAds(true);
                            } else {
                                $('div.adunit.leaderboard:not(.hidden)').css('float', 'none');
                            }
                        }
                    }
                    adUnit.data().afterLoadAdreadyExecuted = true;
                }
            }
        });
    }

    /**
     * Add a script inside the interstitial div, that initialize
     * the slot to show the GPT ad; this function also save the
     * slot's instance in window, to refresh the ad every time
     * this function is called.
     * ad-gallery-container is the div's id
     */
    function interstitialAd(adunit, adsize, divId) {
        var gtag = document.createElement('script');
        gtag.type = 'text/javascript';
        gtag.innerHTML = 'googletag.cmd.push(function() {' +
            'if(window.adInterstitial == undefined){' +
                'var slot1 = googletag.defineSlot("2994/' 
                    + adunit + '", [' + adsize[0] + ', ' 
                    + adsize[1] + '],"'+divId+'");' +
                'window.adInterstitial = slot1;' +
            '}' +
            'else{' +
                'slot1 = window.adInterstitial;' +
            '}' +
            'slot1.addService(googletag.pubads());googletag.'+
                'enableServices();googletag.display("'+divId+'");});';
        //add the script tag inside the div
        $('#'+divId).append(gtag);
        //refresh only the instance of the interstitial ad
        googletag.pubads().refresh([window.adInterstitial]);
    }

    function ngsAdvertisingRefresh(){
        initAds();
    }

    function ngsPageView(pageName,separator){
        if (window.location.hostname.indexOf ("video.") === 0){
            return;
        }
        // if there are refreshable ads on a page then refresh them
        ngsAdvertisingRefresh();
    }

    initAds();
</script>




</body>



</html>

