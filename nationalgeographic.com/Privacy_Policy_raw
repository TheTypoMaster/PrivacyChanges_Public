        <script>
        (function() {
            var responsiveNav = false;
            if (window.MOBILE_FEATURES.forceResponsiveGlobalNav === true) {
                responsiveNav = true;
            } else if (window.MOBILE_FEATURES.isMobilePhone()) {
                if (window.MOBILE_FEATURES.womp === true || window.MOBILE_FEATURES.nativeMobile === true) {
                    responsiveNav = true;
                }
            }
            window.GN = {
                baseContainer: '#global_nav',
                display: 'biggie',
                loadRegistration: true,
                responsive: responsiveNav,
                registrationOptions: {
                    mmdbHost: "https://mmdb.nationalgeographic.com",
                    memcenHost: "https://members.nationalgeographic.com",
                    notificationsHost: "https://notifications.nationalgeographic.com",
                    notificationsKey: "e6c4b4af844c454fc2110a4d09828559489837313bd2e519cf",
                    gigyaKey: "3_LOaUF9fHrs7lFHW7YFFA22qD6MhVsbO_bpqkn6nnKj9-AbGRsec864eKWqmR2Jdu",
                    show_user_ui: false
                },
                searchUrl: "http://www.nationalgeographic.com/search/",
            
            };
            if (responsiveNav === true) {
                loadStyleSheet("//assets.nationalgeographic.com/modules-global-nav/styles/main.css");
            } else {
                loadStyleSheet("//assets.nationalgeographic.com/modules-global-nav/styles/main-nonresponsive.css");
