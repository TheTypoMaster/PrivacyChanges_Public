    <script type="text/javascript">
    (function(window){
        var Tabs = function(selector){
            var $rootElement = $(selector),
                $activeTab = void(0),
                $activeTitle = void(0),
                //Private Methods start here
                attachHandlers = function () {
                    $rootElement.on("click", function (event) {
                        var $el = $(event.currentTarget),
                            dest = "#"+$el.data("tab");

                        if (!$activeTitle){
                            $activeTitle = $el;
                            $activeTitle.addClass("active");
                        } else if ($activeTitle && $activeTab.selector != dest){
                            $activeTitle.removeClass("active");
                            $activeTitle = $el;
                            $activeTitle.addClass("active");
                        } else {
                            $activeTitle.removeClass("active");
                            $activeTitle = void(0);
                        }

                        if (!$activeTab) {
                            $activeTab = $(dest);
                            $activeTab.addClass("active");
                            //$activeTab.show();
                            $activeTab.slideDown();
                        } else if($activeTab && $activeTab.selector != dest) {
                            //$activeTab.hide();
                            $activeTab.slideUp();
                            $activeTab.removeClass("active");
                            $activeTab = $(dest);
                            //$activeTab.show();
                            $activeTab.slideDown();
                        } else {
                            //$activeTab.hide();
                            $activeTab.slideUp();
                            $activeTab = void(0);
                        }
                    });
                };
                //Public Methods start here.
                this.init = function () {
                    attachHandlers();
                };

        },
        tabsInstance = new Tabs(".footer .footer-menu-title");
        tabsInstance.init();
    })(window);
    </script>


    

    






