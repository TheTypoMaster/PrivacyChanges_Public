        <script type="text/javascript">
        function OnSubmitGlobalSearchForm()
        {
            var collection=document.header_search_video.collection;
            var rad=document.header_search_video.search_dest;
            for(var i=0;i<rad.length;i++){
                if(rad[i].checked){
                    switch(rad[i].value){
                    case 'video':
                        // do video-only search
                        if (collection.value === "cms_ngkids"){
                            document.header_search_video.action ="/video/kids/search/";
                        }else{
                            document.header_search_video.action ="http://video.nationalgeographic.com/video/search/";
                        }
                        break;
                    case 'content':
                    default:
                        // do site-wide google content search
                        if (collection.value === "cms_ngkids"){
                            document.header_search_video.action ="http://kids.nationalgeographic.com/kids/search/";
                        }else{
                            document.header_search_video.action ="http://www.nationalgeographic.com/search/";
                        }
                        document.header_search_video.site="default_collection"
                        document.header_search_video.client="default_frontend"
                        document.header_search_video.proxystylesheet="default_frontend"
                        break;
                    }
                }
            }

          return true;
        }
        </script>
        
            
