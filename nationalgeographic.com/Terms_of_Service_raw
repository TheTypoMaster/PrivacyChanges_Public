        <script>
        (function() {
            window.loadStyleSheet = function(uri) {
                if (document.createStyleSheet) {
                // Covers all IE, but needed to load the CSS in IE8
                document.createStyleSheet(uri);
                } else {
                    $('<link href="'+ uri +'" media="screen" rel="stylesheet">').appendTo("head");
                }
            };
            window.MOBILE_FEATURES = {
                mobilePhoneIncludeAgents: ['iphone', 'ipod', 'ipad', 'phone', 'mobile', 'mini', 'silk', 'iemobile'],
                mobilePhoneExcludeAgents: ['ipad', 'tablet'],
                nativeMobile: false,
                forceResponsiveGlobalNav: false,
            
                womp: false,
            
                isMobilePhone: function(){
                    var includeAgents = window.MOBILE_FEATURES.mobilePhoneIncludeAgents.join("|");
                    var excludeAgents = window.MOBILE_FEATURES.mobilePhoneExcludeAgents.join("|");
                    if (new RegExp(includeAgents, 'i').exec(navigator.userAgent)) {
                        if (!new RegExp(excludeAgents, 'i').exec(navigator.userAgent)) {
                            return true;
                        }
                    }
                    return false;
                }
            };
            if (window.MOBILE_FEATURES.isMobilePhone() === true && window.MOBILE_FEATURES.nativeMobile === true) {
                $('<meta name=viewport content="width=device-width">').appendTo("head");
                window.loadStyleSheet("//images.nationalgeographic.com/wpf/sites/common/c/responsive-cb1435230581.css");
            }
        })();
        </script>
        

    

    
